            <opm:queries>
               <opm:query name="ajuste_oblig_contrato" xsi:type="toplink:read-all-query">
                  <opm:arguments>
                     <opm:argument name="programa">
                        <opm:type>java.lang.Number</opm:type>
                     </opm:argument>
                     <opm:argument name="dependencia">
                        <opm:type>java.lang.Number</opm:type>
                     </opm:argument>
                     <opm:argument name="unidad">
                        <opm:type>java.lang.Number</opm:type>
                     </opm:argument>
                     <opm:argument name="salario">
                        <opm:type>java.lang.Number</opm:type>
                     </opm:argument>
                     <opm:argument name="horas">
                        <opm:type>java.lang.Number</opm:type>
                     </opm:argument>
                  </opm:arguments>
                  <toplink:bind-all-parameters>true</toplink:bind-all-parameters>
                  <toplink:cache-statement>false</toplink:cache-statement>
                  <toplink:timeout>0</toplink:timeout>
                  <toplink:prepare>false</toplink:prepare>
                  <toplink:call xsi:type="toplink:sql-call">
                     <toplink:sql>select id_ajuste,b.id_tipoajuste,a.nombre,opcional,nivel,sis_pkg_calculos.GETVALORAJUSTEXVAR(id_ajuste,#salario,#horas,1,1,sysdate) as calculado
from sis_ajuste a,sis_tipo_ajuste b
where
a.opcional='N'
and a.id_tipoajuste=b.id_tipoajuste
and not exists(
select 1 from sis_ajuste_exclud_unidad c
where a.id_ajuste=c.id_ajuste
and c.ID_PROGRAMA=#programa
and c.ID_DEPENDENCIA=#dependencia
and c.ID_UNIDAD=#unidad
)
order by b.operacion 
</toplink:sql>
                  </toplink:call>
                  <toplink:reference-class>enlace_datos.tablas.SisAjuste</toplink:reference-class>
                  <toplink:refresh>true</toplink:refresh>
                  <toplink:remote-refresh>true</toplink:remote-refresh>
                  <toplink:cache-usage>primary-key</toplink:cache-usage>
                  <toplink:lock-mode>none</toplink:lock-mode>
                  <toplink:container xsi:type="toplink:list-container-policy">
                     <toplink:collection-type>java.util.Vector</toplink:collection-type>
                  </toplink:container>
               </opm:query>
