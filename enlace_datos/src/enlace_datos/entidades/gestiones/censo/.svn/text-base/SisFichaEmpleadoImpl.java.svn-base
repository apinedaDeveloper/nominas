NameValuePairs vParametros;

vParametros=new NameValuePairs();                

 if (vRespuestas!=null)
 {
               
    vTamanio=vRespuestas.size();
    
    for (int vPos=1;vPos<vTamanio;vPos++)
    {
         vResp=(NameValuePairs)vRespuestas.get(vPos);        
         vIdPreg=vResp.getAttribute("id_pregunta");
         vBajarResp=vResp.getAttribute("bajar");
         vCodValidacion=(vResp.getAttribute("cod_validacion")==null?"":vResp.getAttribute("cod_validacion").toString());
         
         //  System.out.println("post "+vPos);
         if (vIdPreg!=null && (vBajarResp!=null && vBajarResp.toString().equals("1")))
         {
            vIds=vResp.getAttribute("ids");
            vOtros=vResp.getAttribute("otros");
            vObligatorio=vResp.getAttribute("obligatorio").toString();
                    
             if (vObligatorio.equals("1"))
             {
                        
                if (vIds==null && (vOtros==null || (vOtros!=null && vOtros.equals(""))))
                {
                           
                    vIdControl=vResp.getAttribute("id_control");      
                     
                     if (vIdControl==null)      
                      throw new JboException(" Por favor conteste la pregunta: "+vResp.getAttribute("pregunta"));   
                     else
                      throw utils.getJboException(" Por favor conteste la pregunta.",vIdControl.toString(),false);
//                         throw new JboException(" Por favor conteste la pregunta: "+vResp.getAttribute("pregunta"));   
                }
                        
             }             
             
                 if (!vCodValidacion.equals("")){
                    
                    vParametros.clear(); 
                    vParametros.setAttribute("reg",this.getRegistroEmpleado());
                    vParametros.setAttribute("cod",vCodValidacion);
                    vParametros.setAttribute("idpreg",vIdPreg);
                    vParametros.setAttribute("idreso",vIds);
                    
                    if (vOtros!=null)
                    {
                     try{
                    
                       if (vOtros.getClass()==java.util.Date.class){
                           
                           vParametros.setAttribute("otros",utils.getFechaFormato("dd/MM/yyyy",(java.util.Date)vOtros));
                           
                           
                       }else if (vOtros.getClass()==oracle.jbo.domain.Date.class){
                       
                           vParametros.setAttribute("otros",utils.getFechaFormato("dd/MM/yyyy",(oracle.jbo.domain.Date)vOtros));   
                           
                       }else{
                           
                           vParametros.setAttribute("otros",vOtros.toString()); 
                       }
                    
                     }catch(Exception ee){
                         
                         vParametros.setAttribute("otros","");
                     }
                    }else                    
                    {
                        
                        vParametros.setAttribute("otros","");
                        
                    }
                     
                    vRespuestaFuncion=utils_DB.getValorFuncion(this.getDBTransaction(),"SIS_VAL_CENSO_LABORAL(?,?,?,?,?)",vParametros);
                     
                    if (!vRespuestaFuncion.toString().equals("OK"))
                    {
                        vIdControl=vResp.getAttribute("id_control");      
                         
                         if (vIdControl==null)      
                          throw new JboException(vRespuestaFuncion.toString());   
                         else
                          throw utils.getJboException(vRespuestaFuncion.toString(),vIdControl.toString(),false);
                        
                    }
                     
                 } 
             
             
           }
           
           
           
       } // for 
  }              
             //  System.out.println("entidad "+ this.getRespuestasCenso()); 
}


    
