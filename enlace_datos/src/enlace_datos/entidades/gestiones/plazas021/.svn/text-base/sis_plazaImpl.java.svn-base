    /**Custom DML update/insert/delete logic here.
     */
    protected void doDML(int operation, TransactionEvent e) {
    
        String valUsuario = utils.getAuthUser(e.getDBTransaction());//obtengo el registro del usuario logueado
        if (this.getCreadoPor()== null){
            this.setCreadoPor(valUsuario);
        }
        
        
        // pametros para el procedimiento
        NameValuePairs parametro;
        parametro = new NameValuePairs();
        parametro.setAttribute("1",this.getIdPuesto());
        parametro.setAttribute("2",this.getIdPartida());
        parametro.setAttribute("3",this.getNumeroPlaza());
        parametro.setAttribute("4",this.getCorrelativo());
        if (operation==DML_INSERT){  
            this.setDisponible("SI");
            parametro.setAttribute("5",1);
            //parametro.setAttribute("6",null);
             SimpleDateFormat formato = new SimpleDateFormat("yyyy");
             String cadena = formato.format(this.getFechaInicio().getValue());
             if (this.getCorrelativo().compareTo(Integer.parseInt(cadena.trim()) )!=0){
                 throw new JboException("LA FECHA DE INICIO NO COINCIDE CON EL AÑO DE TRABAJO,  VERIFIQUE EL AÑO DE TRABAJO");
             }
             cadena = formato.format(this.getFechaFin().getValue());
             if (this.getCorrelativo().compareTo(Integer.parseInt(cadena.trim()) )!=0){
                 throw new JboException("LA FECHA FIN NO COINCIDE CON EL AÑO DE TRABAJO,  VERIFIQUE EL AÑO DE TRABAJO");
             }
             
            registrarLogOperaciones(operation, e);  
            super.doDML(operation, e);
            utils_DB.ejecutarProcedimiento(this.getDBTransaction(),"SIS_PKG_PROCS.proyeccion_plaza021 (?,?,?,?,?)", parametro);
        }    
        else if (operation == DML_UPDATE){ 
        //enlace_datos.consultas.gestiones.plazas021.sis_plazaImpl regPlazas;
            //RowIterator fila;
           // sis_plazaImpl regplazas;
            
            if (this.getampliacion() == null){// solamente es una actualizacion de la plaza
                //System.out.println("hola");
                parametro.setAttribute("5",2);
                //parametro.setAttribute("6",null);
            }else {// la actualizacion de la plaza es para ampliarla (reutilizarla)
                //System.out.println("hola222");
                parametro.setAttribute("5",4);
                //parametro.setAttribute("6",this.getFechaInicio().getValue());
               // this.setFechaInicio((Date)this.getPostedAttribute(6));
                
            }
            
            //parametro.setAttribute("5",2);
             SimpleDateFormat formato = new SimpleDateFormat("yyyy");
             String cadena = formato.format(this.getFechaInicio().getValue());
             if (this.getCorrelativo().compareTo(Integer.parseInt(cadena.trim()) )!=0){
                 throw new JboException("LA FECHA DE INICIO NO COINCIDE CON EL AÑO DE TRABAJO,  VERIFIQUE EL AÑO DE TRABAJO");
             }
              cadena = formato.format(this.getFechaFin().getValue());
             if (this.getCorrelativo().compareTo(Integer.parseInt(cadena.trim()) )!=0){
                 throw new JboException("LA FECHA FIN NO COINCIDE CON EL AÑO DE TRABAJO,  VERIFIQUE EL AÑO DE TRABAJO");
             }
             
            registrarLogOperaciones(operation, e);  
            super.doDML(operation, e);
            utils_DB.ejecutarProcedimiento(this.getDBTransaction(),"SIS_PKG_PROCS.proyeccion_plaza021 (?,?,?,?,?)", parametro);
        }            
        else {
            parametro.setAttribute("5",3); 
            utils_DB.ejecutarProcedimiento(this.getDBTransaction(),"SIS_PKG_PROCS.proyeccion_plaza021 (?,?,?,?,?)", parametro);
            
            registrarLogOperaciones(operation, e); 
            super.doDML(operation, e);
        
        }
        
       
        parametro = new NameValuePairs();
        parametro.setAttribute("1",this.getIdPartida());
        parametro.setAttribute("2",this.getCorrelativo());
        Object varSaldo = utils_DB.getValorFuncion(this.getDBTransaction(),"SIS_PKG_UTIL.validar_plazas021(?, ?)",parametro);
        
        
        //temporalmente, revisarlo mas detenidamente
         if (operation == DML_DELETE){
             varSaldo = 1;
         }
        Number saldoPartida = utils.getNumberOracle(varSaldo);
        if (saldoPartida.compareTo(0)== 0) {
            throw new JboException("La partida no tiene suficiente saldo para poder crear la plaza");
        }
    }

