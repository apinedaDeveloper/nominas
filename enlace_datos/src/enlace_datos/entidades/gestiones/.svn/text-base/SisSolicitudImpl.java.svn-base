  private void validar_entidad(){
      
      if (this.getIdTipoSolicitud().compareTo(utils.getNumberOracle("5"))==0){
        if (this.getFechaSolicitud()==null)      
           throw new JboException("Por favor ingrese la fecha de renuncia"); 
      }
      else if (this.getIdTipoSolicitud().compareTo(utils.getNumberOracle("6"))==0){
          if (this.getFechaSolicitud()==null)        
          throw new JboException("Por favor ingrese la fecha de la despido"); 
      }
    else if (this.getIdTipoSolicitud().compareTo(utils.getNumberOracle("9"))==0){
        if (this.getFechaSolicitud()==null)        
        throw new JboException("Por favor ingrese la fecha baja por fallecimiento"); 
    }
    else if (this.getIdTipoSolicitud().compareTo(utils.getNumberOracle("11"))==0){
        if (this.getFechaSolicitud()==null)        
        throw new JboException("Por favor ingrese la fecha baja por retiro obligatorio"); 
    }else if (this.getIdTipoSolicitud().compareTo(utils.getNumberOracle("12"))==0){
        if (this.getFechaSolicitud()==null)        
        throw new JboException("Por favor ingrese la fecha baja por traslado"); 
    }  
    else if (this.getIdTipoSolicitud().compareTo(utils.getNumberOracle("13"))==0){
        if (this.getFechaSolicitud()==null)        
        throw new JboException("Por favor ingrese la fecha baja por rescision de contrato"); 
    }           
}
      
      

    /**Custom DML update/insert/delete logic here.
     */
    protected void doDML(int operation, TransactionEvent e) {
        Number IdSol,IdEstado;
        RowIterator vFilas;
        SisDetLicenciaImpl vFilaLicencia;
        SisOrdenJudicialImpl vFilaOrdenJudicial;
        SisSuspencionIgssImpl vFilaSuspencion;
        SisDeudorImpl vFilaDeudores;
        SisPromReclafImpl vFilaPromReclaf;
        
        String sqlQuerry,vFechaSuspI;
        Object vResultadoConsulta,vFechaI;
        
        //   System.out.println("cambia solicitud");
        
        if (this.insertarAutomat==true){
         validar_entidad();
         
            //Verifica que el tipo de solicitud enviada sera RENUNCIA PARCIAL para cambiar el parametro de nombreTipoSolicitud y el idTipoSolicitud
          if (this.gettipoSolicitud()!=null) {
            String vTipoSol = this.gettipoSolicitud().toString();
            if(vTipoSol.equals("RENUNCIA PARCIAL") || vTipoSol.equals("BAJA SUPENCION LABORAL")){
                Object vResult=utils_DB.getEjecutarQuerry(this.getDBTransaction(),"select id_tipo_solicitud from sis_tipo_solicitud where nombre_solicitud='" + vTipoSol+"'");    
                this.setIdTipoSolicitud(utils.getNumberOracle(vResult));   
                this.nombTipoSol = vTipoSol;
            }
          }
            //-----------------------------------------------------------
            
             if (operation == this.DML_INSERT || operation == this.DML_UPDATE){
                 if (this.nombTipoSol!=null && this.nombTipoSol.equals("RENUNCIA PARCIAL") ) {
                               sqlQuerry = "select  count(*) " + 
                               "from sis_contrato cont " + 
                               "where cont.id_contrato = " +  this.getIdContrato() + 
                               "and tipo_validez = 'I'"  
                               ;
                               
                               vResultadoConsulta=utils_DB.getEjecutarQuerry(this.getDBTransaction(),sqlQuerry);
                               
                                 if (vResultadoConsulta!=null && !vResultadoConsulta.toString().equals("0"))
                                 {
                                    throw new JboException("No es posible crear una Renuncia Parcial si el contrato es indefinido.");   
                                 }
                 
                               
                               sqlQuerry = "select  count(*) " + 
                               "from sis_solicitud s " + 
                                   "inner join sis_tipo_solicitud ts on ts.id_tipo_solicitud = s.id_tipo_solicitud " + 
                               "left join sis_det_licencia det  on det.id_solicitud      = s.id_solicitud " + 
                               "inner join sis_contrato cont on cont.id_contrato = s.id_contrato " + 
                               "where s.id_contrato = " +  this.getIdContrato() + 
                               "and " + 
                               "( ts.nombre_solicitud = 'RENUNCIA' OR ts.NOMBRE_SOLICITUD = 'RENUNCIA PARCIAL' " + 
                               "  or " + 
                               "  ( ts.nombre_solicitud = 'LICENCIA' " + 
                               "    and " + 
                               "    to_date('"+this.getFechaSolicitud()+"','yyyy-mm-dd') between cont.fecha_inicio and nvl (nvl(det.FECHA_FIN, cont.FECHA_FIN), '31/12/'||extract(year from sysdate)) " + 
                               "  ) " + 
                               ")";
                               
                               vResultadoConsulta=utils_DB.getEjecutarQuerry(this.getDBTransaction(),sqlQuerry);
                               
                                 if (vResultadoConsulta!=null && !vResultadoConsulta.toString().equals("0"))
                                 {
                                    throw new JboException("No es posible crear una Renuncia Parcial si se tiene ingresada una licencia, renuncia total o renuncia parcial dentro del periodo.");   
                                 }
                           
                           }
             }
            
        if (operation==this.DML_INSERT){ 
             
          if (this.nombTipoSol!=null && this.nombTipoSol.equals("ALTA SUSPENSION IGSS"))
          {
              sqlQuerry="select count(0) from sis_contrato a, sis_estado_contrato_vw b\n" + 
              "where  b.id_estado=a.estatus and nombre='SUSPENDIDO IGSS' and registro_empleado="+this.getRegistroEmpleado();             
              
              vResultadoConsulta=utils_DB.getEjecutarQuerry(this.getDBTransaction(),sqlQuerry);
              
              if (vResultadoConsulta==null || vResultadoConsulta.toString().equals("0"))
              {
                 
                 vFechaI=this.getSisSuspencionIgss().getAllRowsInRange()[0].getAttribute("FechaAltaSuspIgss"); 
                 
                  //this.getSisSuspencionIgss().getAllRowsInRange()[0].getAttribute("FechaAltaSuspIgss"); 
                 
                 if (vFechaI!=null){
                     
                     vFechaSuspI=utils.getFechaFormato("dd/MM/yyyy",(Date)vFechaI);
                     sqlQuerry=             
                     "select count(0) \n" + 
                     "from sis_solicitud sol, sis_suspencion_igss susp \n" + 
                     "where sol.registro_empleado= " + this.getRegistroEmpleado() + 
                     "and sol.id_solicitud=susp.id_solicitud\n" + 
                     "and sol.id_estado=39\n" + 
                     "and id_tipo_solicitud=2\n" + 
                     "and susp.fecha_inicio_susp_igss<=to_date('" +vFechaSuspI+"','dd/mm/yyyy') "+ 
                     "and susp.FECHA_ALTA_SUSP_IGSS is null";
                     
                     vResultadoConsulta=utils_DB.getEjecutarQuerry(this.getDBTransaction(),sqlQuerry);
                     
                     if (vResultadoConsulta==null || vResultadoConsulta.toString().equals("0")){
                         throw new JboException("El empleado no esta suspendido por el IGSS");   
                     }
                 }                                                             
              }
          }else if (this.nombTipoSol!=null && this.nombTipoSol.equals("SUSPENSION IGSS"))   
          {


              sqlQuerry=             
                "select count(0) \n " + 
                "from sis_solicitud sol, sis_suspencion_igss susp \n " + 
                "where sol.registro_empleado= " + this.getRegistroEmpleado() + 
                "and sol.id_solicitud=susp.id_solicitud\n " + 
                "and sol.id_estado=39\n " + 
                "and id_tipo_solicitud=2\n " +                      
                "and susp.FECHA_ALTA_SUSP_IGSS is null ";
              
                vResultadoConsulta=utils_DB.getEjecutarQuerry(this.getDBTransaction(),sqlQuerry);
              
                if (vResultadoConsulta!=null && !vResultadoConsulta.toString().equals("0"))
                {
                   throw new JboException("El empleado tiene una suspensión IGSS vigente, para ingresar otra suspensiï¿½n IGSS tiene que ingresar el alta.");   
                }             
          }else if(this.nombTipoSol!=null && this.nombTipoSol.equals("FALLECIMIENTO"))
          {
              sqlQuerry=             
                "select count(0) from sis_solicitud\n" + 
                "where registro_empleado="+ this.getRegistroEmpleado() + " "+
                "and id_tipo_solicitud=9\n" + 
                "and id_estado not in (38)";
              
                vResultadoConsulta=utils_DB.getEjecutarQuerry(this.getDBTransaction(),sqlQuerry);
              
                if (vResultadoConsulta!=null && !vResultadoConsulta.toString().equals("0"))
                {
                   throw new JboException("Ya existe un fallecimiento ingresado, por favor revise.");   
                }             
              
          }
         
               SequenceImpl sequence = new SequenceImpl("SIS_SOLICITUD_SQ",e.getDBTransaction());
               IdSol=sequence.getSequenceNumber();

               vFilas=this.getSisDetLicencia();
               while(vFilas.hasNext()){
                   vFilaLicencia=(SisDetLicenciaImpl)vFilas.next();
                   vFilaLicencia.setIdSolicitud(IdSol);
                 //  System.out.println("identificador orden licencia");
               }
               //pasa el id de la solicitud a la orden_Judicial
               vFilas = this.getSisOrdenJudicial();
               while(vFilas.hasNext()){
                  vFilaOrdenJudicial=(SisOrdenJudicialImpl)vFilas.next();
                  vFilaOrdenJudicial.setIdSolicitud(IdSol);
                 // System.out.println("identificador orden judicial");
                 // System.out.println(IdSol);
                }
                
                //pasa el id de la solicitud de la suspencion igss
                vFilas=this.getSisSuspencionIgss();
                 while(vFilas.hasNext()){
                     vFilaSuspencion=(SisSuspencionIgssImpl)vFilas.next();
                     vFilaSuspencion.setIdSolicitud(IdSol);
                 }
                 
                 
                //pasa el id de la solicitud de la suspencion igss
                vFilas=this.getSisDeudor();
                 while(vFilas.hasNext()){
                     vFilaDeudores=(SisDeudorImpl)vFilas.next();
                     vFilaDeudores.setIdSolicitud(IdSol);
                 }
                 
                 //pasa el id de la solicitud a sis_prom_reclaf
                 vFilas=this.getSisPromReclaf();
                 while(vFilas.hasNext()){
                     vFilaPromReclaf=(SisPromReclafImpl)vFilas.next();
                     vFilaPromReclaf.setIdSolicitud(IdSol);
                 }
                    
                 
               IdEstado=utils_DB.getIdEstado(e.getDBTransaction(),"GRABADO","SIS_SOLICITUD");
               this.setIdEstado(IdEstado);
               this.setIdSolicitud(IdSol);
                //this.ObtenerIdSolcilitud(e.getDBTransaction()); 
                
        }
        
        //this.getCreadoPor()
        // this.getSisDetLicencia()

         try{
         
         registrarLogOperaciones(operation, e);         
         super.doDML(operation, e);
         
         }catch(Exception exep){
             
           throw new JboException(utils.getSQLMensaje(exep));  
         }
         
        }
    }


    public void ObtenerIdSolcilitud(DBTransaction e){
        SequenceImpl sequence = new SequenceImpl("SIS_SOLICITUD_SQ",e);
        this.setIdSolicitud(sequence.getSequenceNumber());
        
    }

