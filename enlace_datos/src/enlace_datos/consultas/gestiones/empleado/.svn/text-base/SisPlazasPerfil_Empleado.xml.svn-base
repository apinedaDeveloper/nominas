<ViewObject
   Name="SisPlazasPerfil_Empleado"
   BindingStyle="OracleName"
   CustomQuery="true"
   ComponentClass="enlace_datos.consultas.gestiones.empleado.SisPlazasPerfil_EmpleadoImpl"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
select 
a.id_contrato, 
b1.formato as partida,
a.id_partida,
c.id_puesto,
a.numero_plaza,
a.correlativo,
case when a.id_puesto in (999990,999997,999994,999999) then 
   case when a.nom_plaza is not null 
     then a.nom_plaza when pl.nomplaza is not null 
     then pl.nomplaza 
     else c.NOMBRE 
   end 
else c.NOMBRE 
end AS NOM_PUESTO,
e.nombre as nomDependencia,
a.horascontratadas,
pl.CANTIDAD_HORAS,
a.fecha_inicio,
a.fecha_fin,
a.registro_empleado,
b.nombre as estado
from 
sis_contrato a,
sis_estado_contrato_vw b,
sis_puesto c,
sis_partida2_vw b1,
sis_dependencia e,
sis_plaza pl
where 
a.estatus=b.id_estado
and c.id_puesto=a.id_puesto
and pl.id_partida=a.id_partida
and pl.numero_plaza=a.numero_plaza
and pl.id_puesto=a.id_puesto
and pl.correlativo=a.correlativo
and b1.id_partida=a.id_partida
and b1.d3d4=e.id_dependencia
and b.nombre in ('ACTIVO','BAJA LICENCIA','BAJA ANEXO','SUSPENDIDO IGSS','REVISANDO')
   ]]></SQLQuery>
