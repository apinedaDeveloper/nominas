<ViewObject
   Name="Sis_ListLic_empleado_vw"
   BindingStyle="OracleName"
   CustomQuery="true"
   ComponentClass="enlace_datos.consultas.gestiones.empleado.Sis_ListLic_empleado_vwImpl"
   MsgBundleClass="enlace_datos.consultas.gestiones.empleado.common.Sis_ListLic_empleado_vwRowImplMsgBundle"
   UseGlueCode="false" >
   <DesignTime>
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.42.70" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|Msg|VarAccess" />
   </DesignTime>
   <Variable
      Name="pRegEmpleado"
      Kind="where"
      Type="oracle.jbo.domain.Number" >
   </Variable>
   <SQLQuery><![CDATA[
select 
sol.id_solicitud,
sol.registro_empleado,
lic.fecha_inicio,
lic.fecha_fin,
sol.fecha_creacion,
decode(lic.TIPO_LICENCIA,'SS','SIN GOCE DE SUELDO','CS','CON GOCE DE SUELDO') tipo_licencia,
sol.id_partida,
sol.id_puesto,
sol.numero_plaza,
sol.anio,
part.formato2 as partida,
 pst.nombre
from sis_solicitud sol, sep_partida_vw part,
sis_det_licencia lic,
sis_puesto pst
where sol.id_solicitud=lic.id_solicitud
and sol.origen='E'
and registro_empleado=:pRegEmpleado
and part.id_partida=sol.id_partida
and sol.id_puesto=pst.id_puesto
   ]]></SQLQuery>
