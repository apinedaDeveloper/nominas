      <Attr Name="_codeGenFlag2" Value="Access|Coll|Prog|VarAccess" />
   </DesignTime>
   <Variable
      Name="pUsuario"
      Kind="where"
      Type="java.lang.String" >
   </Variable>
   <SQLQuery><![CDATA[
select id_contrato, d.formato1 as partida,a.numero_plaza,c.nombre as nom_puesto,e.nombre as nomDependencia,a.horascontratadas,fecha_inicio,fecha_fin,a.registro_empleado,b.nombre as estado,
d.id_partida
from sis_contrato a,sis_estado_contrato_vw b,sis_puesto c,sis_partida_vw d,sis_dependencia e
where a.estatus=b.id_estado
and c.id_puesto=a.id_puesto
and d.id_partida=a.id_partida
and d.id_dependencia=e.id_dependencia
and b.nombre='ACTIVO'
and 
(
    exists (select 1 from sis_acceso_dependencia x
        where x.id_dependencia=d.id_dependencia
        and x.usuario=:pUsuario)
    or
    exists ( select 1 from sis_acceso_unidad y
        where y.id_programa=d.id_programa
            and y.id_dependencia=d.id_dependencia
            and y.id_unidad=d.id_unidad
            and y.usuario=:pUsuario
                                    )     
)
and sysdate between a.fecha_inicio and nvl(a.fecha_fin,sysdate)
   ]]></SQLQuery>
