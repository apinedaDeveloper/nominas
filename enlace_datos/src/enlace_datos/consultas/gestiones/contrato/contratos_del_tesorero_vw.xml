   </DesignTime>
   <Variable
      Name="pUsuario"
      Kind="where"
      Type="java.lang.String" >
   </Variable>
   <SQLQuery><![CDATA[
select empl.registro_empleado,
empl.cui,
empl.nombrecompleto,
cnt.id_contrato,
cntes.nombre as estadoContrato,
prt.formato as nopartida,
cnt.numero_plaza,
pst.id_puesto || ' - ' ||pst.nombre as puesto,
cnt.fecha_inicio,
cnt.fecha_fin,
cnt.estatus
from sis_contrato cnt, sis_partida2_vw prt, sis_empleado_vw empl, sis_estado_contrato_vw cntes,sis_puesto pst
where cnt.id_partida=prt.id_partida
and cnt.estatus=cntes.id_estado
and cntes.nombre in ('ACTIVO','SUSPENDIDO IGSS','REVISANDO','OBJETADO','BAJA LICENCIA','INGRESADO')
and cnt.registro_empleado=empl.registro_empleado
and pst.id_puesto=cnt.id_puesto
and
exists(
select 1 from usuario_unidad b, partida c
where b.id_partida=c.id_partida
and usuario=:pUsuario
and b.tipo_usuario='TRABAJADOR'
and sysdate between b.vigencia_de and nvl(b.vigencia_a,sysdate)
and prt.aniomanual=c.aniomanual
and prt.d1=c.d1
and prt.d2=c.d2
and prt.d3d4=c.d3d4
and 
(
(
prt.d7=c.d7
and prt.d6=c.d6
and prt.d5=c.d5
)
or
(
c.d5 is null
and c.d6 is null
and c.d7 is null 
)
)
)
order by nopartida,numero_plaza
   ]]></SQLQuery>
