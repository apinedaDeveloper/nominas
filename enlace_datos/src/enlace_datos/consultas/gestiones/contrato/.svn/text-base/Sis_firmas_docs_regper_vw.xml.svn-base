<ViewObject
   Name="Sis_firmas_docs_regper_vw"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="enlace_datos.consultas.gestiones.contrato.Sis_firmas_docs_regper_vwRowImpl"
   ComponentClass="enlace_datos.consultas.gestiones.contrato.Sis_firmas_docs_regper_vwImpl"
   UseGlueCode="false" >
   <DesignTime>
      <Attr Name="_isCodegen" Value="true" />
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.42.70" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <Variable
      Name="pRegper"
      Kind="where"
      Type="oracle.jbo.domain.Number" >
   </Variable>
   <SQLQuery><![CDATA[
select distinct nombre||'|'||cargo as firma,nombre,cargo,nivel from SIS_AUTH_CONTRATO_DATOS_VW 
where id_auth_contrato in  
(
select id_auth_contrato from sis_auth_contrato auth 
  where fecha_baja is null  
  and exists 
  (
    select 1 from partida part 
      where
    d1=4 and d8 is null and d9d10 is null 
    and exists 
      (
        select 1 from sis_contrato cntr,partida c
        where cntr.id_partida=c.id_partida  
             and cntr.estatus in (4,36,14)
             and cntr.registro_empleado=:pRegper
             and part.aniomanual=c.aniomanual 
             and part.d1=c.d1 
             and part.d2=c.d2 
             and part.d3d4=c.d3d4 
             and 
               ( 
                   (
                     part.d7=c.d7 
                     and part.d6=c.d6 
                     and part.d5=c.d5 
                     ) 
                     or 
                     (
                     c.d5 is null 
                     and c.d6 is null 
                     and c.d7 is null  
                     ) 
                 ) 
             ) 
     and 
     (
        auth.id_dependencia=part.d3d4 
        or
        ( 
            auth.id_unidad_prog=to_number(part.d1||d2) 
            and auth.id_unidad_depen=part.d3d4
            and auth.id_unidad=to_number(part.d5||d6||d7) 
        ) 
    )
    )        
    )
UNION
SELECT 'SEÑORES|CONSEJO DIRECTIVO','SEÑORES CONSEJO DIRECTIVO','CONSEJO DIRECTIVO',4
FROM DUAL
UNION
SELECT 'SEÑORES|JUNTA DIRECTIVA','SEÑORES JUNTA DIRECTIVA','JUNTA DIRECTIVA',4
FROM DUAL
   ]]></SQLQuery>
