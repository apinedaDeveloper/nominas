    /**Gets the attribute value for the calculated attribute DetLicenciaProrroga
     */
    public String getDetLicenciaProrroga() {
  
        String vQuerry;
        String [] vValores;
        Object vResultado;
        
         if (this.getIdDetLicProrroga()!=null){
           if (vCambioIdprorroga){
             vQuerry="select id_solicitud||'|'||decode(tipo_licencia,'SS','Sin goce de sueldo','Con goce de sueldo')||'|'||to_char(fecha_inicio,'dd/mm/yyyy')||'|'||to_char(fecha_fin,'dd/mm/yyyy')||'|'||desc_motivo_licencia as detalle \n" + 
             "from sis_det_licencia\n" + 
             "where id_det_licencia="+this.getIdDetLicProrroga();
             
             vResultado=utils_DB.getEjecutarQuerry(this.getEntity(0).getDBTransaction(),vQuerry);
             vValores=vResultado.toString().split("\\|");
             vDetalleLicencia="<ul><b>Información de la licencia:</b>";
             vDetalleLicencia=vDetalleLicencia+"<li><i>Id:</i>"+vValores[0]+"</li>";
             vDetalleLicencia=vDetalleLicencia+"<li><i>Tipo licencia:</i>"+vValores[1]+"</li>";
             vDetalleLicencia=vDetalleLicencia+"<li><i>Fecha inicio:</i>"+vValores[2]+"</li>";
             vDetalleLicencia=vDetalleLicencia+"<li><i>Fecha fin:</i>"+vValores[3]+"</li>";
             vDetalleLicencia=vDetalleLicencia+"<li><i>Motivo:</i>"+vValores[4]+"</li>";
             vDetalleLicencia=vDetalleLicencia+"</ul>";
           
           }
             vCambioIdprorroga=false;  
         }else{
             
             vDetalleLicencia="";
         }
    
        return vDetalleLicencia;
    }

