   RowClass="enlace_datos.consultas.gestiones.post_mortem.SisPostMortem_vwRowImpl"
   ComponentClass="enlace_datos.consultas.gestiones.post_mortem.SisPostMortem_vwImpl"
   MsgBundleClass="enlace_datos.consultas.gestiones.post_mortem.common.SisPostMortem_vwRowImplMsgBundle"
   ComponentInterface="enlace_datos.consultas.gestiones.post_mortem.common.SisPostMortem_vw"
   ClientProxyName="enlace_datos.consultas.gestiones.post_mortem.client.SisPostMortem_vwClient"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT 	SisIndem.ID_TIPO_PRESTACION,
	SisIndem.ANIO,
	SisIndem.CORRELATIVO_ANIO,
	SisIndem.REGISTRO_EMPLEADO,
	SisIndem.ID_SOLICITUD,
	SisIndem.ANIOS_TIEMPO_SERVICIO,
	SisIndem.MESES_TIEMPO_SERVICIO,
	SisIndem.DIAS_TIEMPO_SERVICIO,
	SisIndem.ID_BENEFICIARIO_SOL,
	SisIndem.ID_ESTADO,
	SisIndem.TOTAL_SUELDOS,
	SisIndem.SUELDO_PROMEDIO,
	SisIndem.TOTAL_PRESTACION,
	SisIndem.GASTO_FUNERARIO,
	SisIndem.FECHA_RETIRO,
	SisIndem.FECHA_SOLICITUD,
	SisIndem.TIPO_RETIRO,
	SisIndem.CREADO_POR,
	SisIndem.FECHA_CREACION,
	SisIndem.MODIFICADO_POR,
	SisIndem.FECHA_MOD,
	SisIndem.NUM_CHEQUE,
	SisIndem.ID_INDEMNIZACION,
	empl.NOMBRECOMPLETO, empl.cui,
	empl.ID_ESTADO AS ID_ESTADO_TRABAJADOR,
	sisEstado.NOMBRE as NOMBRE_ESTADO_INDEMNIZACION,
	sisTipoIndem.NOMBRE as NOMBRE_TIPO_INDEMNIZACION,
	sisEstadoEmpl.NOMBRE as NOMBRE_ESTADO_TRABAJADOR,
	sisSol.FECHA_SOLICITUD as FECHA_EXP_FALLECIMIENTO,
	sisSol.OBSERVACION as OBSERVACION_FALLECIMIENTO,
	(SisIndem.SUELDO_PROMEDIO * SisIndem.ANIOS_TIEMPO_SERVICIO) as PRESTAC_ANIOS,
	(SisIndem.SUELDO_PROMEDIO * SisIndem.MESES_TIEMPO_SERVICIO / 12) as PRESTAC_MESES,
	(SisIndem.SUELDO_PROMEDIO * SisIndem.DIAS_TIEMPO_SERVICIO / 365) as PRESTAC_DIAS,
	SisIndem.SUELDO_PROMEDIO as SUELDO_PROMEDIO_LECTURA,
	SisIndem.GASTO_FUNERARIO as GASTO_FUNERARIO_LECTURA
FROM SIS_INDEMNIZACION SisIndem 
INNER JOIN SIIF.SIS_EMPLEADO_VW empl ON SisIndem.REGISTRO_EMPLEADO = empl.REGISTRO_EMPLEADO
INNER JOIN SIIF.SIS_ESTADO sisEstado ON SisIndem.ID_ESTADO = sisEstado.ID_ESTADO
INNER JOIN SIIF.SIS_TIPO_INDEMNIZACION sisTipoIndem ON SisIndem.ID_TIPO_PRESTACION = sisTipoIndem.ID_TIPO_INDEMNIZACION
INNER JOIN SIIF.SIS_ESTADO sisEstadoEmpl ON empl.ID_ESTADO = sisEstadoEmpl.ID_ESTADO
LEFT JOIN SIIF.SIS_SOLICITUD sisSol ON SisIndem.ID_SOLICITUD = sisSol.ID_SOLICITUD
   ]]></SQLQuery>
