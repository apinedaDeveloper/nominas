<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">

<ViewObject
   Name="SisProgramasTrabajador_vw"
   OrderBy="id_programa asc"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="enlace_datos.consultas.gestiones.indemnizaciones.SisProgramasTrabajador_vwRowImpl"
   ComponentClass="enlace_datos.consultas.gestiones.indemnizaciones.SisProgramasTrabajador_vwImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <DesignTime>
      <Attr Name="_isCodegen" Value="true" />
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.42.70" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <Variable
      Name="pRegistroEmpleado"
      Kind="where"
      Type="oracle.jbo.domain.Number" >
   </Variable>
   <SQLQuery><![CDATA[
select unique part.id_programa, prog.nombre from sis_partida part
inner join sis_programa prog on part.id_programa = prog.id_programa
where id_partida in (
	select unique id_partida from sis_contrato
	where estatus not in (11) and registro_empleado = :pRegistroEmpleado
)
   ]]></SQLQuery>
