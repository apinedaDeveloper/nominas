<ViewObject
   Name="SisIndemnizacion_vw"
   OrderBy="ID_INDEMNIZACION DESC"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="enlace_datos.consultas.gestiones.indemnizaciones.SisIndemnizacion_vwRowImpl"
   ComponentClass="enlace_datos.consultas.gestiones.indemnizaciones.SisIndemnizacion_vwImpl"
   ComponentInterface="enlace_datos.consultas.gestiones.indemnizaciones.common.SisIndemnizacion_vw"
   ClientProxyName="enlace_datos.consultas.gestiones.indemnizaciones.client.SisIndemnizacion_vwClient"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <SQLQuery><![CDATA[
SELECT SisIndem.ID_TIPO_PRESTACION, 
       SisIndem.ANIO, 
       SisIndem.CORRELATIVO_ANIO, 
       SisIndem.REGISTRO_EMPLEADO, 
       SisIndem.ID_SOLICITUD, 
       SisIndem.ANIOS_TIEMPO_SERVICIO, 
       SisIndem.MESES_TIEMPO_SERVICIO, 
       SisIndem.DIAS_TIEMPO_SERVICIO, 
       SisIndem.ID_BENEFICIARIO_SOL, 
       SisIndem.ID_ESTADO, 
       SisIndem.TOTAL_SUELDOS, 
       SisIndem.DIFERIDO_CALC, 
       SisIndem.DIFERIDO_12_CALC, 
       SisIndem.AGUINALDO_CALC, 
       SisIndem.BONO14_CALC, 
       SisIndem.SUELDO_PROMEDIO, 
       SisIndem.TOTAL_PRESTACION, 
       SisIndem.GASTO_FUNERARIO,
       SisIndem.FECHA_RETIRO,
       SisIndem.FECHA_SOLICITUD,
       SisIndem.TIPO_RETIRO, 
       SisIndem.CREADO_POR,
       SisIndem.FECHA_CREACION, 
       SisIndem.MODIFICADO_POR,
       SisIndem.FECHA_MOD, 
       SisIndem.NUM_CHEQUE,
       SisIndem.ID_INDEMNIZACION,
       SisIndem.PROGRAMA,
       SisIndem.ID_PADRE,
       DECODE (SisIndem.TIPO_RETIRO,1,'RENUNCIA',2,'DESPIDO',3,'FINALIZACIÓN DE CONTRATO','DESCONOCIDO') as TIPO_RETIRO_NOMBRE,
       empl.NOMBRECOMPLETO, Trim(To_Char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''')) as CUI,
       empl.ID_ESTADO AS ID_ESTADO_TRABAJADOR,
       sisEstado.NOMBRE as NOMBRE_ESTADO_INDEMNIZACION,
       sisTipoIndem.NOMBRE as NOMBRE_TIPO_INDEMNIZACION,
       sisEstadoEmpl.NOMBRE as NOMBRE_ESTADO_TRABAJADOR,
       sisSol.FECHA_SOLICITUD as FECHA_EXPEDIENTE_RETIRO,
       sisSol.OBSERVACION as OBSERVACION_EXP_RETIRO,
       (SisIndem.DIFERIDO_CALC + SisIndem.DIFERIDO_12_CALC + SisIndem.AGUINALDO_CALC + SisIndem.BONO14_CALC) as TOTAL_PRESTACIONES,
       (SisIndem.DIFERIDO_CALC + SisIndem.DIFERIDO_12_CALC + SisIndem.AGUINALDO_CALC + SisIndem.BONO14_CALC + TOTAL_SUELDOS) as TOTAL_PARA_CALCULO_SUELDO_PROM,
	(SisIndem.SUELDO_PROMEDIO * SisIndem.ANIOS_TIEMPO_SERVICIO) as INDEM_ANIOS,
	(SisIndem.SUELDO_PROMEDIO * SisIndem.MESES_TIEMPO_SERVICIO / 12) as INDEM_MESES,
	(SisIndem.SUELDO_PROMEDIO * SisIndem.DIAS_TIEMPO_SERVICIO / 365) as INDEM_DIAS
FROM SIS_INDEMNIZACION SisIndem
INNER JOIN SIIF.SIS_EMPLEADO_VW empl ON SisIndem.REGISTRO_EMPLEADO = empl.REGISTRO_EMPLEADO
INNER JOIN SIIF.SIS_ESTADO sisEstado ON SisIndem.ID_ESTADO = sisEstado.ID_ESTADO
INNER JOIN SIIF.SIS_TIPO_INDEMNIZACION sisTipoIndem ON SisIndem.ID_TIPO_PRESTACION = sisTipoIndem.ID_TIPO_INDEMNIZACION
INNER JOIN SIIF.SIS_ESTADO sisEstadoEmpl ON empl.ID_ESTADO = sisEstadoEmpl.ID_ESTADO
LEFT OUTER JOIN SIIF.SIS_SOLICITUD sisSol ON SisIndem.ID_SOLICITUD = sisSol.ID_SOLICITUD
   ]]></SQLQuery>
