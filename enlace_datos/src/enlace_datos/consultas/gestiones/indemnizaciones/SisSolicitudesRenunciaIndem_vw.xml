   <Variable
      Name="vRegistroPersonal"
      Kind="where"
      Type="java.lang.String" >
   </Variable>
   <SQLQuery><![CDATA[
select sol.id_solicitud, 
sol.registro_empleado, 
sol.fecha_solicitud, 
Trim(To_Char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''')) cui,
empl.nombre1 || NVL2 (empl.nombre2, ' ' || empl.nombre2, '') || NVL2 (empl.nombre3, ' ' || empl.nombre3, '') || 
' ' || empl.apellido1 || NVL2 (empl.apellido2, ' ' || empl.apellido2, '') || NVL2 (empl.apellidocasada, ' de ' || empl.apellidocasada, '') as nombreCompleto,
sol.id_contrato, 
sol.observacion, 
sol.creado_por, 
sol.fecha_creacion, 
sol.id_partida, 
sol.id_puesto, 
sol.numero_plaza 
from siif.sis_solicitud sol
inner join siif.sis_empleado empl on sol.registro_empleado = empl.registro_empleado
where sol.id_tipo_solicitud in(5) and sol.id_estado = 39 and sol.registro_empleado = :vRegistroPersonal
   ]]></SQLQuery>
