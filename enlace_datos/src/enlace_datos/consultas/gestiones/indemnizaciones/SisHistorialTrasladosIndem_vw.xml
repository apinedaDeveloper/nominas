      <Attr Name="_version" Value="10.1.3.42.70" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <Variable
      Name="pIdIndemnizacion"
      Kind="where"
      Type="oracle.jbo.domain.Number" >
   </Variable>
   <SQLQuery><![CDATA[
SELECT SisAutorizacionDet.ID_AUTORIZACION_DET, 
       SisAutorizacionDet.ID_AUTORIZACION, 
       SisAutorizacionDet.ID_ESTADO, 
       SisAutorizacionDet.USUARIO, 
       SisAutorizacionDet.FECHA, 
       SisAutorizacionDet.COMENTARIO,
	EST.NOMBRE AS NOMBRE_ESTADO
FROM SIS_AUTORIZACION_DET SisAutorizacionDet
INNER JOIN SIS_AUTORIZACION AUT on SisAutorizacionDet.id_autorizacion = AUT.id_autorizacion and AUT.id_indemnizacion = :pIdIndemnizacion
INNER JOIN SIS_ESTADO EST on SisAutorizacionDet.id_estado = EST.id_estado
   ]]></SQLQuery>
