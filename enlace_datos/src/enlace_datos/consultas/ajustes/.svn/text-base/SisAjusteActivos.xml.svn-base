   <Variable
      Name="pUsuario"
      Kind="where"
      Type="java.lang.String" >
   </Variable>
   <SQLQuery><![CDATA[
select id_ajuste,id_tipoajuste,nombre,opcional,removible,modificable,nivel,aplicacion
from sis_ajuste
where fecha_baja is null
and id_ajuste in(
select id_ajuste from sis_perfil_ajuste perf, usuario_perfil perfu
where perf.perfil=perfu.perfil
and perfu.usuario=:pUsuario
)
and forma_utilizar ='SIMPLE'
   ]]></SQLQuery>
