   ComponentClass="enlace_datos.consultas.empleado.sis_licencias_consulImpl"
   MsgBundleClass="enlace_datos.consultas.empleado.common.sis_licencias_consulRowImplMsgBundle"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <DesignTime>
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.42.70" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|Msg|Prog|VarAccess" />
   </DesignTime>
   <Variable
      Name="pUsuario"
      Kind="where"
      Type="java.lang.String" >
   </Variable>
   <SQLQuery><![CDATA[
select a.id_solicitud,b.nombre_solicitud,g.nombre as motivo,a.registro_empleado,h.nombrecompleto, h.cui,f.formato2 as partida,f.id_dependencia,e.id_puesto,e.numero_plaza,decode(d.tipo_licencia,'SS','Sin goce de sueldo','CS','Con goce de sueldo') as tipoLicencia,d.fecha_inicio,d.fecha_fin,d.procede,d.fecha_resolucion,a.fecha_creacion,c.nombre as nomEstado,d.horas_baja,a.id_estado
, d.observ_resolucion, d.DESC_MOTIVO_LICENCIA
from sis_solicitud a,sis_tipo_solicitud b,sis_estado_solicitud_vw c, sis_det_licencia d, sis_contrato e,sis_partida_vw f, sis_motivo_det_licencia g, sis_empleado_vw h,partida part
where a.id_tipo_solicitud=b.id_tipo_solicitud
and d.id_solicitud=a.id_solicitud
and a.registro_empleado=h.registro_empleado
and g.id_motivo_det_licencia=d.id_motivo_det_licencia
and e.id_contrato=a.id_contrato
and e.id_partida=f.id_partida
and part.id_partida=f.id_partida
and c.id_estado=a.id_estado
and
exists(
select 1 from usuario_unidad b, partida c
where b.id_partida=c.id_partida
and usuario=:pUsuario
and b.tipo_usuario='TRABAJADOR'
and sysdate between b.vigencia_de and nvl(b.vigencia_a,sysdate)
and part.aniomanual=c.aniomanual
and part.d1=c.d1
and part.d2=c.d2
and part.d3d4=c.d3d4
and 
(
(part.d7=c.d7
and part.d6=c.d6
and part.d5=c.d5
)
or
(
c.d5 is null
and c.d6 is null
and c.d7 is null 
)
)
)
   ]]></SQLQuery>
