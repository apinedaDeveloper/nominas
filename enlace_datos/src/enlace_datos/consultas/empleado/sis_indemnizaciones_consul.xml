   Name="sis_indemnizaciones_consul"
   OrderBy="FECHA_SOLICITUD DESC"
   BindingStyle="OracleName"
   CustomQuery="true"
   RowClass="enlace_datos.consultas.empleado.sis_indemnizaciones_consulRowImpl"
   ComponentClass="enlace_datos.consultas.empleado.sis_indemnizaciones_consulImpl"
   FetchMode="FETCH_AS_NEEDED"
   UseGlueCode="false" >
   <DesignTime>
      <Attr Name="_isCodegen" Value="true" />
      <Attr Name="_isExpertMode" Value="true" />
      <Attr Name="_version" Value="10.1.3.42.70" />
      <Attr Name="_codeGenFlag2" Value="Access|Coll|VarAccess" />
   </DesignTime>
   <Variable
      Name="pUsuario"
      Kind="where"
      Type="java.lang.String" >
   </Variable>
   <SQLQuery><![CDATA[
SELECT DISTINCT SisIndem.ID_INDEMNIZACION, 
       SisIndem.ANIO, 
       SisIndem.CORRELATIVO_ANIO, 
       SisIndem.REGISTRO_EMPLEADO, 
       SisIndem.ID_ESTADO, 
       SisIndem.TOTAL_PRESTACION, 
       SisIndem.FECHA_RETIRO,
       SisIndem.FECHA_SOLICITUD,
       SisIndem.FECHA_SOLICITUD AS FECHA_INICIO,
       SisIndem.TIPO_RETIRO, 
       SisIndem.CREADO_POR,
       SisIndem.FECHA_CREACION, 
       SisIndem.MODIFICADO_POR,
       SisIndem.FECHA_MOD, 
       (select id_dependencia from (select prt1.id_dependencia, contr.REGISTRO_EMPLEADO from sis_partida prt1
        inner join sis_contrato contr on prt1.id_partida  = contr.id_partida and contr.ESTATUS not in (11,32) order by contr.id_contrato DESC)
        where registro_Empleado=SisIndem.REGISTRO_EMPLEADO and rownum<=1) as id_dependencia,
       DECODE (SisIndem.TIPO_RETIRO,1,'RENUNCIA',2,'DESPIDO',3,'FINALIZACIÓN DE CONTRATO','DESCONOCIDO') as TIPO_RETIRO_NOMBRE,
       empl.NOMBRECOMPLETO, empl.CUI, SisEstado.NOMBRE as NOMBRE_ESTADO_INDEMNIZACION
FROM SIS_INDEMNIZACION SisIndem
INNER JOIN SIIF.SIS_EMPLEADO_VW empl ON SisIndem.REGISTRO_EMPLEADO = empl.REGISTRO_EMPLEADO
INNER JOIN SIIF.SIS_ESTADO sisEstado ON SisIndem.ID_ESTADO = sisEstado.ID_ESTADO
LEFT OUTER JOIN SIIF.SIS_CONTRATO sisCont ON SisIndem.REGISTRO_EMPLEADO = sisCont.REGISTRO_EMPLEADO
LEFT OUTER JOIN SIIF.SEP_PARTIDA_VW part ON sisCont.ID_PARTIDA = part.ID_PARTIDA
WHERE ID_TIPO_PRESTACION = 1
and exists( select 1 from usuario_unidad b, partida c where b.id_partida=c.id_partida
and usuario=:pUsuario and b.tipo_usuario='TRABAJADOR' and sysdate between b.vigencia_de and nvl(b.vigencia_a,sysdate)
and part.aniomanual=c.aniomanual and part.d1=c.d1 and part.d2=c.d2 and part.d3d4=c.d3d4 and 
( ( part.d7=c.d7 and part.d6=c.d6 and part.d5=c.d5 ) or ( c.d5 is null and c.d6 is null and c.d7 is null ) ) )
   ]]></SQLQuery>
