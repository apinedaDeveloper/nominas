         <query-list>
            <query>
               <name>ajuste_oblig_contrato</name>
               <query-type>oracle.toplink.queryframework.ReadAllQuery</query-type>
               <cache-usage>Check Cache by Primary Key</cache-usage>
               <lock-mode>Do Not Acquire Locks</lock-mode>
               <distinct-state>Uncomputed Distinct</distinct-state>
               <in-memory-query-indirection-policy>Throw Indirection Exception</in-memory-query-indirection-policy>
               <bind-all-parameters>true</bind-all-parameters>
               <cache-statement>false</cache-statement>
               <cache-query-results>false</cache-query-results>
               <maintain-cache>true</maintain-cache>
               <refresh-identity-map-result>true</refresh-identity-map-result>
               <refresh-remote-identity-map-result>true</refresh-remote-identity-map-result>
               <use-wrapper-policy>true</use-wrapper-policy>
               <prepare>false</prepare>
               <query-timeout>0</query-timeout>
               <maximum-rows>0</maximum-rows>
               <format>
                  <query-format>
                     <query-string>select id_ajuste,b.id_tipoajuste,a.nombre,opcional,nivel,sis_pkg_calculos.GETVALORAJUSTEXVAR(id_ajuste,#salario,#horas,1,1,sysdate) as calculado
from sis_ajuste a,sis_tipo_ajuste b
where
a.opcional='N'
and a.id_tipoajuste=b.id_tipoajuste
and not exists(
select 1 from sis_ajuste_exclud_unidad c
where a.id_ajuste=c.id_ajuste
and c.ID_PROGRAMA=#programa
and c.ID_DEPENDENCIA=#dependencia
and c.ID_UNIDAD=#unidad
)
order by b.operacion 
</query-string>
                     <query-format-class>MWSQLQueryFormat</query-format-class>
                  </query-format>
               </format>
               <parameter-list>
                  <query-parameter>
                     <name>programa</name>
                     <type>java.lang.Number</type>
                  </query-parameter>
                  <query-parameter>
                     <name>dependencia</name>
                     <type>java.lang.Number</type>
                  </query-parameter>
                  <query-parameter>
                     <name>unidad</name>
                     <type>java.lang.Number</type>
                  </query-parameter>
                  <query-parameter>
                     <name>salario</name>
                     <type>java.lang.Number</type>
                  </query-parameter>
                  <query-parameter>
                     <name>horas</name>
                     <type>java.lang.Number</type>
                  </query-parameter>
               </parameter-list>
            </query>
