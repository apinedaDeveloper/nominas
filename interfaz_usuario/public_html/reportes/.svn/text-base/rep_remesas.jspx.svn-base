            var final=lista.value;
            var vector = final.split("/");
            var anio = vector[2];
                        
            var vPeriodo=document.getElementById('form1:pag:hdn_inptPeriodo').value;
            if(vPeriodo > 9){
                //vPeriodo = 0+''+vPeriodo;
            }
            else{
                vPeriodo = 0+''+vPeriodo;
            }
            
            vFechaInicio = '01/'+vPeriodo+'/'+anio;
            vFechaFin = dias(vPeriodo,anio)+'/'+vPeriodo+'/'+anio;            
            window.open("../verreportex?id="+vIdRem+String.fromCharCode(38)+"rep="+"PLANILLA_IGSS"+String.fromCharCode(38)+"var1="+vFechaInicio+String.fromCharCode(38)+"var2="+vFechaFin+String.fromCharCode(38)+"id2="+vPeriodo,"preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1");             
        }
        
