        //ARCHIVOS CARGA COMPLEMENTARIAS IGSS
        function genReporteComplementariaIGSS(){
            var vIdRem=document.getElementById('form1:pag:hdn_inptRemesa').value                       
            $.getJSON('/Nomina/faces/consuljson?codop=13'+String.fromCharCode(38)+'var1='+vIdRem+String.fromCharCode(38)+"var2="+opcionReporteIgss, function(json){
                if (json!=null){  
                    if (json.length>0){                
                        var respuesta = json[0].respuesta;
                        vCadena=respuesta.split(",");
                        
                        globContador=vCadena.length;                        
                        if(globContador>0){
                            globContador--;
                            var valor = vCadena[globContador].split(";");                            
                            if(opcionReporteIgss == 0){
                                window.open("../verreportex?id="+vIdRem+String.fromCharCode(38)+"var1="+valor[0]+String.fromCharCode(38)+String.fromCharCode(38)+"var2="+valor[1]+String.fromCharCode(38)+"rep=PLANILLA_COMP_IGSS","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1");
                                setTimeout('reportes()', 20000);
                            }
                            else{
                                if (opcionReporteIgss == 1){
                                    window.open("../verreportex?id="+vIdRem+String.fromCharCode(38)+"var1="+valor[0]+String.fromCharCode(38)+String.fromCharCode(38)+"var2="+valor[1]+String.fromCharCode(38)+"rep="+"REMESA_PLANILLA_IGSS_COMP","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1");
                                    setTimeout('reportes()', 15000);
                                }
                                else{
                                    if (opcionReporteIgss == 2){
                                        alert("../verreportepdf?id="+vIdRem+String.fromCharCode(38)+"var1="+valor[0]+String.fromCharCode(38)+String.fromCharCode(38)+"var2="+valor[1]+String.fromCharCode(38)+"rep=REMESA_PLANILLA_IGSS_COMP","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1");
                                        window.open("../verreportepdf?id="+vIdRem+String.fromCharCode(38)+"var1="+valor[0]+String.fromCharCode(38)+String.fromCharCode(38)+"var2="+valor[1]+String.fromCharCode(38)+"rep=REMESA_PLANILLA_IGSS_COMP","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
                                        setTimeout('reportes()', 15000);
                                    }
                                }
                            }
                        }
                        
                        
                    }
                    else{
                        alert('Hubo un error, contacte al administrador del sistema');
                    }
                }
                
            });
           return false; 
        }
        
        function genCargaIGSSCompl(pTipoCarga){
            var vIdRem=document.getElementById('form1:pag:hdn_inptRemesa').value                       
            var vIdRempmt =document.getElementById('form1:pag:linkOutput') ;
            var cmdIdRempmt =document.getElementById('form1:pag:cmdCargaIGSSCompl') ;             
            var cmdIdRempmtf =document.getElementById('form1:pag:cmdCargaIGSSComplFisica') ;         
        
            document.getElementById('form1:pag:pnlCarga').style.visibility="visible";  
            $.getJSON('/Nomina/faces/consuljson?codop=11'+String.fromCharCode(38)+'var1='+vIdRem+String.fromCharCode(38)+'var2='+pTipoCarga+String.fromCharCode(38), function(json){
                if (json!=null){  
                    if (json.length>0){                     
                        var respuesta = json[0].respuesta;
                        if (respuesta != "error"){
                            vIdRempmt.href = "reportes_texto/" + respuesta;
                            if (pTipoCarga == 0)
                                cmdIdRempmt.style.display = 'none';
                            else
                                cmdIdRempmtf.style.display = 'none';
                                
                            vIdRempmt.style.display = 'block';
                            
                            document.getElementById('form1:pag:pnlCarga').style.visibility="hidden";
                    
                            alert('Descargar el archivo');
                            vIdRemesa = vIdRem;
                        }else{
                            alert('Error: Revisar número de Remesa.');
                            document.getElementById('form1:pag:pnlCarga').style.visibility="hidden";
                        }
                    }
                    else{
                        alert('Hubo un error, contacte al administrador del sistema');
                        document.getElementById('form1:pag:pnlCarga').style.visibility="hidden";
                    }
                }
                    
            }); 
            
           return false; 
        }
        
