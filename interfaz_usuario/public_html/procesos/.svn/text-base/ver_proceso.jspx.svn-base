        function openNewWindowSICOINANULADOS(idReporte){ 
                window.open("../verreportepdf?id="+idReporte+String.fromCharCode(38)+"rep=DOCACREDSICOINANULADOS","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
