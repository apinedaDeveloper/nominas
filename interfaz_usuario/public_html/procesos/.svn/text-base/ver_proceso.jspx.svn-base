        function openNewWindowDetSIS25(idReporte){ 
                window.open("../verreportepdf?id="+idReporte+String.fromCharCode(38)+"rep=DetSIS25","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
        }
        
         function openNewWindowDetSIS25EX(idReporte){          

              window.open("../verreporteexcel?id="+idReporte+String.fromCharCode(38)+"rep=SIS25_EXCEL"+String.fromCharCode(38),"preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable=1"); 
              return false;
            } 
        
        
        var res = "";
        var contador = 0;
        function valida(idProceso){
            if(contador == 0){
                $.getJSON("/Nomina/faces/consuljson?codop=9"+String.fromCharCode(38)+"var1="+idProceso+String.fromCharCode(38)+"var2=PROCESO"+String.fromCharCode(38)+"var2=null", function(json){
                    if (json!=null){  
                        if (json.length>0){                
                            res = json[0].respuesta;       
                            if(res=='CORRECTO'){
                                contador = contador +1;
                                document.getElementById('form1:_id0:btn_autorizar').click();
                                
                            }
                            else{
                                if (confirm(res)) {            
                                    window.open("../verreportepdf?id="+idProceso+String.fromCharCode(38)+"rep=EXCENNOMINA","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
                                } 
                            }
                        }
                        else{
                            alert('Hubo un error, contacte al administrador del sistema');
                        }
                    }                
                });
            }
            else{
                contador = 0;
                return true;
            }
            
            return false;        
        }
        
         
                
      </script> 
  
