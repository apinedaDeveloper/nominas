<?xml version='1.0' encoding='windows-1252'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces"
          xmlns:afh="http://xmlns.oracle.com/adf/faces/html">
  <jsp:directive.page contentType="text/html;charset=windows-1252"/>
  <f:view>
    <afh:html>
      <afh:head title="Provision de Indemnizacion Regimen Especial">
        <meta http-equiv="Content-Type"
              content="text/html; charset=windows-1252"/>
        <script language="javascript" type="text/javascript">        
       
         function mostrarReporte(idProc){          
     
              window.open("../verreportepdf?id="+idProc+String.fromCharCode(38)+"rep=REPPROY_INDEM","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable=1"); 
              return false;
            }   
            
         function mostrarReportePoliza(idProc){          
     
              window.open("../verreportepdf?id="+idProc+String.fromCharCode(38)+"rep=POLIZA_PROVINDEM","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable=1"); 
              return false;
            }      
            
        </script>
      </afh:head>
      <afh:body>
        <f:verbatim>
           
        </f:verbatim>
        <af:messages binding="#{backing_procesos_provision_indemnizacion.messages1}"
                     id="messages1"/>
        <af:form>
          <af:page title="Provision de Indemnizacion Regimen Especial" value="#{modeloMenu.modeloBD}" var="node">
            <f:facet name="nodeStamp">
              <af:commandMenuItem text="#{node.label}"
                                  action="#{node.getOutcome}"/>
            </f:facet>
            <f:facet name="infoUser">
              <f:subview id="info1">
                <jsp:include page="/global/facet_infoUser.jspx"/>
              </f:subview>
            </f:facet>
             <f:facet name="menuSwitch">
              <f:subview id="menuSwt">
                <jsp:include page="/global/facet_menuSwitch.jspx"/>
              </f:subview>
            </f:facet>
            <f:facet name="branding">
              <f:subview id="logo">
                <jsp:include page="/global/facet_branding.jspx"/>
              </f:subview>
            </f:facet>
            <af:panelHorizontal halign="center">
            <af:panelBox>
                <af:panelForm binding="#{backing_procesos_provision_indemnizacion.panelForm1}"
                              id="panelForm1">
                  <af:selectInputDate value="#{bindings.pFechaI.inputValue}"
                                      label="Fecha Inicio"
                                      required="#{bindings.pFechaI.mandatory}"
                                      binding="#{backing_procesos_provision_indemnizacion.selectInputDate1}"
                                      id="selectInputDate1">
                    <af:validator binding="#{bindings.pFechaI.validator}"/>
                    <f:convertDateTime pattern="#{bindings.pFechaI.format}"/>
                  </af:selectInputDate>
                  <af:selectInputDate value="#{bindings.pFechaF.inputValue}"
                                      label="Fecha Fin"
                                      required="#{bindings.pFechaF.mandatory}"
                                      binding="#{backing_procesos_provision_indemnizacion.selectInputDate2}"
                                      id="selectInputDate2">
                    <af:validator binding="#{bindings.pFechaF.validator}"/>
                    <f:convertDateTime pattern="#{bindings.pFechaF.format}"/>
                  </af:selectInputDate>
                  <af:panelHorizontal binding="#{backing_procesos_provision_indemnizacion.panelHorizontal1}"
                                      id="panelHorizontal1" halign="center">
                    <af:commandButton actionListener="#{bindings.ExecuteWithParams.execute}"
                                      text="Buscar"
                                      disabled="#{!bindings.ExecuteWithParams.enabled}"
                                      binding="#{backing_procesos_provision_indemnizacion.commandButton1}"
                                      id="commandButton1"/>
                  </af:panelHorizontal>
                </af:panelForm>
                <af:table value="#{bindings['sis_provisionRem_vw1'].collectionModel}"
                          var="row"
                          rows="#{bindings['sis_provisionRem_vw1'].rangeSize}"
                          first="#{bindings['sis_provisionRem_vw1'].rangeStart}"
                          emptyText="#{bindings['sis_provisionRem_vw1'].viewable ? 'No rows yet.' : 'Access Denied.'}"
                          binding="#{backing_procesos_provision_indemnizacion.table1}"
                          id="table1">
                  <f:facet name="selection">
                    <af:tableSelectOne binding="#{backing_procesos_provision_indemnizacion.tableSelectOne1}"
                                       id="tableSelectOne1" autoSubmit="true"/>
                  </f:facet>
                  <af:column sortProperty="IdRemesa" sortable="false"
                             headerText="#{bindings['sis_provisionRem_vw1'].labels.IdRemesa}"
                             binding="#{backing_procesos_provision_indemnizacion.column1}"
                             id="column1">
                    <af:outputText value="#{row.IdRemesa}"
                                   binding="#{backing_procesos_provision_indemnizacion.outputText1}"
                                   id="outputText1">
                      <f:convertNumber groupingUsed="false"
                                       pattern="#{bindings['sis_provisionRem_vw1'].formats.IdRemesa}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="Periodo" sortable="false"
                             headerText="#{bindings['sis_provisionRem_vw1'].labels.Periodo}"
                             binding="#{backing_procesos_provision_indemnizacion.column2}"
                             id="column2">
                    <af:outputText value="#{row.Periodo}"
                                   binding="#{backing_procesos_provision_indemnizacion.outputText2}"
                                   id="outputText2">
                      <f:convertNumber groupingUsed="false"
                                       pattern="#{bindings['sis_provisionRem_vw1'].formats.Periodo}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="Nomperiodo" sortable="false"
                             headerText="#{bindings['sis_provisionRem_vw1'].labels.Nomperiodo}"
                             binding="#{backing_procesos_provision_indemnizacion.column3}"
                             id="column3">
                    <af:outputText value="#{row.Nomperiodo}"
                                   binding="#{backing_procesos_provision_indemnizacion.outputText3}"
                                   id="outputText3"/>
                  </af:column>
                  <af:column sortProperty="FechaInicio" sortable="false"
                             headerText="#{bindings['sis_provisionRem_vw1'].labels.FechaInicio}"
                             binding="#{backing_procesos_provision_indemnizacion.column4}"
                             id="column4">
                    <af:outputText value="#{row.FechaInicio}"
                                   binding="#{backing_procesos_provision_indemnizacion.outputText4}"
                                   id="outputText4">
                      <f:convertDateTime pattern="#{bindings['sis_provisionRem_vw1'].formats.FechaInicio}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="FechaFin" sortable="false"
                             headerText="#{bindings['sis_provisionRem_vw1'].labels.FechaFin}"
                             binding="#{backing_procesos_provision_indemnizacion.column5}"
                             id="column5">
                    <af:outputText value="#{row.FechaFin}"
                                   binding="#{backing_procesos_provision_indemnizacion.outputText5}"
                                   id="outputText5">
                      <f:convertDateTime pattern="#{bindings['sis_provisionRem_vw1'].formats.FechaFin}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="Observaciones" sortable="false"
                             headerText="#{bindings['sis_provisionRem_vw1'].labels.Observaciones}"
                             binding="#{backing_procesos_provision_indemnizacion.column6}"
                             id="column6">
                    <af:outputText value="#{row.Observaciones}"
                                   binding="#{backing_procesos_provision_indemnizacion.outputText6}"
                                   id="outputText6"/>
                  </af:column>
                  <af:column sortProperty="CreadoPor" sortable="false"
                             headerText="#{bindings['sis_provisionRem_vw1'].labels.CreadoPor}"
                             binding="#{backing_procesos_provision_indemnizacion.column7}"
                             id="column7">
                    <af:outputText value="#{row.CreadoPor}"
                                   binding="#{backing_procesos_provision_indemnizacion.outputText7}"
                                   id="outputText7"/>
                  </af:column>
                  <af:column sortProperty="FechaCreacion" sortable="false"
                             headerText="#{bindings['sis_provisionRem_vw1'].labels.FechaCreacion}"
                             binding="#{backing_procesos_provision_indemnizacion.column8}"
                             id="column8">
                    <af:outputText value="#{row.FechaCreacion}"
                                   binding="#{backing_procesos_provision_indemnizacion.outputText8}"
                                   id="outputText8">
                      <f:convertDateTime pattern="#{bindings['sis_provisionRem_vw1'].formats.FechaCreacion}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortable="false"
                             headerText="Proyeccion de Provision Indemnizacion"
                             binding="#{backing_procesos_provision_indemnizacion.column9}"
                             id="column9" rendered="true">
                    <af:objectSpacer width="10" height="10"
                                     binding="#{backing_procesos_provision_indemnizacion.objectSpacer3}"
                                     id="objectSpacer3"/>
                    <af:commandButton text="Ver Proyeccion"
                                      binding="#{backing_procesos_provision_indemnizacion.commandButton2}"
                                      id="commandButton2"
                                      onclick='return mostrarReporte("#{row.IdRemesa}")'
                                      disabled="false"/>
                    <af:objectSpacer width="10" height="10"
                                     binding="#{backing_procesos_provision_indemnizacion.objectSpacer1}"
                                     id="objectSpacer1"/>
                    <af:commandButton text="Generar Poliza"
                                      binding="#{backing_procesos_provision_indemnizacion.commandButton3}"
                                      id="commandButton3"
                                      disabled='#{row.Isaprobado=="0"?false:true}'
                                      action="#{backing_procesos_provision_indemnizacion.cmd_generar_poliza}"/>
                  </af:column>
                  <af:column sortable="false" headerText="Poliza"
                             binding="#{backing_procesos_provision_indemnizacion.column10}"
                             id="column10" rendered="true">
                    <af:inputText binding="#{backing_procesos_provision_indemnizacion.inputText1}"
                                  id="inputText1" columns="11"
                                  disabled='#{row.Inproyeccion!="0" &amp;&amp; row.Isaprobado=="0"?false:true}'
                                  tip="No. Poliza Contable"
                                  value="#{row.NoPolizaContable}"/>
                    <af:commandButton text="Aprobar Poliza"
                                      binding="#{backing_procesos_provision_indemnizacion.commandButton4}"
                                      id="commandButton4"
                                      disabled='#{row.Isaprobado=="0" &amp;&amp; row.Inproyeccion!="0"?false:true}'
                                      action="#{backing_procesos_provision_indemnizacion.cmd_aprobarPoliza}"/>
                    <af:commandButton text="Ver Poliza "
                                      binding="#{backing_procesos_provision_indemnizacion.commandButton5}"
                                      id="commandButton5"
                                      disabled='#{row.InProyeccion=="0"?true:false}'
                                      onclick="return mostrarReportePoliza(&quot;#{row.IdRemesa}&quot;)"/>
                    <af:objectSpacer width="10" height="20"
                                     binding="#{backing_procesos_provision_indemnizacion.objectSpacer2}"
                                     id="objectSpacer2"/>
                  </af:column>
                </af:table>
              </af:panelBox>
            </af:panelHorizontal>
          </af:page>
        </af:form>
      </afh:body>
    </afh:html>
  </f:view>
  <!--oracle-jdev-comment:auto-binding-backing-bean-name:backing_procesos_provision_indemnizacion-->
</jsp:root>
