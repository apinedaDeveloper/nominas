<?xml version='1.0' encoding='windows-1252'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:afh="http://xmlns.oracle.com/adf/faces/html"
          xmlns:af="http://xmlns.oracle.com/adf/faces">
  <jsp:directive.page contentType="text/html;charset=windows-1252"
                      autoFlush="true" language="java"/>
  <f:view>
    <afh:html>
      <afh:head title="Pago Complementos">
        <meta http-equiv="Content-Type"
              content="text/html; charset=windows-1252"/>
      </afh:head>
       <jsp:include page="/js/libJQ.js"/>       
       <jsp:include page="/js/librariesDialog.js"/>           
      <script type="text/javascript" src="/${flujo_usuario.dominioApp}/js/gestiones/scrptAutorizacion.js">
      var xy=new Date().getMilliseconds();      
      </script>
      <script language="javascript" type="text/javascript"> 
       gbl_form="form1";
       gbl_actionId="form1:page1:cmdRefresh";
    
         function newCompl(){
    
         submitForm('form1',1,{source:'form1:page1:btn_nuevo'});
         return false;
         }
      
         function verRepo(IdC){
         
           window.open("../verreportepdf?id="+IdC+String.fromCharCode(38)+"rep=CMPLSAL","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
           return false;    
   
         }
         
         function verRepoDuplicados(IdC){
         
           window.open("../verreportepdf?id="+IdC+String.fromCharCode(38)+"rep=CMPLDUPL","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
           return false;    
   
         }
      
      </script>
    
      <afh:body>
        <af:messages/>
        <h:form id="form1">
          <af:page title="Complementos año actual" value="#{modeloMenu.modeloBD}" var="node"
                   id="page1">
            <f:facet name="nodeStamp">
              <af:commandMenuItem text="#{node.label}"
                                  actionListener="#{modeloMenu.listener_menu}"
                                  disabled="#{node.readOnly}"
                                  rendered="#{node.shown}"
                                  action="#{node.getOutcome}"/>
            </f:facet>
             <f:facet name="infoUser">
              <f:subview id="info1">
                <jsp:include page="/global/facet_infoUser.jspx"/>
              </f:subview>
            </f:facet>
             <f:facet name="menuSwitch">
              <f:subview id="menuSwt">
                <jsp:include page="/global/facet_menuSwitch.jspx"/>
              </f:subview>
            </f:facet>
            <f:facet name="branding">
              <f:subview id="logo">
                <jsp:include page="/global/facet_branding.jspx"/>
              </f:subview>
            </f:facet>
            <af:panelBox>
             <f:verbatim>
                <input type="hidden" name="hdn_tipo" id="hdn_tipo" value="12"/>
                <input type="hidden" name="hdn_Id" id="hdn_Id"
                       value="${bindings.pIdVar1.inputValue}"/>
              </f:verbatim>
              <af:showOneTab>
                <af:showDetailItem text="HIstorico"
                                   binding="#{pago_complementos.tab_histo}">
                  <af:commandLink id="btn_nuevo"
                                  action="#{pago_complementos.btn_nuevo_action}"
                                  disabled="true"/>
                  <af:panelForm>
                    <af:selectInputDate value="#{bindings.fechaI.inputValue}"
                                        label="Fecha Inicio"
                                        required="#{bindings.fechaI.mandatory}">
                      <af:validator binding="#{bindings.fechaI.validator}"/>
                      <f:convertDateTime pattern="#{bindings.fechaI.format}"/>
                    </af:selectInputDate>
                    <af:selectInputDate value="#{bindings.fechaF.inputValue}"
                                        label="Fecha fin"
                                        required="#{bindings.fechaF.mandatory}">
                      <af:validator binding="#{bindings.fechaF.validator}"/>
                      <f:convertDateTime pattern="#{bindings.fechaF.format}"/>
                    </af:selectInputDate>
                    <f:facet name="footer">
                      <af:commandButton actionListener="#{bindings.Compl.execute}"
                                        text="Buscar"
                                        disabled="#{!bindings.Compl.enabled}"/>
                    </f:facet>
                  </af:panelForm>
                  <af:panelHeader text=". . ."/>
                  <af:objectSpacer width="10" height="15"/>
                  <af:table value="#{bindings.SisComplementoVw.collectionModel}"
                            var="row"
                            rows="#{bindings.SisComplementoVw.rangeSize}"
                            first="#{bindings.SisComplementoVw.rangeStart}"
                            emptyText="Ninguna fila"
                            selectionState="#{bindings.SisComplementoVw.collectionModel.selectedRow}"
                            selectionListener="#{bindings.SisComplementoVw.collectionModel.makeCurrent}"
                            partialTriggers="cmd_IdComple" banding="row"
                            bandingInterval="1">
                    <f:facet name="header">
                      <af:outputText value="Listado de complementos"/>
                    </f:facet>
                    <af:column sortProperty="IdComplemento" sortable="false"
                               headerText="#{bindings.SisComplementoVw.labels.IdComplemento}">
                      <af:commandLink text="#{row.IdComplemento}"
                                      disabled="#{!bindings.setCurrentRowWithKey.enabled}"
                                      action="#{pago_complementos.cmd_Complemento}"
                                      id="cmd_IdComple"/>
                    </af:column>
                    <af:column sortProperty="IdEstado" sortable="false"
                               headerText="#{bindings.SisComplementoVw.labels.IdEstado}">
                      <af:outputText value="#{row.Nomestado}">
                        <f:convertNumber groupingUsed="false"
                                         pattern="#{bindings.SisComplementoVw.formats.IdEstado}"/>
                      </af:outputText>
                    </af:column>
                    <af:column sortProperty="IdSubprograma" sortable="false"
                               headerText="#{bindings.SisComplementoVw.labels.IdSubprograma}"
                               noWrap="true">
                      <af:outputText value="#{row.Formato2} - #{row.Nomunidad}"/>
                    </af:column>
                    <af:column sortProperty="Periodo" sortable="false"
                               headerText="#{bindings.SisComplementoVw.labels.Periodo}">
                      <af:outputText value="#{row.Periodo}">
                        <f:convertNumber groupingUsed="false"
                                         pattern="#{bindings.SisComplementoVw.formats.Periodo}"/>
                      </af:outputText>
                    </af:column>
                    <af:column sortProperty="Anio" sortable="false"
                               headerText="Año">
                      <af:outputText value="#{row.Anio}">
                        <f:convertNumber groupingUsed="false"
                                         pattern="#{bindings.SisComplementoVw.formats.Anio}"/>
                      </af:outputText>
                    </af:column>
                    <af:column sortProperty="CreadoPor" sortable="false"
                               headerText="#{bindings.SisComplementoVw.labels.CreadoPor}"
                               noWrap="true">
                      <af:outputText value="#{row.CreadoPor} - #{row.Nomusuario}"/>
                    </af:column>
                    <af:column sortProperty="FechaCreacion" sortable="false"
                               headerText="#{bindings.SisComplementoVw.labels.FechaCreacion}">
                      <af:outputText value="#{row.FechaCreacion}">
                        <f:convertDateTime pattern="#{bindings.SisComplementoVw.formats.FechaCreacion}"/>
                      </af:outputText>
                    </af:column>
                  </af:table>
                </af:showDetailItem>
                <af:showDetailItem text="Nuevo Complemento" disclosed="false"
                                   disabled="false"
                                   onclick="return newCompl();"
                                   binding="#{pago_complementos.tab_detalle}">
                  <af:panelHeader text="Id: [ #{bindings.IdComplemento.inputValue} ]" size="0"/>
                  <af:showOneTab>
                    <af:showDetailItem text="Inicio"
                                       binding="#{pago_complementos.tab_inicio}">
                      <af:panelForm>
                        <af:panelHorizontal>
                          <af:inputText value="#{bindings['reg_empleado'].inputValue}"
                                        label="Registro Empleado"
                                        required="#{bindings['reg_empleado'].mandatory}"
                                        columns="#{bindings['reg_empleado'].displayWidth}"
                                        disabled="#{!bindings.IsFormaActiva.inputValue}">
                            <af:validator binding="#{bindings['reg_empleado'].validator}"/>
                            <f:convertNumber groupingUsed="false"
                                             pattern="#{bindings['reg_empleado'].format}"/>
                          </af:inputText>
                          <af:inputText value="#{bindings['num_cui'].inputValue}"
                                        simple="false"
                                        required="#{bindings['num_cui'].mandatory}"
                                        columns="#{bindings['num_cui'].displayWidth}"
                                        label="CUI">
                            <af:validator binding="#{bindings['num_cui'].validator}"/>
                          </af:inputText>
                          <af:selectBooleanCheckbox value="#{bindings.SisComplementoVwIsautofinanciable.inputValue}"
                                                    label="#{bindings.SisComplementoVwIsautofinanciable.label}"
                                                    disabled="#{bindings.SisComplementoVwSisComplementoDetVw.estimatedRowCount >0 ? true : false}"/>
                          <af:commandButton actionListener="#{bindings.Contratos.execute}"
                                            text="Buscar"
                                            disabled="#{!bindings.Contratos.enabled}"
                                            action="#{pago_complementos.btn_buscar}"/>
                          <f:facet name="separator">
                            <h:panelGroup>
                              <af:objectSpacer width="10" height="10"/>
                            </h:panelGroup>
                          </f:facet>
                        </af:panelHorizontal>
                        <af:outputText value="#{bindings.NombreCompleto.inputValue}"
                                       inlineStyle="color:rgb(0,49,148); font-size:large;"/>
                        <af:table value="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.collectionModel}"
                                  var="row"
                                  rows="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.rangeSize}"
                                  first="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.rangeStart}"
                                  emptyText="Ninguna Plaza"
                                  selectionState="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.collectionModel.selectedRow}"
                                  selectionListener="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.collectionModel.makeCurrent}"
                                  banding="row" bandingInterval="1">
                          <af:column sortable="false" headerText="Seleccionar">
                            <af:selectBooleanCheckbox value="#{row.Seleccionado}"
                                                      label="Label 1"
                                                      selected="#{row.Seleccionado}"/>
                          </af:column>
                          <af:column sortable="false" headerText="Partida">
                            <af:outputText value="#{row.Partida}"/>
                          </af:column>
                          <af:column sortProperty="NumeroPlaza" sortable="false"
                                     headerText="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.labels.NumeroPlaza}">
                            <af:outputText value="#{row.NumeroPlaza}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.formats.NumeroPlaza}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="IdPuesto" sortable="false"
                                     headerText="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.labels.IdPuesto}">
                            <af:outputText value="#{row.IdPuesto}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.formats.IdPuesto}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="NomPuesto" sortable="false"
                                     headerText="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.labels.NomPuesto}">
                            <af:outputText value="#{row.NomPuesto}"/>
                          </af:column>
                          <af:column sortProperty="Nomdependencia"
                                     sortable="false"
                                     headerText="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.labels.Nomdependencia}">
                            <af:outputText value="#{row.Nomdependencia}"/>
                          </af:column>
                          <af:column sortProperty="Horascontratadas"
                                     sortable="false"
                                     headerText="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.labels.Horascontratadas}">
                            <af:outputText value="#{row.Horascontratadas}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.formats.Horascontratadas}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="FechaInicio" sortable="false"
                                     headerText="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.labels.FechaInicio}">
                            <af:outputText value="#{row.FechaInicio}">
                              <f:convertDateTime pattern="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.formats.FechaInicio}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="FechaFin" sortable="false"
                                     headerText="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.labels.FechaFin}">
                            <af:outputText value="#{row.FechaFin}">
                              <f:convertDateTime pattern="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.formats.FechaFin}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="Estado" sortable="false"
                                     headerText="#{bindings.SisEmpleadoVwSisContratosEmplOpVw.labels.Estado}">
                            <af:outputText value="#{row.Estado}"/>
                          </af:column>
                        </af:table>
                        <af:panelHorizontal>
                          <af:selectOneChoice label="Tipo Nomina"
                                              valueChangeListener="#{pago_complementos.tipo_nomina}"
                                              value="#{bindings.pIdNomina1.inputValue}"
                                              autoSubmit="true"
                                              id="cmb_tipoNomina"
                                              disabled="#{!bindings.IsFormaActiva.inputValue}">
                            <af:selectItem label="NOMINA NORMAL" value="1"/>
                            <af:selectItem label="DIFERIDO 12%" value="2"/>
                            <af:selectItem label="DIFERIDO 50%" value="3"/>
                            <af:selectItem label="BONO 14" value="5"/>
                            <af:selectItem label="AGUINALDO" value="6"/>
                          </af:selectOneChoice>
                          <af:selectOneChoice id="navList1" autoSubmit="false"
                                              value="#{bindings.SisPeridoxTipoNomVw.inputValue}"
                                              label="Mes"
                                              partialTriggers="cmb_tipoNomina"
                                              disabled="#{!bindings.IsFormaActiva.inputValue}">
                            <f:selectItems value="#{bindings.SisPeridoxTipoNomVw.items}"/>
                          </af:selectOneChoice>
                          <af:commandLink id="cmdRefresh"
                                          actionListener="#{bindings.actualizarVW.execute}"
                                          disabled="true"
                                          action="#{pago_complementos.cmdRefresh_action}"/>
                          <af:commandButton text="Generar Complemento"
                                            action="#{pago_complementos.btn_GenerarComplemento}"
                                            disabled="#{!bindings.IsFormaActiva.inputValue}"/>
                        </af:panelHorizontal>
                        <af:panelButtonBar>
                          <af:commandButton text="Generar pagos curso vacaciones"
                                            action="#{pago_complementos.btn_vacacion_action}"
                                            disabled="#{!bindings.IsFormaActiva.inputValue}"/>
                          <af:commandLink onclick="verRepo('#{bindings.IdComplemento.inputValue}');">
                            <af:objectImage source="/imagenes/printer_1.png"/>
                          </af:commandLink>
                          <af:commandLink onclick="return validaComplemento();">
                            <af:objectImage source="/imagenes/btn_trasladar.png"/>
                          </af:commandLink>
                          <af:commandLink onclick="return OpenDialogo(2);">
                            <af:objectImage source="/imagenes/btn_anular.png"/>
                          </af:commandLink>
                          <af:commandLink action="dialog:det_autoriza"
                                          launchListener="#{pago_complementos.launch_detalle_auto}"
                                          useWindow="true" windowHeight="300"
                                          windowWidth="600">
                            <af:objectImage source="/imagenes/list_information.png"/>
                          </af:commandLink>
                          <af:commandLink onclick="verRepoDuplicados('#{bindings.IdComplemento.inputValue}');"
                                          shortDesc="Pagos del Complemento ya Realizados o en Gestión">
                            <af:objectImage source="/imagenes/img_pagosComplementos.png"/>
                          </af:commandLink>
                        </af:panelButtonBar>
                        <af:objectSpacer width="10" height="50"/>
                        <af:table value="#{bindings.SisComplementoVwSisComplementoDetVw.collectionModel}"
                                  var="row"
                                  rows="#{bindings.SisComplementoVwSisComplementoDetVw.rangeSize}"
                                  first="#{bindings.SisComplementoVwSisComplementoDetVw.rangeStart}"
                                  emptyText="Ninguna fila"
                                  selectionState="#{bindings.SisComplementoVwSisComplementoDetVw.collectionModel.selectedRow}"
                                  selectionListener="#{bindings.SisComplementoVwSisComplementoDetVw.collectionModel.makeCurrent}"
                                  banding="row" bandingInterval="1">
                          <f:facet name="selection">
                            <af:tableSelectOne text="Select and"
                                               autoSubmit="true"
                                               disabled="#{!bindings.IsFormaActiva.inputValue}"/>
                          </f:facet>
                          <f:facet name="actions">
                            <af:panelButtonBar rendered="#{bindings.IsFormaActiva.inputValue}">
                              <af:commandButton text="Guardar"
                                                action="#{pago_complementos.btn_guardar_ant}"/>
                              <af:commandButton actionListener="#{bindings.Rollback.execute}"
                                                text="Cancelar"
                                                immediate="true">
                                <af:resetActionListener/>
                              </af:commandButton>
                              <af:commandButton text="cancelar edición"
                                                action="#{pago_complementos.btn_cancelar_edit_ant}"/>
                            </af:panelButtonBar>
                          </f:facet>
                          <af:column sortable="false" headerText="Eliminar">
                            <af:commandLink action="#{pago_complementos.cmd_eliminar_det}"
                                            disabled="#{!bindings.IsFormaActiva.inputValue}">
                              <af:objectImage source="/imagenes/borrar.png"/>
                            </af:commandLink>
                          </af:column>
                          <af:column sortable="false" headerText="Editar">
                            <af:commandLink action="#{pago_complementos.cmd_editar}"
                                            disabled="#{!bindings.IsFormaActiva.inputValue}">
                              <af:objectImage source="/imagenes/form_edit.png"/>
                            </af:commandLink>
                          </af:column>
                          <af:column sortProperty="IdComplementoDet"
                                     sortable="false" headerText="IdDet">
                            <af:outputText value="#{row.IdComplementoDet}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.IdComplementoDet}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="RegistroEmpleado"
                                     sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.RegistroEmpleado}">
                            <af:outputText value="#{row.RegistroEmpleado}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.RegistroEmpleado}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="Tiponomina" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.Tiponomina}">
                            <af:outputText value="#{row.Tiponomina}"/>
                          </af:column>
                          <af:column sortable="false" headerText="Partida">
                            <af:outputText value="#{row.Formato1}"/>
                          </af:column>
                          <af:column sortProperty="NumeroPlaza" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.NumeroPlaza}">
                            <af:outputText value="#{row.NumeroPlaza}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.NumeroPlaza}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="IdPuesto" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.IdPuesto}">
                            <af:outputText value="#{row.IdPuesto}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.IdPuesto}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="IdContrato" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.IdContrato}">
                            <af:outputText value="#{row.IdContrato}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.IdContrato}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="Anio" sortable="false"
                                     headerText="Año">
                            <af:outputText value="#{row.Anio}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.Anio}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="Periodo" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.Periodo}">
                            <af:outputText value="#{row.Periodo}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.Periodo}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="SueldoBase" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.SueldoBase}"
                                     formatType="number">
                            <af:outputText value="Q#{row.SueldoBase}"
                                           rendered="#{!row.isSel}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.SueldoBase}"/>
                            </af:outputText>
                            <af:inputText value="#{row.SueldoBase}" columns="10"
                                          rendered="#{row.isSel}"/>
                          </af:column>
                          <af:column sortProperty="Escalafon" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.Escalafon}"
                                     formatType="number">
                            <af:outputText value="Q#{row.Escalafon}"
                                           rendered="#{!row.isSel}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.Escalafon}"/>
                            </af:outputText>
                            <af:inputText value="#{row.Escalafon}" columns="10"
                                          rendered="#{row.isSel}"/>
                          </af:column>
                          <af:column sortProperty="ComplSalarial"
                                     sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.ComplSalarial}"
                                     formatType="number">
                            <af:outputText value="Q#{row.ComplSalarial}"
                                           rendered="#{!row.isSel}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.ComplSalarial}"/>
                            </af:outputText>
                            <af:inputText value="#{row.ComplSalarial}"
                                          columns="10" rendered="#{row.isSel}"/>
                          </af:column>
                          <af:column sortProperty="Nominal" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.Nominal}"
                                     formatType="number">
                            <af:outputText value="Q#{row.Nominal}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.Nominal}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="TotalBono" sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.TotalBono}"
                                     formatType="number">
                            <af:outputText value="Q#{row.TotalBono}"
                                           partialTriggers="tbl_det_desc">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.SisComplementoVwSisComplementoDetVw.formats.TotalBono}"/>
                            </af:outputText>
                          </af:column>
                          <af:column sortProperty="TotalDescuento"
                                     sortable="false"
                                     headerText="#{bindings.SisComplementoVwSisComplementoDetVw.labels.TotalDescuento}"
                                     formatType="number">
