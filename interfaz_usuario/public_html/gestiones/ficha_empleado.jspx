       //Verifica que se haya ingresado un valor en el campo de texto antes de invocar el Web Services
       function validarCuenta(){
            var noCuentaObj, noCuenta;
            noCuentaObj = document.getElementById('page1:NoCuenta');
            if (noCuentaObj!=null) {
                noCuenta = noCuentaObj.value;
                noCuenta = trimStrings(noCuenta);
                noCuentaObj.value = noCuenta;
                if(noCuenta.length > 0){
                    return checkCuenta(noCuenta);
                } else {
                    alert("Ingrese número de cuenta bancaria.");
                    noCuentaObj.focus();
                }
            } else {
                alert("Ingrese número de cuenta bancaria.");
                noCuentaObj.focus();
            }
            return false;
       }
       
       //Abre la Constancia de Actualización (SIS-23)
       function generaReporte(noRegistroEmpleado){ 
            var link;
            link = "/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+noRegistroEmpleado+String.fromCharCode(38)+"rep=CONST_ACT_DATOSPER";
            window.open(link,"preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable=1");
            return false;
       } 
       
       //Abre un Documento Adjunto para su visualización
       function openDocumento(nomArchivo){  //No retorna nada
            window.open("/${flujo_usuario.dominioApp}/faces/ver_documento?source="+nomArchivo,"preview","width=640,height=640,scrollbars=YES");
       }
       
