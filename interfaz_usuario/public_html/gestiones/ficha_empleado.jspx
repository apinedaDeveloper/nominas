            return false;    
        }
        
        //Muestra la pregunta Declaro Bajo Juramento...
        function declararBajoJuramento(){
            var declarado = false;
            var declaracion = "";
            declaracion = "Declaro bajo juramento, que todos los datos solicitados son fidedignos, al igual que todos los documentos subidos en PDF, JPG, JPEG y/o PNG,";
            declaracion = declaracion + " cumplen con los requisitos y características requeridas por la División de Administración de Recursos Humanos.";
            declaracion = declaracion + " En caso de incumplimiento con lo requerido, me someto a las disposiciones de ley por el delito de falsedad ideológica,";
            declaracion = declaracion + " establecida en el Código Penal, artículo 322 y a la penalización que ello conlleva.";
            if (confirm(declaracion)){
                declarado = true;
            }
            return declarado;
        }
        
        //Realiza la Pregunta recibida como parámetro y de ser positiva,
        //realiza una segunda pregunta de Declaro bajo juramento..
        function confirmarInformacion(mensaje) {
            var confirmado = false; //valor inicial
            if (confirm(mensaje)){
                if (declararBajoJuramento()){
                    confirmado = true;
                }
            }
            return confirmado;
        }
        
        //Pregunta si desea confirmar la referencia personal agregada
        function confirmarReferenciaPersonal(idReferenciaPersonal){
            var mensaje = "¿Desea confirmar la referencia personal ID No. " + idReferenciaPersonal + "?";
            return confirmarInformacion(mensaje);
        }
        
        //Pregunta si desea confirmar la experiencia laboral agregada
        function confirmarExperienciaLaboral(idExperienciaLaboral){
            var mensaje = "¿Desea confirmar la experiencia laboral ID No. " + idExperienciaLaboral + "?";
            return confirmarInformacion(mensaje);
        }
        
        //Pregunta si desea Confirmar el Documento Adjunto
        function confirmarDocumentoAdjunto(idDocumentoAdjunto){
            var mensaje = "¿Desea confirmar el Documento No. " + idDocumentoAdjunto + "?";
            return confirmarInformacion(mensaje);
        }
        
        //Pregunta si está seguro de Confirmar la Foto
        function confirmarFotoAdjunto(idFotoAdjunto) {
            var mensaje = "¿Desea confimar la fotografía?";
            return confirmarInformacion(mensaje);
        }
        
        //Pregunta si desea guardar la Información de los botones Guardar del formulario
        function confirmarGuardar(){
            var aceptado = false; //valor inicial
            var mensaje = "¿Desea guardar la información?";
            if (confirm(mensaje)){
                aceptado = true;
            }
            return aceptado;
        }
        
        //Comprueba desde JavaScript que el archivo seleccionado no supere los 2MB
        function showFileSize(ElementID) { 
            var input, file; 
            if (!window.FileReader) { 
                alert("El archivo API no está soportado por este Navegador aún. Salga de la página y utilice otro Navegador por favor."); 
                return false; 
            } 
            input = document.getElementById(ElementID); 
            if (!input) { 
                alert("Um, No se pudo encontrar el elemento FileInput. Intente de Nuevo por favor."); 
                return false; 
            } 
            else if (!input.files) { 
                alert("Este Navegador parece no soportar las propiedades de Entradas de Archivo. Intente de Nuevo por favor."); 
                return false; 
            } 
            else if (input.files[0]) { 
                file = input.files[0]; 
                if ( file.size >= 2097152) {
                    alert("El Archivo Seleccionado es muy grande, seleccione otro por favor."); 
                    return false; 
                } else {
                    return true;
                }
            } else {
                return true;
            }
            return false; 
        }
        
        //Pregunta si desea eliminar la referencia personal agregada
        function eliminarReferenciaPersonal(idReferenciaPersonal){
            var mensaje = "¿Desea eliminar la referencia personal ID No. " + idReferenciaPersonal + "?";
            return confirm(mensaje);
        }
        
        //Pregunta si desea eliminar la experiencia laboral agregada
        function eliminarExperienciaLaboral(idReferenciaPersonal){
            var mensaje = "¿Desea eliminar la experiencia laboral ID No. " + idReferenciaPersonal + "?";
            return confirm(mensaje);
        }
        
      </script>
