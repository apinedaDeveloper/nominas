                      <af:commandButton actionListener="#{bindings.Execute.execute}"
                                        text="#{txtdeducs['ingresar_deducciones.btn_ejecutar.texto']}"
                                        disabled="#{!bindings.Execute.enabled}"/>
                      <af:commandButton text="#{txtdeducs['ingresar_deducciones.btn_commint.texto']}"
