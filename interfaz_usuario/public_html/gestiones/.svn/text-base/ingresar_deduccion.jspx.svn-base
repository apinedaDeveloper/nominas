                                             pattern="#{bindings.PensionesAlimen.format}"/>
                          </af:inputText>
                        </afh:cellFormat>
                      </afh:rowLayout>
                      <afh:rowLayout>
                        <afh:cellFormat>
                          <af:outputText value="#{tx['isrEmpl.labelrentase2.texto']}"
                                         styleClass="textLabel"/>
                        </afh:cellFormat>
                        <afh:cellFormat halign="right">
                          <af:inputText value="Q.#{bindings.RentaExcenta.inputValue}"
                                        required="#{bindings.RentaExcenta.mandatory}"
                                        columns="10" disabled="true"
                                        readOnly="true">
                            <af:validator binding="#{bindings.RentaExcenta.validator}"/>
                            <f:convertNumber groupingUsed="false"
                                             pattern="#{bindings.RentaExcenta.format}"/>
                          </af:inputText>
                        </afh:cellFormat>
                      </afh:rowLayout>
                      <afh:rowLayout>
                          <afh:cellFormat>
                          <af:outputText value="#{tx['isrEmpl.labelcolegios2.texto']}"
                                         styleClass="textLabel"
                                         rendered="#{bindings.shwOpt.inputValue eq '1'}"/>
                            <af:outputText value="Otros gastos: (donaciones y otros)"
                                           styleClass="textLabel"
                                           rendered="#{bindings.shwOpt.inputValue eq '2'}"/>
                          </afh:cellFormat>
                        <afh:cellFormat halign="right">
                          <af:inputText value="#{bindings.Colegiosyotros.inputValue}"                                      
                                        required="#{bindings.Colegiosyotros.mandatory}"
                                        columns="10"
                                        inlineStyle="text-align:right;"
                                        label="Q.">
                            <af:validator binding="#{bindings.Colegiosyotros.validator}"/>
                            <f:convertNumber groupingUsed="false"
                                             pattern="#{bindings.Colegiosyotros.format}"/>
                          </af:inputText>
                        </afh:cellFormat>
                      </afh:rowLayout>
                      <afh:rowLayout>
                        <afh:cellFormat>
                          <af:outputText value="#{tx['isrEmpl.labelRentaImp.texto']}"
                                         styleClass="textLabel"/>
                        </afh:cellFormat>
                        <afh:cellFormat halign="right">
                          <af:inputText value="Q.#{bindings.RentaImponible.inputValue}"
                                        required="#{bindings.RentaImponible.mandatory}"
                                        columns="10"
                                        inlineStyle="text-align:right;"
                                        readOnly="true">
                            <af:validator binding="#{bindings.RentaImponible.validator}"/>
                            <f:convertNumber groupingUsed="false"
                                             pattern="#{bindings.RentaImponible.format}"/>
                          </af:inputText>
                        </afh:cellFormat>
                      </afh:rowLayout>
                      <afh:rowLayout>
                        <afh:cellFormat>
                          <af:outputText value="#{tx['isrEmpl.labelpresenta.texto']}"
                                         styleClass="textLabel"/>
                        </afh:cellFormat>
                        <afh:cellFormat halign="right">
                          <af:selectInputDate
                                              value="#{bindings.FechaPresentacion.inputValue}"/>
                        </afh:cellFormat>
                      </afh:rowLayout>
                    </afh:tableLayout>
                    <af:panelForm/>
                  </af:panelBox>
                  <af:objectSeparator/>
                  <af:commandButton actionListener="#{bindings.Commit.execute}"
                                    text="#{tx['isrEmpl.btn_genRep.texto']}"
                                    onclick="genRep('#{bindings.Anio1.inputValue}','#{flujo_usuario.selRegEmpleado}');"
                                    id="btn_generarRep"/>
                </af:showDetailItem>
                </af:showOneTab>
              </af:panelBox>
            </af:panelHorizontal>
          </af:page>
        </h:form>
      </afh:body>
    </afh:html>
