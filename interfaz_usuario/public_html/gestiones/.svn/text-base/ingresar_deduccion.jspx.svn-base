                        </af:panelHorizontal>
                      </af:panelLabelAndMessage>
                      <af:inputText value="#{bindings.RegistroEmpleado.inputValue}"
                                    label="#{txtdeducs['ingresar_deducciones.inpt_registro_empleado_ingresado.texto']}"
                                    required="#{bindings.RegistroEmpleado.mandatory}"
                                    columns="#{bindings.RegistroEmpleado.displayWidth}"
                                    partialTriggers="btn_crear"
                                    binding="#{Ingresar_deduccion.inpt_regEmpleado}"
                                    disabled="true" rendered="false">
