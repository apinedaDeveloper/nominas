                                    required="#{bindings.Cantidad.mandatory}"
                                    columns="#{bindings.Cantidad.displayWidth}"
                                    disabled="true"
                                    binding="#{Ingresar_deduccion.cantidad}"
                                    showRequired="true">
                        <af:validator binding="#{bindings.Cantidad.validator}"/>
                        <f:convertNumber groupingUsed="false"
                                         pattern="#{bindings.Cantidad.format}"/>
                      </af:inputText>
                      <af:inputText value="#{bindings.Anio.inputValue}"
                                    label="#{txtdeducs['ingresar_deducciones.inpt_anio_ingresado.texto']}"
