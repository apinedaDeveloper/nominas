<?xml version='1.0' encoding='windows-1252'?>
<jsp:root xmlns="http://www.w3.org/1999/xhtml"
          xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces"
          xmlns:afh="http://xmlns.oracle.com/adf/faces/html">
  <f:view>
    <afh:html>
      <jsp:directive.page contentType="text/html;charset=windows-1252"/>
      <f:loadBundle basename="propiedades_trab" var="tx"/>
      <f:loadBundle basename="propiedades" var="txtdeducs"/>
      <afh:head title="#{txtdeducs['ingresar_deducciones.title']}">
        <meta http-equiv="Content-Type"
              content="text/html; charset=windows-1252"/>
              
      <script language="javascript" type="text/javascript"> 
        function genReporte(pReg){ 
                submitForm('form1',1,{source:'form1:page1:btn_generarRep'});
                window.open("/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+pReg+String.fromCharCode(38)+"rep=REPPAGOISR2","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
       } 

        function genReporteMensual(pReg){ 
                submitForm('form1',1,{source:'form1:page1:btn_generarRep'});
                window.open("/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+pReg+String.fromCharCode(38)+"rep=REPPAGOISRMSUL","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
       } 

              
        function genRep(pAnio,preg){ 
                submitForm('form1',1,{source:'form1:page1:btn_generarRep'});
                window.open("/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+pAnio+String.fromCharCode(38)+ "id2="+preg+String.fromCharCode(38)+"rep=DECJURADPAT2","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
       } 
       
       
       function verificarTipo(vControl){
       
         var vIndice=vControl.selectedIndex;             
         var vInputN=document.getElementById('form1:page1:inptNit');
          
          
         if (vControl.options[vIndice].text=='SUELDO OTROS PATRONOS' || vControl.options[vIndice].text=='SUELDO OTRO PATRONO' || vControl.options[vIndice].text=='RENTA NETA OTROS PATRONOS' || vControl.options[vIndice].text=='RETENCIONES OTROS PATRONOS'){
         
            vInputN.disabled=false;
         
         } else{
         
            vInputN.disabled=true;
            vInputN.value="";
            
         }
         return false;
       }
       
       function genOtrasRentas(vRegPer){

             var lista=document.getElementById('form1:page1:cmbAnioTrabajo');
             var indice=lista.selectedIndex;
             var vAnio1 = lista.options[indice].text; 
             
             var vMes = 12;
                    
             window.open("/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+vAnio1+String.fromCharCode(38)+"rep=OTRASRENTAS"+String.fromCharCode(38)+"id2="+vMes+String.fromCharCode(38)+"id3="+vRegPer,"preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
       
       }
       
      </script>         
      </afh:head>
      <afh:body>
        <af:messages/>
        <h:form id="form1">
        <af:page  value="#{modeloMenu.modeloBD}" var="node"
                 title="#{flujo_usuario.datosEmpleadoSel}" id="page1">
            <f:facet name="nodeStamp">
              <af:commandMenuItem text="#{node.label}"
                                  actionListener="#{modeloMenu.listener_menu}"
                                  disabled="#{node.readOnly}"
                                  rendered="#{node.shown}"
                                  action="#{node.getOutcome}"/>
            </f:facet>
             <f:facet name="infoUser">
              <f:subview id="info1">
                <jsp:include page="/global/facet_infoUser.jspx"/>
              </f:subview>
            </f:facet>
             <f:facet name="menuSwitch">
              <f:subview id="menuSwt">
                <jsp:include page="/global/facet_menuSwitch.jspx"/>
              </f:subview>
            </f:facet>
            <f:facet name="branding">
              <f:subview id="logo">
                <jsp:include page="/global/facet_branding.jspx"/>
              </f:subview>
            </f:facet>
            
            <af:panelHorizontal halign="center">
              <af:panelBox text="Año: #{flujo_usuario.anioActual}">
                <af:showOneTab>
                  <af:showDetailItem text="Ingreso deducciones">
                    <af:panelLabelAndMessage label="NIT"
                                             inlineStyle="font-size:large; color:rgb(0,74,115);">
                      <af:inputText
                                    value="#{bindings.Nit.inputValue}"
                                    inlineStyle="font-size:large;"
                                    columns="10"/>
                    </af:panelLabelAndMessage>
                    <af:panelLabelAndMessage label="Tipo"
                                             inlineStyle="font-size:large; color:rgb(0,74,115);">
                      <af:panelHorizontal>
                        <af:selectOneChoice value="#{bindings.pIdTipoCalc1.inputValue}">
                          <af:selectItem label="Proyección" value="1"/>
                          <af:selectItem label="Final" value="2"/>
                        </af:selectOneChoice>
                        <af:commandButton text="Actualizar"
                                          action="#{Ingresar_deduccion.btn_Actualizar_tipo}"/>
                        <f:facet name="separator">
                          <af:objectSpacer width="15" height="10"/>
                        </f:facet>
                      </af:panelHorizontal>
                    </af:panelLabelAndMessage>
                    <af:objectSeparator/>
                    <af:subform>
                      <af:panelForm>
                        <af:inputText value="#{bindings['reg_empleado'].inputValue}"
                                      label="#{txtdeducs['ingresar_deducciones.inpt_registro_empleado.texto']}"
                                      required="#{bindings['reg_empleado'].mandatory}"
                                      columns="#{bindings['reg_empleado'].displayWidth}"
                                      binding="#{backing_bean_comun.inp_regEmpleadoBusqueda}"
                                      rendered="false">
                          <af:validator binding="#{bindings['reg_empleado'].validator}"/>
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings['reg_empleado'].format}"/>
                        </af:inputText>
                        <af:inputText value="#{bindings.anio.inputValue}"
                                      label="#{txtdeducs['ingresar_deducciones.inpt_anio.texto']}"
                                      required="#{bindings.anio.mandatory}"
                                      columns="#{bindings.anio.displayWidth}"
                                      binding="#{Ingresar_deduccion.inpAnioBusqueda}"
                                      rendered="false">
                          <af:validator binding="#{bindings.anio.validator}"/>
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings.anio.format}"/>
                        </af:inputText>
                        <af:commandButton text="#{txtdeducs['ingresar_deducciones.btn_buscar_empleado.texto']}"
                                          disabled="#{''}"
                                          action="#{Ingresar_deduccion.commandButton_action}"
                                          rendered="false"/>
                      </af:panelForm>
                      <af:panelHeader text="#{txtdeducs['ingresar_deducciones.hdrpnl_datos.texto']}"
                                      rendered="false">
                        <af:panelHorizontal>
                          <af:outputLabel value="#{txtdeducs['ingresar_deducciones.lbl_nombre_empleado.texto']} : "/>
                          <af:outputFormatted value="&lt;b>#{bindings['empleado_cont_by_regNombreCompleto'].inputValue}&lt;/b>"
                                              styleUsage="instruction"/>
                        </af:panelHorizontal>
                        <af:panelHorizontal>
                          <af:outputLabel value="#{txtdeducs['ingresar_deducciones.outpt_lbl.texto']}"/>
                          <af:outputFormatted value="&lt;b>#{bindings.empleado_cont_by_reg_reg_empleado}&lt;/b>"
                                              styleUsage="instruction"/>
                        </af:panelHorizontal>
                      </af:panelHeader>
                    </af:subform>
                    <af:panelHeader text="#{txtdeducs['ingresar_deducciones.pnl_1.header']}"/>
                    <af:panelForm partialTriggers="Radio tabla_deduccion_isr">
                      <af:panelLabelAndMessage label="#{txtdeducs['cambioAnio.cmb_anios.title']}">
                        <af:panelHorizontal>
                          <af:selectOneChoice value="#{bindings.vAnio1.inputValue}"
