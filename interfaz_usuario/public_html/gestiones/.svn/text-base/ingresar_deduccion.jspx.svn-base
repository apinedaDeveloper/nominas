                        <af:validator binding="#{bindings.Anio.validator}"/>
                        <f:convertNumber groupingUsed="false"
                                         pattern="#{bindings.Anio.format}"/>
                      </af:inputText>
                      <af:inputText label="Nit" columns="10"
                                    value="#{bindings.NitDedu.inputValue}"
                                    id="inptNit" disabled="true"
                                    binding="#{Ingresar_deduccion.inptNitDedu}"/>
                      <af:inputText label="Descripción" rows="2"
                                    value="#{bindings.Descripcion.inputValue}"
                                    disabled="true"
                                    binding="#{Ingresar_deduccion.inpt_descrip}"/>
                    </af:panelForm>
                    <af:panelHorizontal>
                      <af:commandButton text="#{txtdeducs['ingresar_deducciones.btn_crear.texto']}"
