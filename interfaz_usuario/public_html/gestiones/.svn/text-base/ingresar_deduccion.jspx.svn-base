                                        id="btn_commit"/>
                      <af:commandButton text="#{txtdeducs['ingresar_deducciones.btn_rollback.texto']}"
                                        immediate="true"
                                        action="#{Ingresar_deduccion.commandButton_Rollback}">
                        <af:resetActionListener/>
                      </af:commandButton>
                      <f:facet name="separator">
                        <af:objectSpacer width="20" height="10"/>
                      </f:facet>
                    </af:panelHorizontal>
                    <af:objectSpacer width="10" height="25"/>
                    <af:table value="#{bindings['sis_deduccion_isr1'].collectionModel}"
                              var="row"
                              rows="#{bindings['sis_deduccion_isr1'].rangeSize}"
                              first="#{bindings['sis_deduccion_isr1'].rangeStart}"
                              emptyText="#{bindings['sis_deduccion_isr1'].viewable ? 'Ninguna fila.' : 'Ninguna fila'}"
                              selectionState="#{bindings['sis_deduccion_isr1'].collectionModel.selectedRow}"
                              selectionListener="#{bindings['sis_deduccion_isr1'].collectionModel.makeCurrent}"
                              id="tabla_deduccion_isr"
                              partialTriggers="btn_commit" banding="row"
                              bandingInterval="1">
                      <f:facet name="selection">
                        <af:tableSelectOne text="Select and" autoSubmit="true"
                                           id="Radio"/>
                      </f:facet>
                      <f:facet name="header">
                        <af:outputText value="#{txtdeducs['ingresar_deducciones.tbl_resumen.titulo1.texto']}"/>
                      </f:facet>
                      <af:column sortable="false" headerText="-">
                        <af:commandLink action="#{Ingresar_deduccion.cmd_editar}">
                          <af:objectImage source="/imagenes/form_edit.png"/>
                        </af:commandLink>
                      </af:column>
                      <af:column sortable="false" headerText="-">
                        <af:commandLink action="#{Ingresar_deduccion.cmd_eliminar}"
                                        disabled="#{bindings.pIdTipoCalc1.inputValue eq '2'}">
                          <af:objectImage source="/imagenes/borrar.png"
                                          shortDesc="Eliminar"/>
                        </af:commandLink>
                      </af:column>
                      <af:column sortProperty="Nombre" sortable="false"
                                 headerText="#{txtdeducs['ingresar_deducciones.tbl_resumen.col_tipo_deduccion.texto']}"
                                 noWrap="true">
                        <af:outputText value="#{row.Nombre}"/>
                      </af:column>
                      <af:column sortProperty="Cantidad" sortable="false"
                                 headerText="#{txtdeducs['ingresar_deducciones.tbl_deducciones.col_cantidad.texto']}"
                                 formatType="number">
                        <af:outputText value="#{row.Cantidad}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings['sis_deduccion_isr1'].formats.Cantidad}"/>
                        </af:outputText>
                      </af:column>
                      <af:column sortable="false" headerText="Nit">
                        <af:outputText value="#{row.Nit}"/>
                      </af:column>
                      <af:column sortProperty="FechaCreacion" sortable="false"
                                 headerText="#{bindings['sis_deduccion_isr1'].labels.FechaCreacion}"
                                 noWrap="true">
                        <af:outputText value="#{row.FechaCreacion}">
                          <f:convertDateTime pattern="#{bindings['sis_deduccion_isr1'].formats.FechaCreacion}"/>
                        </af:outputText>
                      </af:column>
                    </af:table>
                    <af:objectSeparator/>
                    <af:table value="#{bindings['sis_resumen_deduccion_isr'].collectionModel}"
                              var="row"
                              rows="#{bindings['sis_resumen_deduccion_isr'].rangeSize}"
                              first="#{bindings['sis_resumen_deduccion_isr'].rangeStart}"
                              emptyText="#{bindings['sis_resumen_deduccion_isr'].viewable ? 'Ninguna fila' : 'Ninguna fila'}"
                              partialTriggers="tabla_deduccion_isr"
                              banding="row" bandingInterval="1">
                      <f:facet name="footer">
                        <af:outputText value="TOTAL"/>
                      </f:facet>
                      <f:facet name="header">
                        <af:outputText value="#{txtdeducs['ingresar_deducciones.tbl_resumen.titulo.texto']}"/>
                      </f:facet>
                      <af:column sortProperty="Nombre" sortable="false"
                                 headerText="#{txtdeducs['ingresar_deducciones.tbl_resumen.col_tipo_deduccion.texto']}">
                        <af:outputText value="#{row.Nombre}"/>
                      </af:column>
                      <af:column sortProperty="SumSisDeduccionIsrCantidad"
                                 sortable="false"
                                 headerText="#{txtdeducs['ingresar_deducciones.tbl_resumen.col_cantidad.texto']}"
                                 formatType="number">
                        <af:outputText value="#{row.Monto}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings['sis_resumen_deduccion_isr'].formats.SumSisDeduccionIsrCantidad}"/>
                        </af:outputText>
                        <f:facet name="footer">
                          <h:panelGroup>
                            <af:outputText value="#{bindings['sis_resumen_deduccion_isrTotal'].inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings['sis_resumen_deduccion_isrTotal'].format}"/>
                            </af:outputText>
                          </h:panelGroup>
                        </f:facet>
                      </af:column>
                    </af:table>
                    <af:objectSeparator/>
                    <af:panelButtonBar>
                      <af:commandButton text="Generar Isr Definitivo"
                                        action="#{Ingresar_deduccion.btn_isr_definitivo}"/>
                      <af:commandButton text="#{txtdeducs['ingresar_deducciones.btn_calcularProy.texto']}"
                                        action="#{Ingresar_deduccion.btn_reprocesar}"/>
                      <af:commandButton text="#{txtdeducs['ingresar_deducciones.btn_imprimir_bolpago.texto']}"
                                        onclick="genReporte(#{flujo_usuario.selRegEmpleado});"/>
                      <af:commandButton text="Boleta ISR Mensual"
                                        onclick="genReporteMensual('#{flujo_usuario.selRegEmpleado}');"/>
                      <af:commandButton text="Detalle Otras Rentas"
                                        onclick="genOtrasRentas('#{flujo_usuario.selRegEmpleado}');"
                                        rendered="true"/>
                    </af:panelButtonBar>
                  </af:showDetailItem>
                  <af:showDetailItem text="Salarios" disclosed="false"
                                     disclosureListener="#{Ingresar_deduccion.tab_deduccion}">
                    <af:table value="#{bindings['SisSueldosEmpl_ISR_vw'].collectionModel}"
                              var="row"
                              rows="#{bindings['SisSueldosEmpl_ISR_vw'].rangeSize}"
                              first="#{bindings['SisSueldosEmpl_ISR_vw'].rangeStart}"
                              emptyText="#{txtdeducs['tbl_empty.texto']}"
                              banding="row" bandingInterval="1">
                      <f:facet name="footer">
                        <af:panelGroup layout="vertical">
                          <af:outputText value="Total"/>
                          <af:outputText value="Total Anulados"/>
                          <af:outputText value="Total - Anulados"/>
                        </af:panelGroup>
                      </f:facet>
                      <af:column sortProperty="Partida1" sortable="false"
                                 headerText="Partida">
                        <af:outputText value="#{row.Partida1}"/>
                      </af:column>
                      <af:column sortProperty="NumeroPlaza" sortable="false"
                                 headerText="NumPlaza">
                        <af:outputText value="#{row.NumeroPlaza}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings['SisSueldosEmpl_ISR_vw'].formats.NumeroPlaza}"/>
                        </af:outputText>
                      </af:column>
                      <af:column sortProperty="Tiponomina" sortable="false"
                                 headerText="#{bindings['SisSueldosEmpl_ISR_vw'].labels.Tiponomina}"
                                 noWrap="true">
                        <af:outputText value="#{row.Tiponomina}"/>
                      </af:column>
                      <af:column sortProperty="Periodo" sortable="false"
                                 headerText="#{bindings['SisSueldosEmpl_ISR_vw'].labels.Periodo}">
                        <af:outputText value="#{row.Periodo}"/>
                      </af:column>
                      <af:column sortProperty="Nominal" sortable="false"
                                 headerText="#{bindings['SisSueldosEmpl_ISR_vw'].labels.Nominal}"
                                 formatType="number">
                        <af:outputText value="#{row.Nominal}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings['SisSueldosEmpl_ISR_vw'].formats.Nominal}"/>
                        </af:outputText>
                        <f:facet name="footer">
                          <af:panelGroup layout="vertical">
                            <af:outputText value="#{bindings.getTotalNominalSA1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalNominalSA1.format}"/>
                            </af:outputText>
                            <af:outputText value="#{bindings.getTotalNominal1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalNominal1.format}"/>
                            </af:outputText>
                            <af:outputText value="#{bindings.getTotalNominalDiff1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalNominalDiff1.format}"/>
                            </af:outputText>
                          </af:panelGroup>
                        </f:facet>
                      </af:column>
                      <af:column sortProperty="TotalBono" sortable="false"
                                 headerText="#{bindings['SisSueldosEmpl_ISR_vw'].labels.TotalBono}"
                                 formatType="number">
                        <af:outputText value="#{row.TotalBono}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings['SisSueldosEmpl_ISR_vw'].formats.TotalBono}"/>
                        </af:outputText>
                        <f:facet name="footer">
                          <af:panelGroup layout="vertical">
                            <af:outputText value="#{bindings.getTotalBonoSA1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalBonoSA1.format}"/>
                            </af:outputText>
                            <af:outputText value="#{bindings.getTotalBono1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalBono1.format}"/>
                            </af:outputText>
                            <af:outputText value="#{bindings.getTotalBonoDiff1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalBonoDiff1.format}"/>
                            </af:outputText>
                          </af:panelGroup>
                        </f:facet>
                      </af:column>
                      <af:column sortable="false" headerText="IGSS"
                                 formatType="number">
                        <af:outputText value="#{row.Totaligss}"/>
                        <f:facet name="footer">
                          <af:panelGroup layout="vertical">
                            <af:outputText value="#{bindings.getTotalIgssSA1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalIgssSA1.format}"/>
                            </af:outputText>
                            <af:outputText value="#{bindings.getTotalIgss1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalIgss1.format}"/>
                            </af:outputText>
                            <af:outputText value="#{bindings.getTotalIgssDiff1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalIgssDiff1.format}"/>
                            </af:outputText>
                          </af:panelGroup>
                        </f:facet>
                      </af:column>
                      <af:column sortProperty="Totalisr" sortable="false"
                                 headerText="#{bindings['SisSueldosEmpl_ISR_vw'].labels.Totalisr}"
                                 formatType="number">
                        <af:outputText value="#{row.Totalisr}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings['SisSueldosEmpl_ISR_vw'].formats.Totalisr}"/>
                        </af:outputText>
                        <f:facet name="footer">
                          <af:panelGroup layout="vertical">
                            <af:outputText value="#{bindings.getTotalISRSA1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalISRSA1.format}"/>
                            </af:outputText>
                            <af:outputText value="#{bindings.getTotalISR1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalISR1.format}"/>
                            </af:outputText>
                            <af:outputText value="#{bindings.getTotalISRDiff1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.getTotalISRDiff1.format}"/>
                            </af:outputText>
                          </af:panelGroup>
                        </f:facet>
