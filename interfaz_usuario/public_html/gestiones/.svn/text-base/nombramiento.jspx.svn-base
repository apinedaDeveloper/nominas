<?xml version='1.0' encoding='windows-1252'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:af="http://xmlns.oracle.com/adf/faces"
          xmlns:afh="http://xmlns.oracle.com/adf/faces/html">
  <jsp:output omit-xml-declaration="true" doctype-root-element="HTML"
              doctype-system="http://www.w3.org/TR/html4/loose.dtd"
              doctype-public="-//W3C//DTD HTML 4.01 Transitional//EN"/>
  <jsp:directive.page contentType="text/html;charset=windows-1252"/>
  <f:view>
    <f:loadBundle basename="propiedades" var="tx"/>
    <afh:html>
      <afh:head title="#{tx['contrato.title']}">
        <meta http-equiv="Content-Type"
              content="text/html; charset=windows-1252"/>
       <jsp:include page="/js/libJQ.js"/>          
       <jsp:include page="/js/libMask.js"/>
       <jsp:include page="/js/librariesDialog.js"/>   
         
      </afh:head>
      <afh:body>
      <script type="text/javascript" language="javascript">
        function openNewWindow(nomArchivo){
                window.open("../ver_documento?source="+nomArchivo,"preview","width=640,height=640,scrollbars=YES");
            }
            
        function  verReporte(idReporte){ 
               window.open("../verreportepdf?id="+idReporte+String.fromCharCode(38)+"rep=SIS-04","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
        }
        
        function verReporteDts(idReporte){ 
               window.open("../verreportepdf?id="+idReporte+String.fromCharCode(38)+"rep=CONTDTSPER","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
        }
        
        function  verReporteCatedra(idReporte){ 
               window.open("../verreportepdf?id="+idReporte+String.fromCharCode(38)+"rep=AUXCATEDRA","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
        }
         
        </script> 
        <style type="text/css">
         .mostrar { display: none}
        </style>
        
        <af:messages binding="#{backing_gestiones_nombramiento.mensajes}"
                     partialTriggers="btn_guardar_Contrato"/>
        <af:form usesUpload="true">
        <af:page title="#{flujo_usuario.datosEmpleadoSel}"
           value="#{modeloMenu.modeloBD}" var="node"  >
           <f:facet name="nodeStamp">
              <af:commandMenuItem text="#{node.label}"
                                  actionListener="#{modeloMenu.listener_menu}"
                                  disabled="#{node.readOnly}"
                                  rendered="#{node.shown}"
                                  action="#{node.getOutcome}"/>
            </f:facet>
             <f:facet name="infoUser">
              <f:subview id="info1">
                <jsp:include page="/global/facet_infoUser.jspx"/>
              </f:subview>
            </f:facet>
             <f:facet name="menuSwitch">
              <f:subview id="menuSwt">
                <jsp:include page="/global/facet_menuSwitch.jspx"/>
              </f:subview>
            </f:facet>
            <f:facet name="branding">
              <f:subview id="logo">
                <jsp:include page="/global/facet_branding.jspx"/>
              </f:subview>
            </f:facet>
          
            <af:panelBox text="Numero Contrato: #{bindings.NumContratoFormato.inputValue}">
              <af:showOneTab position="above" id="tab_principal">
                <af:showDetailItem text="#{tx['contrato.tab.dat_per.texto']}"
                                   id="pestania1"
                                   binding="#{backing_gestiones_nombramiento.shw_pestania1}">
                  <af:panelForm partialTriggers="btn_auto_contrato">
                  <af:panelHeader text="#{tx['contrato.pnl_puesto.pnlhd1.texto']}">
                      <af:selectOneChoice value="#{bindings['tipo_contratoNombre'].inputValue}"
                                        label="#{tx['contrato_pnl_puesto.cmb_tipo_cont.texto']}"
                                        binding="#{backing_gestiones_nombramiento.cmb_tipoContrato}"
                                        disabled="#{bindings.estadoContrl.inputValue}"
                                          showRequired="true" readOnly="false"
                                          partialTriggers="btn_selec_plaza">
                      <f:selectItems value="#{bindings['tipo_contratoNombre'].items}"/>
                    </af:selectOneChoice>
                    <af:objectSpacer width="10" height="20"/>
                    <af:inputText label="#{tx['contrato_pnl_puesto.inpt_base_legal']}"
                                  rows="4" columns="100"
                                  disabled="#{bindings.estadoContrl.inputValue}"
                                  value="#{bindings.BaseLegal.inputValue}"
                                  showRequired="true"/>
                    <af:panelHeader>
            
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.nombreCompl.texto']}">
                          <af:outputText value="#{bindings.NombreCompleto.inputValue}"/>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.reg_empleado.texto']}">
                          <af:outputText value="#{ bindings.RegistroEmpleado.inputValue}"
                                         converter="javax.faces.Character"/>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.cui.texto']}">
                          <af:outputText value="#{bindings.Cui.inputValue}"/>
                        </af:panelLabelAndMessage>
                       
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.num_cedula.texto']}">
                          <af:outputText value="#{bindings.NumCedula.inputValue}"/>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.extendida.texto']}">
                          <af:outputText value="#{bindings.Extendida.inputValue}"/>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.edad.texto']}">
                          <af:outputText value="#{bindings.Edad.inputValue}">
                            <f:convertNumber groupingUsed="false"
                                             pattern="#{bindings.Edad.format}"/>
                          </af:outputText>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.est_civ.texto']}">
                          <af:outputText value="#{bindings.Estadocivil.inputValue}"/>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.nacio.texto']}">
                          <af:outputText value="#{bindings.Nacionalidad.inputValue}"/>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.direc.texto']}">
                          <af:outputText value="#{bindings.Direccion.inputValue}"
                                         truncateAt="70"/>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.nivel_est.texto']}">
                          <af:outputText value="#{bindings.Descripcion.inputValue}"/>
                        </af:panelLabelAndMessage>
                        <af:panelLabelAndMessage label="#{tx['contrato.pnl_datos.cole.texto']}">
                          <af:outputText value="#{bindings.Colegiado.inputValue}"/>
                        </af:panelLabelAndMessage>
                        <af:selectBooleanCheckbox
                                                  label="#{tx['contrato.chk_primerI']}"
                                                  value="#{bindings.PrimerIngreso.inputValue}"
                                                  selected="#{bindings.PrimerIngreso.inputValue}"
                                                  disabled="#{bindings.estadoContrl.inputValue}"/>
                      </af:panelHeader>
                    <af:objectSpacer width="10" height="50"/>
                    </af:panelHeader>
                  <af:panelHeader text="#{tx['contrato_pnl_puesto.texto']}"
                                  partialTriggers="btn_selec_plaza">
                     
                          <af:panelLabelAndMessage label="#{tx['contrato.pnl_puesto.tbl_plaza.col_dep.texto']}">
                            <af:outputText value="#{bindings.Dependencia.inputValue}"/>
                          </af:panelLabelAndMessage>
                          <af:panelLabelAndMessage label="#{tx['contrato.pnl_puesto.tbl_plaza.col_unid.texto']}">
                            <af:outputText value="#{bindings.Unidad.inputValue}"/>
                          </af:panelLabelAndMessage>
     
                          <af:panelLabelAndMessage label="#{tx['contrato.pnl_puesto.tbl_plaza.col_psto.texto']}">
                            <af:outputText value="#{bindings.Puesto.inputValue}"
                                           partialTriggers="btn_selec_plaza"/>
                          </af:panelLabelAndMessage>
                          <af:panelLabelAndMessage label="#{tx['contrato.pnl_puesto.tbl_plaza.col_partda.texto']}">
                            <af:outputText value="#{bindings.Partida.inputValue}"/>
                          </af:panelLabelAndMessage>
                          <af:panelLabelAndMessage label="#{tx['contrato.pnl_puesto.tbl_plaza.col_cod_psto.texto']}">
                            <af:outputText value="#{bindings.IdPuesto1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.IdPuesto1.format}"/>
                            </af:outputText>
                          </af:panelLabelAndMessage>
                          <af:panelLabelAndMessage label="#{tx['contrato.pnl_puesto.tbl_plaza.col_plaza.texto']}">
                            <af:outputText value="#{bindings.NumeroPlaza1.inputValue}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings.NumeroPlaza1.format}"/>
                            </af:outputText>
                          </af:panelLabelAndMessage>
                      <af:panelLabelAndMessage label="#{tx['contrato.pnl_puesto.tbl_plaza.col_cant_horas.texto']}">
                        <af:outputText value="#{bindings.CantidadHorasDet.inputValue}">
                            <f:convertNumber groupingUsed="false"
                                             pattern="#{bindings.CantidadHorasDet.format}"/>
                          </af:outputText>
                      </af:panelLabelAndMessage>
                      <af:inputText value="#{bindings['SisContrato_vwNomPlaza'].inputValue}"
                                    label="#{tx['contrato.pnl_puesto.inpt_nomPlaza.texto']}"
                                    required="#{bindings['SisContrato_vwNomPlaza'].mandatory}"
                                    columns="25"
                                    disabled="#{bindings.estadoContrl.inputValue}"
                                    tip="Para plazas fuera de carrera o fuera de clasificación">
                        <af:validator binding="#{bindings['SisContrato_vwNomPlaza'].validator}"/>
                      </af:inputText>
                      <af:panelLabelAndMessage label="#{tx['contrato.pnl_puesto.chk_curso.texto']}">
                        <af:panelHorizontal>
                          <af:selectBooleanCheckbox disabled="#{bindings.estadoContrl.inputValue}"
                                                    value="#{bindings.ContractVacacion.inputValue}"
                                                    id="chk_curso_vaca"
                                                    onclick="check_rule(this);"/>
                          <af:objectSpacer width="20" height="10"/>
                          <af:selectBooleanCheckbox
                                                    label="¿Paga Bono?"
                                                    value="#{bindings.PagaBono.inputValue}"
                                                    selected="#{bindings.PagaBono.inputValue}"
                                                    id="chk_paga_bono"
                                                    tip="Aplica solo para curso vacaciones"
                                                    disabled="#{bindings.estadoContrl.inputValue==false?!bindings.ContractVacacion.inputValue:true}"/>
                        </af:panelHorizontal>
                      </af:panelLabelAndMessage>
                      <af:outputText value="#{bindings.HabilitaBono.inputValue}"
                                     rendered="false"/>
                      <af:objectSpacer width="10" height="30"/>
                      <af:commandButton text="#{tx['contrato.pnl_puesto.btn_selec.texto']}"
                                        action="dialog:sel_plaza"
                                        useWindow="true" windowHeight="600"
                                        windowWidth="700"
                                        returnListener="#{backing_gestiones_nombramiento.return_sel_plazas}"
                                        disabled="#{bindings.estadoContrl.inputValue}"
                                        id="btn_selec_plaza"
                                        partialSubmit="false">
                        <af:setActionListener from="1"
                                              to="#{bindings.Escalaf.inputValue}"/>
                      </af:commandButton>
                      <af:commandButton text="Reporte Auxiliar de Catedra"
                                        onclick="return verReporteCatedra(#{bindings.RegistroEmpleado.inputValue})"
                                        rendered="#{bindings.isAuxCatedra.inputValue}"/>
                    </af:panelHeader>
                      <af:panelHeader partialTriggers="btn_selec_plaza">
                        <af:inputText value="#{bindings.Sueldobase.inputValue}"
                                      label="#{tx['contrato_pnl_puesto.inpt_sueldo_base.texto']}"
                                      columns="12"
                                      binding="#{backing_gestiones_nombramiento.inpt_sueldoBase}"
                                      partialTriggers="btn_sel_plaza btn_luisxm"
                                      disabled="true">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings.Sueldobase.format}"/>
                        </af:inputText>
                        <af:inputText value="#{bindings.ComplSal.inputValue}"
                                      label="#{tx['contrato.pnl_puesto.inpt_compleSal.texto']}"
                                      columns="12"
                                      partialTriggers="btn_selec_plaza btn_luisxm"
                                      binding="#{backing_gestiones_nombramiento.inpt_complemento_sal}"
                                      disabled="#{bindings.estadoContrl.inputValue}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings.ComplSal.format}"/>
                        </af:inputText>
                      <af:inputText value="#{bindings.Escalaf.inputValue}"
                                      label="#{tx['contrato.pnl_puesto.inpt_escalafon.texto']}"
                                      columns="12"
                                      disabled="#{bindings.estadoContrl.inputValue}"
                                      partialTriggers="btn_selec_plaza btn_luisxm"
                                    binding="#{backing_gestiones_nombramiento.inpt_escalafon}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings.Escalaf.format}"/>
                        </af:inputText>
                      </af:panelHeader>
                      <af:panelHeader
                                      rendered="true"
                                      partialTriggers="btn_selec_plaza"
                                      text="#{tx['contrato.pnlVigencia.texto']}">
                        <af:panelHorizontal partialTriggers="btn_selec_plaza"
                                            valign="top">
                          <af:selectInputDate value="#{bindings.FechaInicio.inputValue}"
                                              label="#{tx['contrato_pnl_puesto.inp_fechaI.texto']}"
                                              disabled="#{bindings.estadoContrl.inputValue}"
                                              binding="#{backing_gestiones_nombramiento.inpt_fecha_i}"
                                              partialTriggers="btn_sel_plaza"
                                              showRequired="true">
                            <f:convertDateTime pattern="#{bindings.FechaInicio.format}"/>
                            <af:validator binding="#{bindings.FechaInicio.validator}"/>
                          </af:selectInputDate>
                          <af:selectInputDate value="#{bindings.FechaFin.inputValue}"
                                              label="#{tx['contrato_pnl_puesto.inp_fechaF.texto']}"
                                              binding="#{backing_gestiones_nombramiento.inpt_fecha_f}"
                                              disabled="#{bindings.estadoContrl.inputValue}"
                                              partialTriggers="btn_sel_plaza"
                                              showRequired="true">
                            <f:convertDateTime pattern="#{bindings.FechaFin.format}"/>
                            <af:validator binding="#{bindings.FechaFin.validator}"/>
                          </af:selectInputDate>
                          <f:facet name="separator">
                            <af:objectSpacer width="40" height="10"/>
                          </f:facet>
                        <af:selectOneChoice
                                            value="#{bindings.TipoValidez.inputValue}"
                                            disabled="#{bindings.estadoContrl.inputValue}"
                                            onchange="verificaValidez();"
                                            id="cmbValidezC"
                                            valuePassThru="true">
                          <af:selectItem label="- - - - - - - - - - - -"
                                         value="N"/>
                          <af:selectItem label="Indefinido" value="I"/>
                          <af:selectItem label="Interino por Suspensión IGSS del Titular"
                                         value="G"/>
                          <af:selectItem label="Pendiente Resolución JUP"
                                         value="J"/>
                          <af:selectItem label="Hasta Alta Suspensión IGSS del titular"
                                         value="A"/>
                          <af:selectItem label="Hasta que tome posesión la nueva Autoridad"
                                         value="P"/>
                          <af:selectItem label="Hasta que sancione titularidad el Consejo Superior Universitario"
                                         value="T"/>
                          <af:selectItem label="Por oposición (Auxiliares de Cátedra y Auxiliares de Investigación)"
                                         value="O"/>
                        </af:selectOneChoice>
                        <af:selectOneChoice id="cmb_tipoIndef"
                                            value="#{bindings.OpcionIndef.inputValue}"
                                            shortDesc="Solo aplica para contratos indefinidos y por oposición."
                                            disabled="#{bindings.estadoContrl.inputValue}"
                                            tip="Solo Para Contratos Indefinidos">
                          <af:selectItem label=" " value="0"/>                  
                          <af:selectItem label="Todo el año" value="1"/>
                          <af:selectItem label="Primer Semestre" value="2"/>
                          <af:selectItem label="Segundo Semestre" value="3"/>
                        </af:selectOneChoice>
                        <af:selectInputDate label="No exceda de"
                                            disabled="#{bindings.estadoContrl.inputValue}"
                                            value="#{bindings.NoExcedaDe.inputValue}"
                                            id="inptNoExceda">
                           <f:convertDateTime pattern="#{bindings.NoExcedaDe.format}"/>
                            <af:validator binding="#{bindings.NoExcedaDe.validator}"/>
                       </af:selectInputDate>                     
                      </af:panelHorizontal>
                      <af:objectSpacer width="10" height="20"/>
                      <af:panelHorizontal partialTriggers="btn_horario_crear tbl_horario:btn_hora_borrar btn_hora_ejec tbl_horario"
                                            rendered="false">
                        <af:commandButton actionListener="#{bindings.Create1.execute}"
                                          text="#{tx['contrato.pnl_puesto.btn_agr_horario.texto']}"
                                          disabled="#{!bindings.Create1.enabled}"
                                          partialSubmit="true"
                                          id="btn_horario_crear"
                                          action="#{backing_gestiones_nombramiento.btn_crear_horario_action}"
                                          binding="#{backing_gestiones_nombramiento.btn_horario_crear}"
                                          onclick="createHorarioAction();"/>
                        <af:commandButton actionListener="#{bindings.Execute.execute}"
                                            text="#{tx['contrato.pnl_puesto.btn_Act_horario.texto']}"
                                            disabled="#{!bindings.Execute.enabled}"
                                            partialSubmit="true"
                                            id="btn_hora_ejec"
                                          onclick="createHorarioAction();"/>
                          <f:facet name="separator">
                            <af:objectSpacer width="30" height="10"/>
                          </f:facet>
                        </af:panelHorizontal>
                        <af:objectSpacer width="10" height="20"/>
                      <af:panelHorizontal>
                        <af:table rows="#{bindings['SisContrato_vwSisHorarioContrato'].rangeSize}"
                                  first="#{bindings['SisContrato_vwSisHorarioContrato'].rangeStart}"
                                  emptyText="#{tx['contrato_pnl_puesto.tbl_horario.norows.texto']}"
                                  selectionState="#{bindings['SisContrato_vwSisHorarioContrato'].collectionModel.selectedRow}"
                                  selectionListener="#{bindings['SisContrato_vwSisHorarioContrato'].collectionModel.makeCurrent}"
                                  id="tbl_horario"
                                  partialTriggers="btn_horario_crear tbl_horario:btn_hora_borrar btn_hora_ejec btn_sig_datos tbl_horario:btn_crearHorario tbl_horario:btn_tblHorarioOK tbl_horario:cmd_editH tbl_horario:cmd_borrarH1"
                                  binding="#{backing_gestiones_nombramiento.tbl_horario}"
                                  rendered="true"
                                  value="#{bindings['SisContrato_vwSisHorarioContrato'].collectionModel}"
                                  var="row" immediate="false" banding="row"
                                  bandingInterval="1">
                          <f:facet name="selection">
                            <af:tableSelectOne autoSubmit="true"
                                               partialTriggers="btn_sig_datos"
                                               disabled="#{bindings.estadoContrl.inputValue}"
                                               binding="#{backing_gestiones_nombramiento.tbl_selection}"
                                               onclick="createHorarioAction();"/>
                          </f:facet>
                          <f:facet name="footer">
                            <af:outputText value="#{bindings.numHoras.inputValue}  horas semanales"/>
                          </f:facet>
                          <f:facet name="actions">
                            <h:panelGroup rendered="#{bindings.TipoValidez.inputValue eq 'I'?true:!bindings.estadoContrl.inputValue}">
                              <af:commandButton text="#{tx['contrato.tbl_horario.btn_crear.texto']}"
                                                action="#{backing_gestiones_nombramiento.btn_horario_crear}"
                                                partialSubmit="true"
                                                id="btn_crearHorario"
                                                onclick="createHorarioAction();"
                                                binding="#{backing_gestiones_nombramiento.btn_crear_horarioNew}"
                                                disabled="#{bindings.estadoContrl.inputValue}"/>
                              <af:commandButton text="#{tx['contrato.tbl_horario.btn_ok.texto']}"
                                                action="#{backing_gestiones_nombramiento.btn_horario_OK}"
                                                partialSubmit="true"
                                                id="btn_tblHorarioOK"
                                                disabled="#{bindings.estadoContrl.inputValue}"/>
                            </h:panelGroup>
                          </f:facet>
                          <af:column sortable="false" headerText="Editar"
                                     rendered="#{!bindings.estadoContrl.inputValue}">
                            <af:commandLink shortDesc="Editar"
                                            action="#{backing_gestiones_nombramiento.btn_editar_horarioNew}"
                                            binding="#{backing_gestiones_nombramiento.cm_lnk_editarHorario}"
                                            id="cmd_editH" partialSubmit="true">
                              <af:objectImage source="/imagenes/form_edit.png"/>
                            </af:commandLink>
                          </af:column>
                          <af:column sortProperty="IdContrato" sortable="false"
                                     headerText="#{tx['empleado.tbl_horario.nomD.texto']}"
                                     noWrap="true">
                            <af:outputText value="#{row.nomDia}"
                                           rendered="#{!row.Selected}">
                              <f:convertNumber groupingUsed="false"
                                               pattern="#{bindings['SisContrato_vwSisHorarioContrato'].formats.IdContrato}"/>
                            </af:outputText>
                            <af:selectOneChoice value="#{bindings['dias_Semana_vwNombre'].inputValue}"
                                                label="#{tx['contrato_pnl_puesto.cmb_dia.texto']}"
                                                disabled="#{bindings.estadoContrl.inputValue}"
                                                rendered="#{row.Selected}">
                              <f:selectItems value="#{bindings['dias_Semana_vwNombre'].items}"/>
                            </af:selectOneChoice>
                          </af:column>
                          <af:column sortProperty="HoraEntrada2"
                                     sortable="false"
                                     headerText="#{tx['empleado.tbl_horario.HI.texto']}">
                            <af:outputText value="#{row.HoraEntrada2}"
                                           rendered="#{!row.Selected}"/>
                            <af:inputText label="#{tx['contrato_pnl_puesto.inp_horaE.texto']}"
                                          value="#{bindings.SisHorarioContratoHoraEntrada.inputValue}"
                                          simple="false"
                                          required="#{bindings.SisHorarioContratoHoraEntrada.mandatory}"
                                          columns="5"
                                          binding="#{backing_gestiones_nombramiento.inpt_horaE}"
                                          disabled="#{bindings.estadoContrl.inputValue}"
                                          id="inptHoraE"
                                          onfocus="setMask(this);"
                                          rendered="#{row.Selected}">
                              <af:validator binding="#{bindings.SisHorarioContratoHoraEntrada.validator}"/>
                            </af:inputText>
                          </af:column>
                          <af:column sortProperty="HoraSalida2" sortable="false"
                                     headerText="#{tx['empleado.tbl_horario.HF.texto']}">
                            <af:outputText value="#{row.HoraSalida2}"
                                           rendered="#{!row.Selected}"/>
                            <af:inputText value="#{bindings.SisHorarioContratoHoraSalida.inputValue}"
                                          simple="false"
                                          required="#{bindings.SisHorarioContratoHoraSalida.mandatory}"
                                          columns="5"
                                          label="#{tx['contrato_pnl_puesto.inp_horaS.texto']}"
                                          binding="#{backing_gestiones_nombramiento.inpt_horaS}"
                                          disabled="#{bindings.estadoContrl.inputValue}"
                                          id="inptHoraS"
                                          onfocus="setMask2(this);"
                                          rendered="#{row.Selected}">
                              <af:validator binding="#{bindings.SisHorarioContratoHoraSalida.validator}"/>
                            </af:inputText>
                          </af:column>
                          <af:column sortable="false" headerText="Borrar">
                            <af:commandLink
                                            action="#{backing_gestiones_nombramiento.cmd_borrarH}"
                                            id="cmd_borrarH1"
                                            partialSubmit="true"
                                            binding="#{backing_gestiones_nombramiento.cmd_link_borrarH}"
                                            rendered="#{!bindings.estadoContrl.inputValue}">
                              <af:objectImage source="/imagenes/borrar.png"/>
                            </af:commandLink>
                          </af:column>
                        </af:table>
