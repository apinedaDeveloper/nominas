<?xml version='1.0' encoding='windows-1252'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0"
          xmlns:h="http://java.sun.com/jsf/html"
          xmlns:f="http://java.sun.com/jsf/core"
          xmlns:afh="http://xmlns.oracle.com/adf/faces/html"
          xmlns:af="http://xmlns.oracle.com/adf/faces">
  <jsp:directive.page contentType="text/html;charset=windows-1252"
                      autoFlush="true" language="java"/>
  <f:view>
    <afh:html>
      <afh:head title="Complementos">
        <meta http-equiv="Content-Type"
              content="text/html; charset=windows-1252"/>
      </afh:head>
      <jsp:include page="/js/libJQ.js"/>       
      <jsp:include page="/js/librariesDialog.js"/>           
      <script type="text/javascript" src="/${flujo_usuario.dominioApp}/js/gestiones/scrptAutorizacion.js">
      var xy=new Date().getMilliseconds();      
      </script>
      <script language="javascript" type="text/javascript"> 
       gbl_form="form1";
       gbl_actionId="form1:page1:cmdRefresh";
    
         function verRepo(IdC){
         
           window.open("../verreportepdf?id="+IdC+String.fromCharCode(38)+"rep=CMPLSAL","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
           return false;    
   
         }
         
         function verRepoCompIgss(IdC){
         
           window.open("../verreportepdf?id="+IdC+String.fromCharCode(38)+"rep=SIS-15","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
           return false;    
   
         }
      
      </script>      
      <afh:body>
        <af:messages/>
        <h:form id="form1">
          <af:page title="Complementos IGSS" value="#{modeloMenu.modeloBD}" var="node" id="page1">
            <f:facet name="nodeStamp">
              <af:commandMenuItem text="#{node.label}"
                                  actionListener="#{modeloMenu.listener_menu}"
                                  disabled="#{node.readOnly}"
                                  rendered="#{node.shown}"
                                  action="#{node.getOutcome}"/>
            </f:facet>
             <f:facet name="infoUser">
              <f:subview id="info1">
                <jsp:include page="/global/facet_infoUser.jspx"/>
              </f:subview>
            </f:facet>
             <f:facet name="menuSwitch">
              <f:subview id="menuSwt">
                <jsp:include page="/global/facet_menuSwitch.jspx"/>
              </f:subview>
            </f:facet>
            <f:facet name="branding">
              <f:subview id="logo">
                <jsp:include page="/global/facet_branding.jspx"/>
              </f:subview>
            </f:facet>
            <f:verbatim>
                <input type="hidden" name="hdn_tipo" id="hdn_tipo" value="12"/>
                <input type="hidden" name="hdn_Id" id="hdn_Id"
                       value="${bindings.pIdVar1.inputValue}"/>
              </f:verbatim>
            <af:showDetail undisclosedText="Mostrar consejos"
                           disclosedText="Ocultar consejos">
              <af:panelHeader text="Tips:">
                <af:panelList>
                  <af:outputFormatted value="Recuerde que debe grabar los datos de la suspensión IGSS"
                                      inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>
                  <af:outputFormatted value='Para crear un complemento IGSS haga clic sobre la pestaña "Nuevo complemento IGSS"'
                                      inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>
                  <af:outputFormatted value='Para editar un complemento que no se ha trasladado haga clic en la pestaña "Histórico" y luego haba clic sobre el número de complemento ubicado en la columna "IdComplemento"'
                                      inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>
                  <af:outputFormatted value="Recuerde que debe trasladar el complemento para que el analista de sueldos lo pueda autorizar. Para trasladar el complemento debe ingresar al modo de edición siguiendo los pasos del item anterior."
                                      inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>
                </af:panelList>
              </af:panelHeader>
            </af:showDetail>
            <af:objectSpacer width="10" height="10"/>
            <af:showOneTab>
              <af:showDetailItem text="Histórico"
                                 binding="#{complementos_igss_teso.tabHistorico}">
                <af:panelForm>
                  <af:selectInputDate value="#{bindings.fechaI.inputValue}"
                                      label="Fecha inicio"
                                      required="#{bindings.fechaI.mandatory}">
                    <af:validator binding="#{bindings.fechaI.validator}"/>
                    <f:convertDateTime pattern="#{bindings.fechaI.format}"/>
                  </af:selectInputDate>
                  <af:selectInputDate value="#{bindings.fechaF.inputValue}"
                                      label="Fecha fin"
                                      required="#{bindings.fechaF.mandatory}">
                    <af:validator binding="#{bindings.fechaF.validator}"/>
                    <f:convertDateTime pattern="#{bindings.fechaF.format}"/>
                  </af:selectInputDate>
                  <f:facet name="footer">
                    <af:commandButton actionListener="#{bindings.ExecuteSisComplemento.execute}"
                                      text="Buscar"
                                      disabled="#{!bindings.ExecuteSisComplemento.enabled}"/>
                  </f:facet>
                  <af:objectSpacer width="10" height="10"/>
                </af:panelForm>
                <af:panelHeader text="..."/>
                <af:commandLink actionListener="#{bindings.ExecuteSisComplemento.execute}"
                                action="ExecuteSisComplemento"
                                text="Actualizar listado"
                                disabled="#{!bindings.ExecuteSisComplemento.enabled}"/>
                <af:table value="#{bindings.SisComplementoVw.collectionModel}"
                          var="row"
                          rows="#{bindings.SisComplementoVw.rangeSize}"
                          first="#{bindings.SisComplementoVw.rangeStart}"
                          emptyText="#{bindings.SisComplementoVw.viewable ? 'No rows yet.' : 'Access Denied.'}"
                          selectionState="#{bindings.SisComplementoVw.collectionModel.selectedRow}"
                          selectionListener="#{bindings.SisComplementoVw.collectionModel.makeCurrent}"
                          banding="row" bandingInterval="1">
                  <af:column sortProperty="IdComplemento" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.IdComplemento}">
                    <af:outputText value="#{row.IdComplemento}"
                                   rendered="false">
                      <f:convertNumber groupingUsed="false"
                                       pattern="#{bindings.SisComplementoVw.formats.IdComplemento}"/>
                    </af:outputText>
                    <af:commandLink action="#{complementos_igss_teso.commandLink_action}"
                                    text="#{row.IdComplemento}"
                                    disabled="#{!bindings.setCurrentRowWithKey.enabled}"/>
                  </af:column>
                  <af:column sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.Nomestado}">
                    <af:outputText value="#{row.Nomestado}"/>
                  </af:column>
                  <af:column sortProperty="IdEstado" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.IdEstado}"
                             rendered="false">
                    <af:outputText value="#{row.IdEstado}">
                      <f:convertNumber groupingUsed="false"
                                       pattern="#{bindings.SisComplementoVw.formats.IdEstado}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="IdTipo" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.IdTipo}"
                             rendered="false">
                    <af:outputText value="#{row.IdTipo}">
                      <f:convertNumber groupingUsed="false"
                                       pattern="#{bindings.SisComplementoVw.formats.IdTipo}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="IdSubprograma" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.IdSubprograma}">
                    <af:outputText value="#{row.IdSubprograma}"
                                   rendered="false">
                      <f:convertNumber groupingUsed="false"
                                       pattern="#{bindings.SisComplementoVw.formats.IdSubprograma}"/>
                    </af:outputText>
                    <af:outputFormatted value="#{row.Formato2} - #{row.Nomunidad}"/>
                  </af:column>
                  <af:column sortProperty="Periodo" sortable="false"
                             headerText="Mes operado">
                    <af:outputText value="#{row.Periodo}" rendered="false">
                      <f:convertNumber groupingUsed="false"
                                       pattern="#{bindings.SisComplementoVw.formats.Periodo}"/>
                    </af:outputText>
                    <af:panelHorizontal>
                      <af:objectSpacer width="10" height="10"/>
                      <af:outputFormatted value='#{row.Nomperiodo}#{" - "}#{row.Anio}'/>
                    </af:panelHorizontal>
                  </af:column>
                  <af:column sortProperty="Anio" sortable="false"
                             headerText="Año" rendered="false">
                    <af:outputText value="#{row.Anio}">
                      <f:convertNumber groupingUsed="false"
                                       pattern="#{bindings.SisComplementoVw.formats.Anio}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="FechaPresentacion" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.FechaPresentacion}">
                    <af:outputText value="#{row.FechaPresentacion}">
                      <f:convertDateTime pattern="#{bindings.SisComplementoVw.formats.FechaPresentacion}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="CreadoPor" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.CreadoPor}">
                    <af:outputText value="#{row.CreadoPor}"/>
                  </af:column>
                  <af:column sortProperty="FechaCreacion" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.FechaCreacion}">
                    <af:outputText value="#{row.FechaCreacion}">
                      <f:convertDateTime pattern="#{bindings.SisComplementoVw.formats.FechaCreacion}"/>
                    </af:outputText>
                  </af:column>
                  <af:column sortProperty="ModPor" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.ModPor}">
                    <af:outputText value="#{row.ModPor}"/>
                  </af:column>
                  <af:column sortProperty="FechaMod" sortable="false"
                             headerText="#{bindings.SisComplementoVw.labels.FechaMod}">
                    <af:outputText value="#{row.FechaMod}">
                      <f:convertDateTime pattern="#{bindings.SisComplementoVw.formats.FechaMod}"/>
                    </af:outputText>
                  </af:column>
                </af:table>
                <af:objectSpacer width="10" height="10"/>
                <af:objectSpacer width="10" height="10"/>
              </af:showDetailItem>
              <af:showDetailItem text="Editar complemento IGSS"
                                 binding="#{complementos_igss_teso.tabEditarComplemento}"
                                 disclosed="false">
                <af:panelHeader text="Id [ #{bindings.IdComplemento.inputValue} ]"/>
                <af:panelHorizontal halign="left">
                  <af:panelBox>
                    <af:panelGroup>
                      <af:objectSpacer width="10" height="10"/>
                      <af:panelButtonBar>
                        <af:selectOneChoice label="Label 2"
                                            value="#{bindings['Periodo'].inputValue}"
                                            disabled="#{!bindings.IsFormaActiva.inputValue}"
                                            simple="true">
                          <f:selectItems value="#{bindings['Periodo'].items}"/>
                        </af:selectOneChoice>
                        <af:selectOneChoice label="#{tx['cambioAnio.cmb_anios.title']}"
                                        simple="true" rendered="true"
                                        disabled="#{!bindings.IsFormaActiva.inputValue}"
                                        value="#{bindings.vAnio1.inputValue}">
                            <f:selectItems value="#{backing_bean_comun.listaComboAniosTrabajo}"/>
                        </af:selectOneChoice>
                        <af:commandButton actionListener="#{bindings.agregarComplementoIGSS.execute}"
                                          text="agregarComplementoIGSS"
                                          disabled="#{!bindings.agregarComplementoIGSS.enabled}"
                                          rendered="false"/>
                        <af:commandButton text="Agregar complemento"
                                          disabled="#{!bindings.IsFormaActiva.inputValue}"
                                          action="#{complementos_igss_teso.agregarComplemento}"/>
                        <af:commandButton actionListener="#{bindings.crearComplementoIGSS.execute}"
                                          text="CrearComplementoIGSS"
                                          disabled="#{!bindings.crearComplementoIGSS.enabled}"
                                          rendered="false"/>
                        <af:commandButton actionListener="#{bindings.Commit.execute}"
                                          text="Commit" rendered="false"/>
                        <af:commandButton actionListener="#{bindings.Rollback.execute}"
                                          text="Rollback" immediate="true"
                                          rendered="false">
                          <af:resetActionListener/>
                        </af:commandButton>
                        <af:commandButton actionListener="#{bindings.reprocesarComplementoIGSS.execute}"
                                          text="reprocesarComplementoIGSS"
                                          disabled="#{!bindings.reprocesarComplementoIGSS.enabled}"
                                          rendered="false"/>
                        <af:commandButton text="Reprocesar complemento"
                                          action="#{complementos_igss_teso.reprocesar_complemento}"
                                          disabled="#{!bindings.IsFormaActiva.inputValue}"
                                          onclick="return confirm('Está seguro de reprocesar el complemento ?');"/>
                        <af:commandButton text="Guardar"
                                          action="#{complementos_igss_teso.guardar}"
                                          disabled="#{!bindings.IsFormaActiva.inputValue}"/>
                        <af:commandButton text="Cancelar"
                                          action="#{complementos_igss_teso.deshacer}"
                                          disabled="#{!bindings.IsFormaActiva.inputValue}"/>
                        <af:commandLink
                                      id="cmdRefresh"
                                      actionListener="#{bindings.actualizarVW.execute}"
                                      disabled="true"
                                      action="#{complementos_igss_teso.cmdRefresh_action}"/>
                        <af:commandLink onclick="verRepoCompIgss('#{bindings.IdComplemento.inputValue}');">
                          <af:objectImage source="/imagenes/printer_1.png"
                                          shortDesc="Ver SIS - 15"/>
                        </af:commandLink>
                        <af:commandLink onclick="verRepo('#{bindings.IdComplemento.inputValue}');">
                          <af:objectImage source="/imagenes/printer_1.png"
                                          shortDesc="Ver Nomina de complemento"
                                          rendered="false"/>
                        </af:commandLink>
                        <af:commandLink onclick="return OpenDialogo(1);"
                                        returnListener="#{complementos_igss_teso.returnAnulacionComple}">
                          <af:objectImage source="/imagenes/btn_trasladar.png"/>
                        </af:commandLink>
                        <af:commandLink onclick="return OpenDialogo(2);"
                                        returnListener="#{complementos_igss_teso.returnAnulacionComple}">
                          <af:objectImage source="/imagenes/btn_anular.png"/>
                        </af:commandLink>
                        <af:commandLink
                                      action="dialog:det_autoriza"
                                      launchListener="#{complementos_igss_teso.launch_detalle_auto}"
                                      useWindow="true" windowHeight="300"
                                      windowWidth="600">
                            <af:objectImage source="/imagenes/list_information.png"/>
                        </af:commandLink>
                      </af:panelButtonBar>
                    </af:panelGroup>
                  </af:panelBox>
                </af:panelHorizontal>
                <af:panelHorizontal valign="top">
                  <af:outputFormatted value="Nota:"
                                      inlineStyle="color:rgb(255,33,33); font-weight:bold; font-size:small;"/>
                  <af:objectSpacer width="10" height="10"/>
                  <af:panelList>
                    <af:outputFormatted value="Si tiene algún comentario respecto del funcionamiento de esta página, por favor escribanos al correo: siif@usac.edu.gt"
                                        inlineStyle="color:rgb(0,107,165); font-size:smaller;"/>
                    <af:outputFormatted value="Recuerde que debe grabar cualquier cambio que realice"
                                        inlineStyle="color:rgb(0,107,165); font-size:small;"/>
                  </af:panelList>
                </af:panelHorizontal>
                 <af:showDetail undisclosedText="Mostrar instrucciones"
                           disclosedText="Ocultar instrucciones"
                                disclosed="false">
                    <af:panelBox background="transparent" width="700px">
                    <af:panelList>
                      <af:outputFormatted value='Para modificar los valores que el sistema a calculado debe hacer clic sobre la imagen ubicada en la columna "Editar"'
                                          inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>
                      <af:outputFormatted value='Para finalizar la modificación de valores debe hacer clic sobre el botón "Guardar" ubicado mas arriba o bien hacer clic sobre la imagen que se muestra en la columna "Editar"'
                                          inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>
                      <af:outputFormatted value='Al colocar el valor "NO" en la columna "Recalcular valores" usted le esta indicando al sistema que no debe modificar los valores de esta fila, por lo que al relizar un reproceso del complemento no se modificará ninguno de los valores de las filas con el valor "NO".'
                                        inlineStyle="color:rgb(0,107,165); font-size:smaller;"/>
                      <af:outputFormatted value='Para recalcular los montos a pagar debe modificar el rango de días a pagar (día de inicio y día fin), colocar el valor "SI" en la columna "Recalcular valores" y guardar los cambios, luego hacer clic sobre el botón "Reprocesar complemento" ubicado mas arriba, esto hara que el sistema recalcule los valores en base al nuevo rango de dias.'
                                          inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>
                      <af:outputFormatted value='Para eliminar un contrato de este complemento haga clic sobre la imagen ubicada en la columna "Borrar".'
                                          inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>                                          
                      <af:outputFormatted value='Para ver el formulario "SIS-15" haga clic sobre la imagen de la impresora ubicada mas arriba.'
                                          inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>                                                                                    
                      <af:outputFormatted value='Para trasladar el complemento hacia el analista haga clic sobre la imagen con el chequesito en color verde ubicada mas arriba.'
                                          inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>                                                                                         
                      <af:outputFormatted value='Para anular el complemento haga clic sobre la imagen de no permitido en color rojo ubicada mas arriba.'
                                          inlineStyle="color:rgb(0,99,148); font-size:smaller;"/>                                                                                                                                   
                    </af:panelList>
                    </af:panelBox>
                </af:showDetail>
                <af:objectSpacer width="10" height="10"/>
                <af:panelHorizontal valign="top">
                <af:panelBox background="transparent">
                    <af:table value="#{bindings.SisComplementoVwSisComplementoIgssVw.collectionModel}"
                              var="row"
                              rows="#{bindings.SisComplementoVwSisComplementoIgssVw.rangeSize}"
                              first="#{bindings.SisComplementoVwSisComplementoIgssVw.rangeStart}"
                              emptyText="#{bindings.SisComplementoVwSisComplementoIgssVw.viewable ? 'No rows yet.' : 'Access Denied.'}"
                              banding="row" bandingInterval="1"
                              selectionState="#{bindings.SisComplementoVwSisComplementoIgssVw.collectionModel.selectedRow}"
                              selectionListener="#{bindings.SisComplementoVwSisComplementoIgssVw.collectionModel.makeCurrent}"
                              id="tablaCompIgss"
                              partialTriggers="tablaCompIgss:cmdEditarReg tablaCompIgss:cmdFinEditarReg tablaCompIgss:cmdBorrarReg">                              
                      <f:facet name="actions"/>
                      <f:facet name="selection">
                        <af:tableSelectOne autoSubmit="true"/>
                      </f:facet>
                      <af:column sortable="false" headerText="Borrar">
                        <af:outputText/>
                        <af:panelHorizontal>
                          <af:objectSpacer width="10" height="10"/>
                          <af:commandLink actionListener="#{bindings.removeRowWithKey.execute}"
                                          action="#{complementos_igss_teso.confirmarBorrar}"
                                          disabled="#{!bindings.IsFormaActiva.inputValue}"
                                          onclick="return confirm('Está seguro de borrar esta fila?');"
                                          partialSubmit="true"
                                          id="cmdBorrarReg">
                            <af:objectImage source="/imagenes/borrar.png"
                                            rendered="#{bindings.IsFormaActiva.inputValue}"/>
                          </af:commandLink>
                          <af:objectSpacer width="10" height="10"/>
                        </af:panelHorizontal>
                      </af:column>
                      <af:column sortable="false" headerText="Editar">
                        <af:outputText/>
                        <af:panelHorizontal>
                          <af:objectSpacer width="10" height="10"/>
                          <af:commandLink disabled="#{!bindings.IsFormaActiva.inputValue}"
                                          rendered="#{!row.isSel}"
                                          action="#{complementos_igss_teso.cmd_editar}"
                                          partialSubmit="true"
                                          id="cmdEditarReg">
                            <af:objectImage source="/imagenes/form_edit.png"
                                            rendered="#{bindings.IsFormaActiva.inputValue}"
                                            shortDesc="Editar fila"/>
                          </af:commandLink>
                          <af:commandLink disabled="#{!bindings.IsFormaActiva.inputValue}"
                                          rendered="#{row.isSel}"
                                          action="#{complementos_igss_teso.btn_cancelar_edicion}"
                                          partialSubmit="true"
                                          id="cmdFinEditarReg">
                            <af:objectImage source="/imagenes/finished-work.png"
                                            rendered="#{bindings.IsFormaActiva.inputValue}"
                                            shortDesc="Finalizar edición"/>
                          </af:commandLink>
                          <af:objectSpacer width="10" height="10"/>
                        </af:panelHorizontal>
                      </af:column>
                      <af:column headerText="Recalcular valores"
                                 sortable="false">
                        <af:selectOneChoice value="#{row.ReprocesarCalculos}"
                                            simple="true"
                                            required="#{bindings.SisComplementoVwSisComplementoIgssVw.attrDefs.ReprocesarCalculos.mandatory}"
                                            label="Label 1" readOnly="false"
                                            disabled="#{!bindings.IsFormaActiva.inputValue}"
                                            rendered="#{row.isSel}">
                          <af:validator binding="#{bindings.ReprocesarCalculos.validator}"/>
                          <af:selectItem label="SI" value="SI"/>
                          <af:selectItem label="NO" value="NO"/>
                        </af:selectOneChoice>
                        <af:outputText value="#{row.ReprocesarCalculos}"
                                       rendered="#{!row.isSel}"/>
                      </af:column>
                      <af:column sortable="false" headerText="RegPer">
                        <af:commandLink text="#{row.RegistroEmpleado}"
                                        disabled="#{!bindings.IsFormaActiva.inputValue}"
                                        action="#{complementos_igss_teso.verParametrosSuspIgss}"
                                        rendered="false">
                          <af:setActionListener from="#{row.RegistroEmpleado}"
                                                to="#{processScope.CIT_regper_emp}"/>
                          <af:setActionListener from="#{row.Nombrecompleto}"
                                                to="#{processScope.CIT_nombre_emp}"/>
                        </af:commandLink>
                        <af:outputText value="#{row.RegistroEmpleado}"
                                       rendered="true">
                          <f:convertNumber groupingUsed="false"/>
                        </af:outputText>
                      </af:column>
                      <af:column sortable="false" headerText="CUI">
                        <af:outputText value="#{row.Cui}"/>
                      </af:column>
                      <af:column sortable="false" headerText="Nombre">
                        <af:outputText value="#{row.Nombrecompleto}"/>
                      </af:column>
                      <af:column sortable="false" headerText="Partida">
                        <af:outputText value="#{row.Numpartida}"/>
                      </af:column>
                      <af:column sortable="false" headerText="No. Plaza">
                        <af:outputText value="#{row.NumeroPlaza}"/>
                      </af:column>
                      <af:column sortProperty="IdContrato" sortable="false"
                                 headerText="#{bindings.SisComplementoVwSisComplementoIgssVw.labels.IdContrato}">
                        <af:inputText value="#{row.IdContrato}"
                                      required="#{bindings.SisComplementoVwSisComplementoIgssVw.attrDefs.IdContrato.mandatory}"
                                      columns="#{bindings.SisComplementoVwSisComplementoIgssVw.attrHints.IdContrato.displayWidth}"
                                      readOnly="true">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings.SisComplementoVwSisComplementoIgssVw.formats.IdContrato}"/>
                        </af:inputText>
                      </af:column>
                      <af:column sortProperty="DiaInicio" sortable="false"
                                 headerText="#{bindings.SisComplementoVwSisComplementoIgssVw.labels.DiaInicio}">
                        <af:inputText value="#{row.DiaInicio}"
                                      required="#{bindings.SisComplementoVwSisComplementoIgssVw.attrDefs.DiaInicio.mandatory}"
                                      columns="#{bindings.SisComplementoVwSisComplementoIgssVw.attrHints.DiaInicio.displayWidth}"
                                      rendered="#{row.isSel}"
                                      disabled="#{!bindings.IsFormaActiva.inputValue}">
                          <f:convertNumber groupingUsed="false"
                                           pattern="#{bindings.SisComplementoVwSisComplementoIgssVw.formats.DiaInicio}"/>
                        </af:inputText>
                        <af:outputText value="#{row.DiaInicio}"
                                       rendered="#{!row.isSel}"/>
                      </af:column>
                      <af:column sortProperty="DiaFin" sortable="false"
                                 headerText="#{bindings.SisComplementoVwSisComplementoIgssVw.labels.DiaFin}">
