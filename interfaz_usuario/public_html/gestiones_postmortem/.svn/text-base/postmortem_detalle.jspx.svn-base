<?xml version='1.0' encoding='windows-1252'?>
<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0" xmlns:f="http://java.sun.com/jsf/core" xmlns:af="http://xmlns.oracle.com/adf/faces" xmlns:afh="http://xmlns.oracle.com/adf/faces/html">
  <f:view>
    <afh:html>
      <jsp:directive.page contentType="text/html;charset=windows-1252"/>
      <afh:head title="Prestaciones Post-mortem">
        <meta http-equiv="Content-Type" content="text/html; charset=windows-1252"/>
        <jsp:include page="/js/libJQ.js"/>
        <jsp:include page="/js/librariesDialog.js"/>
        <script type="text/javascript" src="/${flujo_usuario.dominioApp}/js/gestiones/scrptAutorizacion.js">var xy=new Date().getMilliseconds();</script>
        <script language="javascript" type="text/javascript">
        //Abre el reporte de Solicitud de Pago de Prestaciones Post-mortem (DARH PM-01)
        function generarReporteDARH_PM_01(idPostMortem){ 
            var link;
            link = "/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+idPostMortem+String.fromCharCode(38)+"rep=DARHPM01_POSTMORTEM";
            window.open(link,'preview','width=1200,height=723,scrollbars=YES,location=NO,directories=NO,resizable=1');
            return false;
        }
        //Abre el reporte de Cálculo de Prestaciones Post-mortem (DARH PM-02)
        function generarReporteDARH_PM_02(idIndemnizacion){ 
            var link;
            link = "/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+idIndemnizacion+String.fromCharCode(38)+"rep=DARHPM02_POSTMORTEM";
            window.open(link,'preview','width=1200,height=723,scrollbars=YES,location=NO,directories=NO,resizable=1');
            return false;
        }
        //Abre el reporte de Liquidación de Prestaciones Post-mortem (DARH PM-03)
        function generarReporteDARH_PM_03(idIndemnizacion){ 
            var link;
            link = "/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+idIndemnizacion+String.fromCharCode(38)+"rep=DARHPM03_POSTMORTEM";
            window.open(link,'preview','width=1200,height=723,scrollbars=YES,location=NO,directories=NO,resizable=1');
            return false;
        }
        //Pregunta si se desea guardar la información
        function preguntarGuardar(){ return confirm("¿Desea guardar la información?"); }
        //Pregunta si se desea cancelar la operación
        function preguntarCancelar(){ return confirm("¿Desea cancelar la operación?"); }
        //Pregunta si se desea retornar a la pantalla anterior
        function preguntarRetornar(){ return confirm("¿Desea retornar a la pantalla anterior?"); }
        </script>
      </afh:head>
      <afh:body>
        <af:messages/>
        <af:form id="form1">
          <f:verbatim>
            <input type="hidden" name="hdn_tipo" id="hdn_tipo" value="27"/>
            <!--Tipo de Autorización (tipo 27 corresponde a Prestaciones Post-mortem)-->
            <input type="hidden" name="hdn_Id" id="hdn_Id" value="${bindings.IdIndemnizacion.inputValue}"/>
            <!--Id de las prestaciones post-mortem-->
            <jsp:include page="../util/dialog_autoriza.txt"/>
            <!--Ventana de diálogo para los traslados de la solicitud-->
          </f:verbatim>
          <af:page title="Prestaciones Post-mortem" value="#{modeloMenu.modeloBD}" var="node">
            <f:facet name="branding">
              <f:subview id="logo">
                <jsp:include page="/global/facet_branding.jspx"/>
              </f:subview>
            </f:facet>
            <f:facet name="infoUser">
              <f:subview id="infoUser">
                <jsp:include page="/global/facet_infoUser.jspx"/>
              </f:subview>
            </f:facet>
            <f:facet name="menuSwitch">
              <f:subview id="menuSwt">
                <jsp:include page="/global/facet_menuSwitch.jspx"/>
              </f:subview>
            </f:facet>
            <f:facet name="nodeStamp">
              <af:commandMenuItem text="#{node.label}" action="#{node.getOutcome}" actionListener="#{modeloMenu.listener_menu}" disabled="#{node.readOnly}" rendered="#{node.shown}"/>
            </f:facet>
            <af:panelBox width="100%" id="pnlBoxPrincipal">
              <af:panelGroup layout="vertical" inlineStyle="text-align:center;" rendered="#{!bindings.EsSolicitudNueva.inputValue}">
                <af:outputFormatted value="INFORMACIÓN GENERAL DEL EXPEDIENTE" inlineStyle="font-weight:bold; color:rgb(0,0,128); font-size:medium;"/>
                <af:objectSeparator/>
                <af:panelHorizontal halign="center" inlineStyle="padding:2.0pt;">
                  <af:panelLabelAndMessage label="No. de expediente:" inlineStyle="font-weight:bold;">
                    <af:outputText value="#{bindings.CorrelativoAnio.inputValue}-#{bindings.Anio.inputValue}-#{bindings.RegistroEmpleado.inputValue}"
                                   inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128); font-size:small;"/>
                  </af:panelLabelAndMessage>
                  <af:panelLabelAndMessage label="Estado actual:" inlineStyle="font-weight:bold;">
                    <af:outputText inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128); font-size:small;" value="#{bindings.NombreEstadoIndemnizacion.inputValue}"/>
                  </af:panelLabelAndMessage>
                  <af:panelLabelAndMessage label="ID Interno:" inlineStyle="font-weight:bold;">
                    <af:outputText inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128); font-size:small;" value="#{bindings.IdIndemnizacion.inputValue}">
                      <f:convertNumber groupingUsed="false"/>
                    </af:outputText>
                  </af:panelLabelAndMessage>
                  <f:facet name="separator">
                    <af:objectSpacer height="10" width="10"/>
                  </f:facet>
                </af:panelHorizontal>
                <af:panelHorizontal halign="center" inlineStyle="padding:2.0pt;">
                  <af:panelLabelAndMessage label="Estado del trabajador:" inlineStyle="font-weight:bold;">
                    <af:outputText value="#{bindings.NombreEstadoTrabajador.inputValue}" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128); font-size:small;"/>
                  </af:panelLabelAndMessage>
                  <af:panelLabelAndMessage label="No. de Solicitud de fallecimiento:" inlineStyle="font-weight:bold;">
                    <af:outputText value="#{bindings.IdSolicitudFallecimiento.inputValue}" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128); font-size:small;">
                      <f:convertNumber groupingUsed="false"/>
                    </af:outputText>
                  </af:panelLabelAndMessage>
                  <af:panelLabelAndMessage label="Fecha:" inlineStyle="font-weight:bold;">
                    <af:outputText value="#{bindings.FechaSolicitudFallecimiento.inputValue}" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128); font-size:small;">
                      <f:convertDateTime pattern="dd/MM/yyyy"/>
                    </af:outputText>
                  </af:panelLabelAndMessage>
                  <f:facet name="separator">
                    <af:objectSpacer height="10" width="10"/>
                  </f:facet>
                </af:panelHorizontal>
                <af:panelHorizontal halign="center" inlineStyle="padding:2.0pt;">
                  <af:outputText inlineStyle="font-weight:bold; font-size:small;" value="Observación Fallecimiento:"/>
                  <af:objectSpacer height="10" width="10"/>
                  <af:panelBox width="700px" inlineStyle="text-align:justify;">
                    <af:outputText value="#{bindings.ObservacionFallecimiento.inputValue}" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128); font-size:small;" rendered="true"/>
                  </af:panelBox>
                </af:panelHorizontal>
              </af:panelGroup>
              <af:panelBox width="100%">
                <af:panelForm shortDesc="Panel de información del expediente del trabajador fallecido">
                  <af:panelHorizontal halign="center" rendered="#{bindings.EsSolicitudNueva.inputValue}">
                    <af:outputFormatted value="NUEVO EXPEDIENTE" inlineStyle="font-size:medium; font-weight:bold; color:rgb(0,0,132);"/>
                  </af:panelHorizontal>
                  <af:panelHorizontal halign="center">
                    <af:outputFormatted value="Las casillas con * son requeridos" inlineStyle="color:rgb(128,0,0); font-weight:bold;"/>
                  </af:panelHorizontal>
                  <af:panelHorizontal halign="center">
                    <f:facet name="separator">
                      <af:objectSpacer height="10" width="10"/>
                    </f:facet>
                    <af:commandLink partialSubmit="true" id="cdmLink_buscarTrabajador" binding="#{Postmortem_detalle.cdmLink_buscarTrabajador}" useWindow="true" windowHeight="500" windowWidth="800"
                                    shortDesc="Buscador de trabajador" action="dialog:buscaEmpl" returnListener="#{Postmortem_detalle.cdmLink_buscarTrabajador_returnListener}" immediate="true"
                                    rendered='#{bindings.EsSolicitudNueva.inputValue || (bindings.NombreEstadoIndemnizacion.inputValue!=null &amp;&amp; bindings.NombreEstadoIndemnizacion.inputValue eq "GRABADO")}'>
                      <af:goButton text="Buscar Trabajador" inlineStyle="font-weight:bold; color:rgb(0,0,132); font-size:large;"/>
                    </af:commandLink>
                    <af:outputText inlineStyle="font-weight:bold; color:rgb(128,0,0);" value="*"/>
                    <af:outputText inlineStyle="font-weight:bold;" value="Registro de Personal:"/>
                    <af:panelForm inlineStyle="border-color:rgb(0,0,0); border-style:solid; border-width:thin;" width="75">
                      <af:panelHorizontal halign="center">
                        <af:inputText value="#{bindings.RegistroEmpleado.inputValue}" id="inptText_registroEmpleado" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);"
                                      partialTriggers="cdmLink_buscarTrabajador" binding="#{Postmortem_detalle.inptText_registroEmpleado}" readOnly="true" shortDesc="Registro personal del trabajador">
                          <f:convertNumber groupingUsed="false"/>
                        </af:inputText>
                        <af:objectSpacer height="10" width="10"/>
                        <af:objectSpacer height="10" width="10"/>
                      </af:panelHorizontal>
                    </af:panelForm>
                    <af:outputText value="*" inlineStyle="font-weight:bold; color:rgb(128,0,0);"/>
                    <af:outputText value="Nombre del Trabajador" inlineStyle="font-weight:bold;"/>
                    <af:panelForm width="125" inlineStyle="border-color:rgb(0,0,0); border-style:solid; border-width:thin;">
                      <af:panelHorizontal halign="center">
                        <af:inputText inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);" id="inptText_nombreCompleto" partialTriggers="cdmLink_buscarTrabajador"
                                      binding="#{Postmortem_detalle.inptText_nombreCompleto}" value="#{bindings.Nombrecompleto.inputValue}" readOnly="true" shortDesc="Nombre completo del trabajador"/>
                        <af:objectSpacer height="10" width="10"/>
                        <af:objectSpacer height="10" width="10"/>
                      </af:panelHorizontal>
                    </af:panelForm>
                    <af:outputText inlineStyle="font-weight:bold;" value="CUI:"/>
                    <af:panelForm width="75" inlineStyle="border-color:rgb(0,0,0); border-style:solid; border-width:thin;">
                      <af:panelHorizontal halign="center">
                        <af:inputText id="inptText_cuiTrabajador" value="#{bindings.Cui.inputValue}" binding="#{Postmortem_detalle.inptText_cuiTrabajador}" partialTriggers="cdmLink_buscarTrabajador"
                                      readOnly="true" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);" shortDesc="CUI del trabajador"/>
                        <af:objectSpacer height="10" width="10"/>
                        <af:objectSpacer height="10" width="10"/>
                      </af:panelHorizontal>
                    </af:panelForm>
                  </af:panelHorizontal>
                  <af:panelHorizontal halign="center">
                    <f:facet name="separator">
                      <af:objectSpacer height="10" width="10"/>
                    </f:facet>
                    <af:outputText value="*" inlineStyle="font-weight:bold; color:rgb(128,0,0);"/>
                    <af:outputText value="Tiempo de Servicio (para cálculo de Prestaciones):" inlineStyle="font-weight:bold;"/>
                    <af:outputText value="Años:" inlineStyle="font-weight:bold;"/>
                    <af:inputText value="#{bindings.AniosTiempoServicio.inputValue}" columns="3" id="inptText_AniosServicio" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);"
                                  maximumLength="2" shortDesc="Años de Servicio (Máximo 12)"
                                  readOnly='#{!bindings.EsSolicitudNueva.inputValue &amp;&amp; (bindings.NombreEstadoIndemnizacion.inputValue eq "GRABADO"?false:true)}'
                                  binding="#{Postmortem_detalle.inptText_AniosServicio}">
                      <f:convertNumber groupingUsed="false" pattern="###"/>
                    </af:inputText>
                    <af:outputText value="Meses:" inlineStyle="font-weight:bold;"/>
                    <af:inputText value="#{bindings.MesesTiempoServicio.inputValue}" id="inptText_MesesServicio" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);" columns="3"
                                  maximumLength="2" shortDesc="Meses de Servicio (Máximo 11)"
                                  readOnly='#{!bindings.EsSolicitudNueva.inputValue &amp;&amp; (bindings.NombreEstadoIndemnizacion.inputValue eq "GRABADO"?false:true)}'
                                  binding="#{Postmortem_detalle.inptText_MesesServicio}">
                      <f:convertNumber groupingUsed="false" pattern="###"/>
                    </af:inputText>
                    <af:outputText value="Días:" inlineStyle="font-weight:bold;"/>
                    <af:inputText value="#{bindings.DiasTiempoServicio.inputValue}" columns="3" id="inptText_DiasServicio" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);"
                                  maximumLength="2" shortDesc="Días de Servicio (Máximo 29)"
                                  readOnly='#{!bindings.EsSolicitudNueva.inputValue &amp;&amp; (bindings.NombreEstadoIndemnizacion.inputValue eq "GRABADO"?false:true)}'
                                  binding="#{Postmortem_detalle.inptText_DiasServicio}">
                      <f:convertNumber groupingUsed="false" pattern="###"/>
                    </af:inputText>
                  </af:panelHorizontal>
                  <af:panelHorizontal halign="center">
                    <af:outputText value="*" inlineStyle="font-weight:bold; color:rgb(128,0,0);"/>
                    <af:outputText value="Sueldo Promedio (para cálculo de Prestaciones):" inlineStyle="font-weight:bold;"/>
                    <af:inputText id="inptText_sueldoPromedio" value="#{bindings.SueldoPromedio.inputValue}" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);" columns="10"
                                  readOnly='#{!bindings.EsSolicitudNueva.inputValue &amp;&amp; (bindings.NombreEstadoIndemnizacion.inputValue eq "GRABADO"?false:true)}'
                                  binding="#{Postmortem_detalle.inptText_sueldoPromedio}">
                      <f:convertNumber groupingUsed="true" maxFractionDigits="2" minIntegerDigits="1" pattern="###,###.##" minFractionDigits="2"/>
                    </af:inputText>
                    <af:outputText value="Fecha de la Solicitud:" inlineStyle="font-weight:bold;"/>
                    <af:selectInputDate id="slctInputDate_fechaSolicitud" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);" shortDesc="Fecha de la Solicitud"
                                        value="#{bindings.FechaSolicitud.inputValue}" required="true"
                                        readOnly='#{!bindings.EsSolicitudNueva.inputValue &amp;&amp; (bindings.NombreEstadoIndemnizacion.inputValue eq "GRABADO"?false:true)}'
                                        binding="#{Postmortem_detalle.slctInputDate_fechaSolicitud}">
                      <af:convertDateTime pattern="dd/MM/yyyy"/>
                    </af:selectInputDate>
                    <f:facet name="separator">
                      <af:objectSpacer height="10" width="10"/>
                    </f:facet>
                  </af:panelHorizontal>
                  <af:panelHorizontal halign="center" rendered="#{!bindings.EsSolicitudNueva.inputValue}">
                    <af:table value="#{bindings.tblBeneficiarios.collectionModel}" var="row" rows="#{bindings.tblBeneficiarios.rangeSize}" first="#{bindings.tblBeneficiarios.rangeStart}"
                              emptyText="#{bindings.tblBeneficiarios.viewable ? 'No hay información para mostrár aún.' : 'Access Denied.'}" selectionState="#{bindings.tblBeneficiarios.collectionModel.selectedRow}"
                              selectionListener="#{bindings.tblBeneficiarios.collectionModel.makeCurrent}" banding="row" id="tbl_beneficiarios_fallecido" width="100%"
                              shortDesc="Listado de beneficiarios del trabajador fallecido" binding="#{Postmortem_detalle.tbl_beneficiarios_fallecido}">
                      <f:facet name="selection">
                        <af:tableSelectOne text="Seleccione un beneficiario que solicita las prestaciones y guardar ">
                          <af:commandButton text="Guardar"/>
                        </af:tableSelectOne>
                      </f:facet>
                      <af:column sortProperty="NombreCompleto" sortable="false" headerText="Nombre Completo">
                        <af:outputText value="#{row.NombreCompleto}" inlineStyle="padding:3.0pt;"/>
                      </af:column>
                      <af:column sortProperty="Cui" sortable="false" headerText="CUI" formatType="icon">
                        <af:outputText value="#{row.Cui}" inlineStyle="padding:3.0pt;"/>
                      </af:column>
                      <af:column sortProperty="Parentesco" sortable="false" headerText="Parentesco" formatType="icon">
                        <af:outputText value="#{row.Parentesco}" inlineStyle="padding:3.0pt;"/>
                      </af:column>
                      <af:column sortProperty="CreadoPor" sortable="false" headerText="Ingresado Por" formatType="icon">
                        <af:outputText value="#{row.CreadoPor}" inlineStyle="padding:3.0pt;"/>
                      </af:column>
                      <af:column sortProperty="FechaCreacion" sortable="false" headerText="Fecha Ingresado" formatType="icon">
                        <af:outputText value="#{row.FechaCreacion}" inlineStyle="padding:3.0pt;">
                          <f:convertDateTime pattern="#{bindings.tblBeneficiarios.formats.FechaCreacion}"/>
                        </af:outputText>
                      </af:column>
                      <af:column sortProperty="IdBeneficiario" sortable="false" headerText="ID Interno" formatType="icon">
                        <af:outputText value="#{row.IdBeneficiario}" inlineStyle="padding:3.0pt;">
                          <f:convertNumber groupingUsed="false" pattern="#{bindings.tblBeneficiarios.formats.IdBeneficiario}"/>
                        </af:outputText>
                      </af:column>
                      <af:column sortProperty="IdSolicitud" sortable="false" headerText="#{bindings.tblBeneficiarios.labels.IdSolicitud}" rendered="false">
                        <af:outputText value="#{row.IdSolicitud}">
                          <f:convertNumber groupingUsed="false" pattern="#{bindings.tblBeneficiarios.formats.IdSolicitud}"/>
                        </af:outputText>
                      </af:column>
                      <af:column sortProperty="IdParentesco" sortable="false" headerText="#{bindings.tblBeneficiarios.labels.IdParentesco}" rendered="false">
                        <af:outputText value="#{row.IdParentesco}">
                          <f:convertNumber groupingUsed="false" pattern="#{bindings.tblBeneficiarios.formats.IdParentesco}"/>
                        </af:outputText>
                      </af:column>
                      <af:column sortProperty="Nombre1" sortable="false" headerText="#{bindings.tblBeneficiarios.labels.Nombre1}" rendered="false">
                        <af:outputText value="#{row.Nombre1}"/>
                      </af:column>
                      <af:column sortProperty="Nombre2" sortable="false" headerText="#{bindings.tblBeneficiarios.labels.Nombre2}" rendered="false">
                        <af:outputText value="#{row.Nombre2}"/>
                      </af:column>
                      <af:column sortProperty="Nombre3" sortable="false" headerText="#{bindings.tblBeneficiarios.labels.Nombre3}" rendered="false">
                        <af:outputText value="#{row.Nombre3}"/>
                      </af:column>
                      <af:column sortProperty="Apellido1" sortable="false" headerText="#{bindings.tblBeneficiarios.labels.Apellido1}" rendered="false">
                        <af:outputText value="#{row.Apellido1}"/>
                      </af:column>
                      <af:column sortProperty="Apellido2" sortable="false" headerText="#{bindings.tblBeneficiarios.labels.Apellido2}" rendered="false">
                        <af:outputText value="#{row.Apellido2}"/>
                      </af:column>
                      <f:facet name="header">
                        <af:outputText value="Beneficiarios del Fallecido"/>
                      </f:facet>
                    </af:table>
                    <af:objectSpacer height="10" width="20"/>
                    <af:outputText value="Gasto Funerario Q." inlineStyle="font-weight:bold;"/>
                    <af:objectSpacer height="10" width="10"/>
                    <af:inputText columns="10" value="#{bindings.GastoFunerario.inputValue}" inlineStyle="font-weight:bold; text-align:center; color:rgb(0,0,128);" id="inptText_gastoFunerario"
                                  binding="#{Postmortem_detalle.inptText_gastoFunerario}">
                      <f:convertNumber maxFractionDigits="2" minIntegerDigits="1" pattern="###,###.##" minFractionDigits="2"/>
                    </af:inputText>
                  </af:panelHorizontal>
                  <af:panelGroup layout="vertical" inlineStyle="text-align:center;" rendered="#{!bindings.EsSolicitudNueva.inputValue &amp;&amp; bindings.TotalPrestacion.inputValue != null}">
                    <af:objectSpacer height="10" width="10"/>
                    <af:outputFormatted value="PRESTACIONES A LIQUIDAR" inlineStyle="font-weight:bold; color:rgb(0,0,128); font-size:medium;"/>
                    <af:objectSeparator/>
                    <af:panelHorizontal halign="center" inlineStyle="padding:2.0pt;">
                      <af:outputText value="Sueldo Promedio:" inlineStyle="color:rgb(0,0,128); font-weight:bold;"/>
                      <af:inputText value="#{bindings.SueldoPromedioLectura.inputValue}" readOnly="true" inlineStyle="font-size:medium; font-weight:bold; text-align:center;">
                        <f:convertNumber maxFractionDigits="2" type="currency"/>
                      </af:inputText>
                      <af:outputText value="Sueldo Promedio * Años Laborados:" inlineStyle="color:rgb(0,0,128); font-weight:bold;"/>
                      <af:inputText value="#{bindings.PrestacAnios.inputValue}" readOnly="true" inlineStyle="font-size:medium; font-weight:bold; text-align:center;">
                        <f:convertNumber maxFractionDigits="2" type="currency"/>
                      </af:inputText>
                      <f:facet name="separator">
                        <af:objectSpacer height="10" width="10"/>
                      </f:facet>
                    </af:panelHorizontal>
                    <af:panelHorizontal halign="center" inlineStyle="padding:2.0pt;">
                      <af:outputText value="Sueldo Promedio * Meses Laborados:" inlineStyle="color:rgb(0,0,128); font-weight:bold;"/>
                      <af:inputText value="#{bindings.PrestacMeses.inputValue}" readOnly="true" inlineStyle="font-size:medium; font-weight:bold; text-align:center;">
                        <f:convertNumber maxFractionDigits="2" type="currency"/>
                      </af:inputText>
                      <af:outputText value="Sueldo Promedio * Días Laborados:" inlineStyle="color:rgb(0,0,128); font-weight:bold;"/>
                      <af:inputText value="#{bindings.PrestacDias.inputValue}" readOnly="true" inlineStyle="font-size:medium; font-weight:bold; text-align:center;">
                        <f:convertNumber maxFractionDigits="2" type="currency"/>
                      </af:inputText>
                      <f:facet name="separator">
                        <af:objectSpacer height="10" width="10"/>
                      </f:facet>
                    </af:panelHorizontal>
                    <af:panelHorizontal halign="center" inlineStyle="padding:2.0pt;">
                      <af:outputText value="Gasto Funerario:" inlineStyle="color:rgb(0,0,128); font-weight:bold;"/>
                      <af:inputText value="#{bindings.GastoFunerarioLectura.inputValue}" readOnly="true" inlineStyle="font-size:medium; font-weight:bold; text-align:center;">
                        <f:convertNumber maxFractionDigits="2" type="currency"/>
                      </af:inputText>
                      <af:outputText value="Total Prestaciones a Liquidar:" inlineStyle="color:rgb(0,0,128); font-weight:bold;"/>
                      <af:inputText value="#{bindings.TotalPrestacion.inputValue}" readOnly="true" inlineStyle="font-size:medium; font-weight:bold; text-align:center;">
                        <f:convertNumber maxFractionDigits="2" type="currency"/>
                      </af:inputText>
                      <f:facet name="separator">
                        <af:objectSpacer height="10" width="10"/>
                      </f:facet>
                    </af:panelHorizontal>
                  </af:panelGroup>
                  <af:panelHorizontal halign="center">
                    <af:commandButton text="Guardar" inlineStyle="font-weight:bold; font-size:large;" id="cmdBtn_guardar" binding="#{Postmortem_detalle.cmdBtn_guardar}"
                                      action="#{Postmortem_detalle.cmdBtn_guardar_action}" onclick="return preguntarGuardar();" shortDesc="Guardar Cambios"/>
                    <af:commandButton text="Cancelar" immediate="true" inlineStyle="font-weight:bold; font-size:large;" id="cmdBtn_cancelar" binding="#{Postmortem_detalle.cmdBtn_cancelar}"
                                      action="#{Postmortem_detalle.cmdBtn_cancelar_action}" onclick="return preguntarCancelar();" shortDesc="Cancelar Cambios">
                      <af:resetActionListener/>
                    </af:commandButton>
                    <af:commandButton text="Retornar" shortDesc="Retornar a la pantalla anterior" inlineStyle="font-weight:bold; font-size:large;" immediate="true" id="cmdBtn_retornar"
                                      binding="#{Postmortem_detalle.cmdBtn_retornar}" action="#{Postmortem_detalle.cmdBtn_retornar_action}" actionListener="#{bindings.Rollback.execute}"
