                            <f:convertNumber groupingUsed="false"
                                             pattern="#{bindings.Gtosmedicos1.format}"/>
                          </af:inputText>
                        </afh:cellFormat>
                      </afh:rowLayout>
                      <afh:rowLayout rendered="#{bindings.shwOpt.inputValue eq '1'}">
