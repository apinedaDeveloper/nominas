      <script language="javascript" type="text/javascript"> 
        function genRep(pAnio){ 
                submitForm('form1',1,{source:'form1:page1:btn_generarRep'});
                window.open("/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+pAnio+String.fromCharCode(38)+"rep=DECJURADPAT","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
       } 
       
       function genReporteIsr(vAnio){               
                window.open("/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+vAnio+String.fromCharCode(38)+"rep=REPPAG_ISR","preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
       } 
      
       function genSAT1921(vAnio){    
       
             var lista=document.getElementById('form1:page1:cmb_consisr_a');
             var indice=lista.selectedIndex;
             var vAnio1 = lista.options[indice].value; 
             
             lista=document.getElementById('form1:page1:cmb_consisr_m');
             indice=lista.selectedIndex;
             var vMes = lista.options[indice].value;
             
       
              window.open("/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+vAnio1+String.fromCharCode(38)+"rep=SAT1921USC"+String.fromCharCode(38)+"id2="+vMes,"preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
       } 
       
       function genOtrasRentas(vRegPer){

             var lista=document.getElementById('form1:page1:cmb_consisr_a');
             var indice=lista.selectedIndex;
             var vAnio1 = lista.options[indice].value; 
             
             lista=document.getElementById('form1:page1:cmb_consisr_m');
             indice=lista.selectedIndex;
             var vMes = lista.options[indice].value;
             
       
             window.open("/${flujo_usuario.dominioApp}/faces/verreportepdf?id="+vAnio1+String.fromCharCode(38)+"rep=OTRASRENTAS"+String.fromCharCode(38)+"id2="+vMes+String.fromCharCode(38)+"id3="+vRegPer,"preview","width=640,height=640,scrollbars=YES,location=NO,directories=NO,resizable =1"); 
       
       }

       
