                                          valuePassThru="true"
                                          value="1">
                        <af:selectItem value="1" label="ENERO"/>
                        <af:selectItem value="2" label="FEBRERO"/>
                        <af:selectItem value="3" label="MARZO"/>
                        <af:selectItem value="4" label="ABRIL"/>
                        <af:selectItem value="5" label="MAYO"/>
                        <af:selectItem value="6" label="JUNIO"/>
                        <af:selectItem value="7" label="JULIO"/>
                        <af:selectItem value="8" label="AGOSTO"/>
                        <af:selectItem value="9" label="SEPTIEMBRE"/>
                        <af:selectItem value="10" label="OCTUBRE"/>
                        <af:selectItem value="11" label="NOVIEMBRE"/>
                        <af:selectItem value="12" label="DICIEMBRE"/>
                      </af:selectOneChoice>
                      <af:objectSpacer width="10" height="10"/>
                      <af:commandButton text="Generar Constancia"
                                        onclick="genSAT1921('#{bindings.pAnio1.inputValue}');"/>
                      <af:objectSpacer width="10" height="10"/>
                      <af:commandButton text="Generar Detalle Otras Rentas"
                                        onclick="genOtrasRentas('#{flujo_usuario.selRegEmpleado_login}');"
                                        rendered="true"/>
                    </af:panelForm>
                    <af:objectSpacer width="10" height="50"/>
                  </af:panelBox>
                </af:showDetailItem>
              </af:showOneTab>
            </af:panelBox>
          </af:page>
