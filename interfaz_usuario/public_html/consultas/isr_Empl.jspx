                                        columns="10"
                                        inlineStyle="text-align:right;"
                                        label="Q.">
                            <af:validator binding="#{bindings.PensionesAlimen.validator}"/>
                            <f:convertNumber groupingUsed="false"
                                             pattern="#{bindings.PensionesAlimen.format}"/>
                          </af:inputText>
                        </afh:cellFormat>
                      </afh:rowLayout>
                      <afh:rowLayout>
