    <af:outputFormatted value="#{flujo_usuario.usuarioLogueado}"
                        styleUsage="instruction"
                        inlineStyle="color:rgb(0,107,165);"/>
    <af:outputFormatted value="&lt;b>Rol:&lt;/b>#{flujo_usuario.perfilesEmpleadoLogin}"
                        styleUsage="instruction" rendered="false"/>
  </af:panelGroup>
</jsp:root>
