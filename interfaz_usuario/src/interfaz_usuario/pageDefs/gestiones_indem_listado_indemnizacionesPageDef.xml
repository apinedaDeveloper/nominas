<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="10.1.3.42.70" id="gestiones_indem_listado_indemnizacionesPageDef" Package="interfaz_usuario.pageDefs">
  <executables>
    <iterator id="SisIndemnizacion_vwIterator" Binds="SisIndemnizacion_vw" RangeSize="30" DataControl="QUERRYSDataControl" StateValidation="false">
      <sortCriteria>
        <sort attribute="Anio" ascending="false"/>
        <sort attribute="CorrelativoAnio" ascending="false"/>
      </sortCriteria>
    </iterator>
    <invokeAction Binds="Rollback" id="RollBackInicio" Refresh="renderModel" RefreshCondition="${!adfFacesContext.postback}"/>
    <invokeAction Binds="ObtenerSolicitudes" id="Invoke_obtenerSolicitudes"
                  RefreshCondition="${!adfFacesContext.postback &amp;&amp; !(roles_usuario.profesionalCaja || roles_usuario.auditorIndemnizacion) }" Refresh="renderModel"/>
    <invokeAction id="Invoke_obtenerSolicitudesProfCaja" Binds="ObtenerSolicitudesProfesionalCaja" Refresh="renderModel"
                  RefreshCondition="${!adfFacesContext.postback &amp;&amp; roles_usuario.profesionalCaja}"/>
    <invokeAction Binds="ObtenerSolicitudesAuditor" id="Invoke_obtenerSolicitudesAuditor" Refresh="renderModel"
                  RefreshCondition="${!adfFacesContext.postback &amp;&amp; roles_usuario.auditorIndemnizacion}"/>
  </executables>
  <bindings>
    <table IterBinding="SisIndemnizacion_vwIterator" id="Tbl_Solicitudes">
      <AttrNames>
        <Item Value="IdIndemnizacion"/>
        <Item Value="Anio"/>
        <Item Value="CorrelativoAnio"/>
        <Item Value="CreadoPor"/>
        <Item Value="Cui"/>
        <Item Value="FechaCreacion"/>
        <Item Value="FechaSolicitud"/>
        <Item Value="IdEstado"/>
        <Item Value="IdTipoPrestacion"/>
        <Item Value="Nombrecompleto"/>
        <Item Value="NombreEstadoIndemnizacion"/>
        <Item Value="NombreTipoIndemnizacion"/>
        <Item Value="RegistroEmpleado"/>
        <Item Value="TipoRetiro"/>
        <Item Value="TipoRetiroNombre"/>
        <Item Value="TotalPrestacion"/>
      </AttrNames>
    </table>
    <methodAction IterBinding="SisIndemnizacion_vwIterator" id="ObtenerSolicitudes" MethodName="ObtenerSolicitudesIndemnizacion" RequiresUpdateModel="true" Action="999" IsViewObjectMethod="true" DataControl="QUERRYSDataControl"
                  InstanceName="QUERRYSDataControl.SisIndemnizacion_vw"/>
    <methodAction IterBinding="SisIndemnizacion_vwIterator" id="ObtenerSolicitudesAuditor" MethodName="ObtenerSolicitudesAuditor" RequiresUpdateModel="true" Action="999" IsViewObjectMethod="true"
                  DataControl="QUERRYSDataControl" InstanceName="QUERRYSDataControl.SisIndemnizacion_vw"/>
    <methodAction IterBinding="SisIndemnizacion_vwIterator" id="ObtenerSolicitudesProfesionalCaja" MethodName="ObtenerSolicitudesProfesionalCaja" RequiresUpdateModel="true" Action="999"
                  IsViewObjectMethod="true" DataControl="QUERRYSDataControl" InstanceName="QUERRYSDataControl.SisIndemnizacion_vw"/>
    <action id="Rollback" InstanceName="QUERRYSDataControl" DataControl="QUERRYSDataControl" RequiresUpdateModel="false" Action="101"/>
  </bindings>
</pageDefinition>
