package interfaz_usuario.backing.util;
import oracle.jbo.domain.Date;
public class interrupcion_laboral {
    private String vTipoInterrupcion;
    private oracle.jbo.domain.Date vFechaI;
    private oracle.jbo.domain.Date vFechaF;
    private boolean selected;

    public interrupcion_laboral() {
    }

    public void setTipoInterrupcion(String vTipoInterrupcion) {
        this.vTipoInterrupcion = vTipoInterrupcion;
    }

    public String getTipoInterrupcion() {
        return vTipoInterrupcion;
    }

    public void setFechaI(Date vFechaI) {
        this.vFechaI = vFechaI;
    }

    public Date getFechaI() {
        return vFechaI;
    }
    public void setFechaF(Date vFechaF) {
        this.vFechaF = vFechaF;
    }

    public Date getFechaF() {
        return vFechaF;
    }

    public void setSelected(boolean selected) {
        this.selected = selected;
    }

    public boolean isSelected() {
        return selected;
    }
}
