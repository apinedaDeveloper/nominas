    LEFT JOIN SIS_PUESTO SP ON SC.ID_PUESTO=SP.ID_PUESTO
    LEFT JOIN SIS_EMPLEADO SES ON SC.REGISTRO_EMPLEADO=SES.REGISTRO_EMPLEADO
