	<queryString><![CDATA[SELECT
sis_tip_contrat.nombre as nomTipoAcuerdo,
decode(sis_tip_contrat.nombre,'CONTRATO','CONTRATAR','NOMBRAMIENTO','NOMBRAR',sis_tip_contrat.nombre) tipo_acuerdo,
SIS_PARTIDA_VW.FORMATO2 AS partida,
sis_contrato.id_partida as id_partida,
to_char(SIS_CONTRATO.FECHA_CREACION,'DD/MM/YYYY') AS fecha_creacion,
to_char(nvl(sis_contrato.num_contrato_fisico,SIS_CONTRATO.ID_CONTRATO),'09G9999G9999','NLS_NUMERIC_CHARACTERS= '',-''') AS id_contrato_formateado ,
(select sis_pkg_util.GETNUMERO_TO_LETRAS(SIS_CONTRATO.SUELDOBASE+nvl(sis_contrato.escalaf,0)+nvl(sis_contrato.compl_sal,0),1) from dual) AS salario_letras,
(select nombre from sis_municipio where id_municipio=SIS_CONTRATO.LUGAR_POSESION) as lugar_posesion,
(MUNINOMBRA.NOMBRE)||', '|| trim(to_char( nvl(sis_contrato.fecha_firma,sis_pkg_util.GETFECHAFIRMACONTRATO(sis_contrato.id_contrato)),'dd" de" Month','NLS_DATE_LANGUAGE = SPANISH'))||' de ' ||to_char(sis_contrato.fecha_firma,'yyyy')  AS lugar_y_fecha_posecion,
(select nombre from sis_departamento where id_departamento = (select id_departamento from sis_municipio where id_municipio=SIS_CONTRATO.LUGAR_POSESION)) AS departamento_posesion,
case when to_char(SIS_CONTRATO.FECHA_POSESION,'MI')>0 
  then sis_pkg_util.getnumero_entero_to_letras(to_char(SIS_CONTRATO.FECHA_POSESION,'HH24'))||' CON ' ||sis_pkg_util.getnumero_entero_to_letras(to_char(SIS_CONTRATO.FECHA_POSESION,'MI')) else 
sis_pkg_util.getnumero_entero_to_letras(to_char(SIS_CONTRATO.FECHA_POSESION,'HH24')) end AS hora_posesion,
to_char(SIS_CONTRATO.FECHA_POSESION,'HH24:MI') AS hora_posesion2,
sis_pkg_util.getnumero_entero_to_letras(to_char(SIS_CONTRATO.FECHA_POSESION,'DD')) AS dia_posesion,
sis_pkg_util.getnumero_mes(to_char(SIS_CONTRATO.FECHA_POSESION,'MM')) as mes_posesion,
sis_pkg_util.getnumero_entero_to_letras(to_char(SIS_CONTRATO.FECHA_POSESION,'YYYY')) AS anho_posesion,
SIS_CONTRATO.ID_PARTIDA AS partida,
SIS_CONTRATO.NUMERO_PLAZA AS plaza,
SIS_EMPLEADO1.REGISTRO_EMPLEADO AS SIS_EMPLEADO_REGISTRO_EMPLEADO,
SIS_CONTRATO.ID_CONTRATO AS SIS_CONTRATO_ID_CONTRATO,
SIS_CONTRATO.REGISTRO_EMPLEADO AS SIS_CONTRATO_REGISTRO_EMPLEADO,
SIS_EMPLEADO1.NOMBRE1 AS SIS_EMPLEADO_NOMBRE1,
SIS_EMPLEADO1.APELLIDO1 AS SIS_EMPLEADO_APELLIDO1,
SIS_EMPLEADO1.TITULO AS SIS_EMPLEADO_TITULO,
SIS_EMPLEADO1.COLEGIADO AS SIS_EMPLEADO_COLEGIADO,
SIS_EMPLEADO1.ORDEN_CEDULA AS SIS_EMPLEADO_ORDEN_CEDULA,
SIS_EMPLEADO1.REGISTRO_CEDULA AS SIS_EMPLEADO_REGISTRO_CEDULA,
(SELECT SIS_MUNICIPIO.NOMBRE FROM SIS_MUNICIPIO WHERE  SIS_MUNICIPIO.ID_MUNICIPIO=SIS_EMPLEADO1.MUNICIPIO_CEDULA) AS MUNICIPIO_CEDULA,
SIS_PLAZA.ID_PUESTO AS ID_PUSETO,
SIS_CONTRATO.HORASCONTRATADAS AS HORASCONTRATADAS,
SIS_DEPARTAMENTO.NOMBRE AS SIS_DEPARTAMENTO_NOMBRE,
SIS_DEPARTAMENTO.NOMBRE AS SIS_DEPARTAMENTO_A_NOMBRE,
decode(SIS_EMPLEADO1.ESTADOCIVIL,'C','CASADO (a)','S','SOLTERO (a)')as estadocivil,
(SIS_DEPARTAMENTO.nombre||', '||SIS_MUNICIPIO.NOMBRE||' '||SIS_DIRECCION.DIRECCION) as SIS_DIRECCION,
floor(months_between(nvl(sis_contrato.fecha_inicio_doc,sis_contrato.fecha_inicio),Sis_Empleado1.FECHA_NACIMIENTO)/12) as SIS_EMPLEADO_EDAD,
SIS_EMPLEADO1.ESTADOCIVIL AS SIS_EMPLEADO_ESTADOCIVIL,
SIS_EMPLEADO1.NOMBRE2 AS SIS_EMPLEADO_NOMBRE2,
SIS_EMPLEADO1.NOMBRE3 AS SIS_EMPLEADO_NOMBRE3,
SIS_EMPLEADO1.NOMBRE1||' '||SIS_EMPLEADO1.NOMBRE2||' '||SIS_EMPLEADO1.NOMBRE3 AS EMPLEADO_NOMBRES,
SIS_EMPLEADO1.APELLIDO2 AS SIS_EMPLEADO_APELLIDO2,
SIS_EMPLEADO1.APELLIDOCASADA AS SIS_EMPLEADO_APELLIDOCASADA,
sis_PAIS.NACIONALIDAD AS SIS_PAIS_A_NACIONALIDAD,
SIS_NIVEL_ACADEMICO.DESCRIPCION AS SIS_NIVEL_ACADEMICO_A_DESCRIPC,
SIS_DEPENDENCIA.NOMBRE AS SIS_DEPENDENCIA_NOMBRE,
SIS_UNIDAD_EJECUTORA.NOMBRE AS NOMBRE_UNIDAD,
SIS_NIVEL_ACADEMICO.DESCRIPCION AS SIS_NIVEL_ACADEMICO_DESCRIPCIO,
case when  sis_contrato.id_puesto in (999990,999997,999994,999999) 
     then case when sis_contrato.nom_plaza is not null then sis_contrato.nom_plaza when sis_plaza.nomplaza is not null then sis_plaza.nomplaza else SIS_PUESTO.NOMBRE 
end else SIS_PUESTO.NOMBRE end AS SIS_PUESTO_NOMBRE,
SIS_PLAZA.SALARIO_BASE AS SIS_PLAZA_SALARIO_BASE,
SIS_CONTRATO.SUELDOBASE AS SIS_CONTRATO_A_SUELDOBASE,
to_char(nvl(SIS_CONTRATO.FECHA_INICIO_DOC,SIS_CONTRATO.FECHA_INICIO),'DD/MM/YYYY') AS SIS_CONTRATO_A_FECHA_INICIO,
to_char(nvl(SIS_CONTRATO.FECHA_FIN_DOC,SIS_CONTRATO.FECHA_FIN),'DD/MM/YYYY') AS SIS_CONTRATO_A_FECHA_FIN,
SIS_CONTRATO.LUGAR_POSESION AS SIS_CONTRATO_A_LUGAR_POSESION,
SIS_CONTRATO.FECHA_POSESION AS SIS_CONTRATO_A_FECHA_POSESION,
to_char(SIS_CONTRATO.FECHA_POSESION,'DD') AS SIS_CONTRATO_dia,
SIS_CONTRATO.INSTALACION_POSESION AS SIS_CONTRATO_A_INSTALACION_POS,
SIS_FORMA_OBT_PUESTO.NOMBRE AS SIS_FORMA_OBT_PUESTO_NOMBRE,
SIS_EMPLEADO1.nombrecompleto AS nombresapellidos,
SIS_MUNICIPIO.NOMBRE AS SIS_MUNICIPIO_NOMBRE,
SIS_DEPARTAMENTO.NOMBRE AS SIS_DEPARTAMENTO_NOMBRE,
SIS_CONTRATO.ATRIBUCION AS ATRIBUCIONES,
nvl(sis_contrato.firma_jefe_unidad,(select nombre ||'|'|| cargo from sis_auth_contrato_datos_vw where id_auth_contrato=sis_pkg_util.getIdAuthContratoNom(sis_contrato.id_partida,sis_contrato.registro_empleado,1))) as auth_nombramiento,
nvl(sis_contrato.firma_jefe_subprog,(select nombre ||'|'|| cargo from sis_auth_contrato_datos_vw where id_auth_contrato=sis_pkg_util.getIdAuthContratoNom(sis_contrato.id_partida,sis_contrato.registro_empleado,2))) as auth_acta_toma_pos,
nvl(sis_contrato.firma_tesorero,(select nombre ||'|'|| cargo from sis_auth_contrato_datos_vw where id_auth_contrato=sis_pkg_util.getIdAuthContratoNom(sis_contrato.id_partida,sis_contrato.registro_empleado,3))) as auth_acta_tramite,
(SELECT NOMBRECOMPLETO FROM SIS_EMPLEADO_VW WHERE TO_CHAR(REGISTRO_EMPLEADO)=SIS_CONTRATO.CREADO_POR) AS CREADO_POR,
SIS_CONTRATO.base_legal,
SIS_CONTRATO.observ_formaobtpuesto|| decode(SIS_PLAZA.DICTAMEN,null,'',' .Dictamen: ' ||SIS_PLAZA.DICTAMEN) AS observ_formaobtpuesto,
Decode(SIS_CONTRATO.observHorario,null,SIS_CONTRATO.observHorario,', observaciones: '||SIS_CONTRATO.observHorario) observHorario,
nvl(sis_contrato.fecha_firma,sis_pkg_util.GETFECHAFIRMACONTRATO(sis_contrato.id_contrato)) as fechaFirma,
case when sis_empleado1.registro_empleado = 20211169 AND sis_PAIS.REGION <> 1 
     then sis_empleado1.no_pasaporte 
else to_char(sis_empleado1.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''')
end   as cui,
MUNINOMBRA.NOMBRE AS NOMNUMINOMBRA,
DBMS_LOB.SUBSTR(sis_contrato.atribucion_anexo, 4000, 1) as atribucion_anexo,
sis_contrato.tipo_validez,
sis_contrato.escalaf,
sis_contrato.compl_sal,
nvl(sis_contrato.escalaf,0)+nvl(sis_contrato.compl_sal,0)+nvl(sis_contrato.sueldobase,0) as nominal,
sis_empleado1.no_pasaporte,
SIS_EMPLEADO1.TITULO,
sis_contrato.no_exceda_De,
to_char(sis_contrato.no_exceda_De,'dd/mm/yyyy') as no_exceda_de2,
decode(nvl(sis_contrato.OPCION_INDEF,1),1,'Indefinido',2,'Indefinido Primer Semestre',3,'Indefinido Segundo Semestre') as opcion_indef,
sis_contrato.OPCION_INDEF as cod_opcion_indef,
nvl(SIS_PLAZA.PAGO_UNICO,0) as pago_unico,
(case when SIS_CONTRATO.FECHA_CREACION>=
(select to_date(valor_cadena,'dd/mm/yyyy') from sis_parametros_aplicacion where nom_parametro='SIN_CEDULA')
then 1 else 0 end) sin_cedula
, (select partida from SICOIN_PARTIDA_EGRESOS_VIEW where id_partida_egresos = sis_contrato.id_partida_sicoin) partida_sicoin
, SIS_PAIS.REGION
FROM
     SIS_EMPLEADO_vw  SIS_EMPLEADO1
     INNER JOIN SIS_CONTRATO SIS_CONTRATO ON SIS_EMPLEADO1.REGISTRO_EMPLEADO = SIS_CONTRATO.REGISTRO_EMPLEADO
     INNER JOIN SIS_DIRECCION SIS_DIRECCION ON SIS_EMPLEADO1.REGISTRO_EMPLEADO = SIS_DIRECCION.REGISTRO_EMPLEADO
        AND SIS_DIRECCION.ID_DIRECCION=(SELECT MIN(ID_DIRECCION) FROM SIS_DIRECCION WHERE REGISTRO_EMPLEADO=SIS_EMPLEADO1.REGISTRO_EMPLEADO)
     INNER JOIN SIS_MUNICIPIO SIS_MUNICIPIO ON SIS_MUNICIPIO.ID_MUNICIPIO=SIS_DIRECCION.ID_MUNICIPIO
     INNER JOIN SIS_NIVEL_ACADEMICO SIS_NIVEL_ACADEMICO ON SIS_EMPLEADO1.ID_NIVEL = SIS_NIVEL_ACADEMICO.ID_NIVEL
     INNER JOIN PAIS SIS_PAIS ON SIS_EMPLEADO1.ID_NACIONALIDAD = SIS_PAIS.codigo
     INNER JOIN SIS_PLAZA SIS_PLAZA ON
           SIS_CONTRATO.ID_PARTIDA = SIS_PLAZA.ID_PARTIDA
        AND SIS_PLAZA.NUMERO_PLAZA = SIS_CONTRATO.NUMERO_PLAZA
        AND SIS_PLAZA.CORRELATIVO = SIS_CONTRATO.CORRELATIVO
        AND SIS_PLAZA.ID_PUESTO = SIS_CONTRATO.ID_PUESTO
     INNER JOIN SIS_PARTIDA_VW SIS_PARTIDA_VW ON SIS_PLAZA.ID_PARTIDA = SIS_PARTIDA_VW.ID_PARTIDA
     INNER JOIN SIS_PUESTO SIS_PUESTO ON SIS_PLAZA.ID_PUESTO = SIS_PUESTO.ID_PUESTO
     INNER JOIN SIS_UNIDAD_EJECUTORA SIS_UNIDAD_EJECUTORA ON
            SIS_PARTIDA_VW.ID_PROGRAMA = SIS_UNIDAD_EJECUTORA.ID_PROGRAMA
        AND SIS_UNIDAD_EJECUTORA.ID_DEPENDENCIA = SIS_PARTIDA_VW.ID_DEPENDENCIA
        AND SIS_UNIDAD_EJECUTORA.ID_UNIDAD = SIS_PARTIDA_VW.ID_UNIDAD
     INNER JOIN SIS_MUNICIPIO MUNINOMBRA ON
            MUNINOMBRA.ID_MUNICIPIO=SIS_UNIDAD_EJECUTORA.ID_MUNICIPIO
     INNER JOIN SIS_DEPENDENCIA SIS_DEPENDENCIA ON SIS_UNIDAD_EJECUTORA.ID_DEPENDENCIA = SIS_DEPENDENCIA.ID_DEPENDENCIA         
     INNER JOIN SIS_DEPARTAMENTO SIS_DEPARTAMENTO ON SIS_MUNICIPIO.ID_DEPARTAMENTO = SIS_DEPARTAMENTO.ID_DEPARTAMENTO
     INNER JOIN SIS_FORMA_OBT_PUESTO SIS_FORMA_OBT_PUESTO ON SIS_CONTRATO.ID_FORMA_OBT_PUESTO = SIS_FORMA_OBT_PUESTO.ID_FORMA_OBT_PUESTO
     INNER JOIN sis_tipo_contrato sis_tip_contrat on sis_contrato.id_tipo_contrato=sis_tip_contrat.id_tipo_contrato
WHERE SIS_CONTRATO.ID_CONTRATO=$P{contrato}]]></queryString>

