FROM (
SELECT CASE WHEN ID_PARTIDA_SICOIN <> 0 AND BONO <> 0 THEN sis_pkg_pagos.getPartidaSICOIN(id_partida_siif,ID_PARTIDA_SICOIN,27) ELSE '' END PARTIDA_SICOIN
,  'BONO MENSUAL' TIPO
, T.*
FROM (
