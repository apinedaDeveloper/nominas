	<queryString><![CDATA[select * from (
select hr.id_hora_extra,
cnt.registro_empleado, 
empl.nombrecompleto,
empl.cui,
part.formato as partida,
cnt.numero_plaza,
cnt.id_puesto as clasif,
hr.fechaini,
hr.fechafin,
tex.total_horas_extra as totalhorasextra, 
est.nombre as Nomestado,
hr.fecha_creacion,
hr.id_dependencia,
hr.fechaini as fecha_inicio,
hr.fechafin as fecha_fin,
est.id_estado  
from sis_hora_extra hr, sis_tiempo_extraordinario tex, sis_contrato cnt, sis_empleado_vw empl, sis_partida2_vw part, sis_estado est
where 
hr.id_hora_extra=tex.id_hora_extra
and tex.id_contrato=cnt.id_contrato
and cnt.registro_empleado=empl.registro_empleado
and cnt.id_partida=part.id_partida
and hr.estatus=est.id_estado
and
exists(
select 1 from usuario_unidad b, partida c
where b.id_partida=c.id_partida
and usuario=$P{pUsuario}
and b.tipo_usuario='TRABAJADOR'
and sysdate between b.vigencia_de and nvl(b.vigencia_a,sysdate)
and part.aniomanual=c.aniomanual
and part.d1=c.d1
and part.d2=c.d2
and part.d3d4=c.d3d4
and 
(
(part.d7=c.d7
and part.d6=c.d6
and part.d5=c.d5
)
or
(
c.d5 is null
and c.d6 is null
and c.d7 is null 
)
)
)
)where 
(registro_empleado = $P{pRegEmpl} or $P{pRegEmpl} =0)
and (cui = $P{pCui} or $P{pCui} =0)
and (id_estado = $P{pEstado} or $P{pEstado} = 0)
and (id_dependencia = $P{pIdDep} or $P{pIdDep} =0)
and (trunc (fecha_inicio) between nvl(to_date($P{pFechaInicial} ,'yyyy-mm-dd'),sysdate) and nvl(to_date($P{pFechaFinal} ,'yyyy-mm-dd'),sysdate) )]]></queryString>

