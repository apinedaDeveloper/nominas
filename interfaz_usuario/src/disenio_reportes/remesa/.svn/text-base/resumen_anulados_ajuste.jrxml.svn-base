	<parameter name="fechainicio" isForPrompting="true" class="java.util.Date"/>
	<parameter name="fechafin" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[select
round(abs(valor_aplicado)/nom.nominal,4) as porcentaje,sum(valor_aplicado) descu,sum(nominal) as sueldos
from
sis_nomina nom,
sis_det_calculo_ajuste deta,
sis_proceso prc1,
sis_pago pg,
sis_documento_banco docb
where 
nom.id_nomina=deta.id_nomina
and prc1.id_proceso=nom.id_proceso
and deta.id_ajuste in (21)
and pg.id_documento_banco=docb.id_documento_banco
and pg.id_nomina=nom.id_nomina
and trunc(docb.fecha_emision) between $P{fechainicio}  and $P{fechafin}
and trunc(docb.fecha_anulacion) between $P{fechainicio}  and $P{fechafin}
and prc1.id_estado in (44,53)
and nom.id_tipo_nomina <>20
group by round(abs(valor_aplicado)/nom.nominal,4)]]></queryString>

