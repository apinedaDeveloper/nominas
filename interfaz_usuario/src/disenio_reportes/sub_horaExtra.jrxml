	<queryString><![CDATA[select Extract (year from SysDate) AnioActual, hr.id_hora_extra,cnt.registro_empleado,empl.cui, empl.nombrecompleto,part.formato as partida,cnt.numero_plaza,cnt.id_puesto as clasif,hr.fechaini,hr.fechafin,tex.total_horas_extra as totalhorasextra, est.nombre as Nomestado,hr.fecha_creacion,hr.id_dependencia
from sis_hora_extra hr, sis_tiempo_extraordinario tex, sis_contrato cnt, sis_empleado_vw empl, sis_partida2_vw part, sis_estado est
where 
hr.id_hora_extra=tex.id_hora_extra
and tex.id_contrato=cnt.id_contrato
and cnt.registro_empleado=empl.registro_empleado
and cnt.id_partida=part.id_partida
and hr.estatus=est.id_estado
and $P{idPartida} = cnt.ID_PARTIDA
and $P{numPlaza} = cnt.NUMERO_PLAZA
and cnt.fecha_inicio between $P{fecha1} and $P{fecha2}
order by hr.id_hora_extra]]></queryString>

