	<parameter name="pIdRemesa" isForPrompting="true" class="java.math.BigDecimal"/>
	<queryString><![CDATA[SELECT * FROM (
select 1 ORDEN, r.fecha_inicio, r.fecha_fin , pt.FORMATO, pt.nombre
, round(nvl((d.valor_neto* i.porcentaje)/100 ,0),2) debe, null haber
from SIS_PROV_INDEMNIZACION_DET d
inner join SIS_PROV_INDEMNIZACION i on i.ID_PROV_INDEMNIZACION = d.ID_PROV_INDEMNIZACION
inner join sis_remesa r on r.id_remesa = i.id_remesa
inner join SEP_PARTIDA_vW pt on pt.id_partida = d.ID_PARTIDA_PROVISION
where 
i.ID_REMESA = $P{pIdRemesa}
UNION
select 2 ORDEN, r.fecha_inicio, r.fecha_fin , pt.FORMATO, pt.nombre
, NULL DEBE , round(nvl((d.valor_neto* i.porcentaje)/100 ,0),2) HABER
from SIS_PROV_INDEMNIZACION_DET d
inner join SIS_PROV_INDEMNIZACION i on i.ID_PROV_INDEMNIZACION = d.ID_PROV_INDEMNIZACION
inner join sis_remesa r on r.id_remesa = i.id_remesa
inner join sep_partida_vw pt on pt.id_partida = d.ID_PARTIDA_CONTABLE
where 
i.ID_REMESA = $P{pIdRemesa}
)
ORDER BY ORDEN, formato]]></queryString>

