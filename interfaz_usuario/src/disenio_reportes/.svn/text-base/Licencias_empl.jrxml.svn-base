	<queryString><![CDATA[select datos.*,
case when meseslic<>0 then 
 case when (mesesLic=1 ) then meseslic || ' mes' else meseslic||' meses' end else null end as mesesDura,
 case when diaslic<>0 then 
 case when (diasLic=1 ) then diaslic || ' día' else diaslic||' días' end else null end as diasDura,
 sis_pkg_util.GETCADENA(jefeinmediato,'|',1) as jefe,
 sis_pkg_util.GETCADENA(jefeinmediato,'|',2) as cargo,
SIS_UTIL_REPORT_PKG.GETPRORROGALICENCIA(datos.id_det_licencia)
as fechaPeriodoProg,
(
SELECT mtlic.nombre
   FROM sis_det_licencia prog,sis_motivo_det_licencia mtlic
where prog.id_det_licencia <>datos.id_det_licencia
and prog.id_motivo_det_licencia=mtlic.id_motivo_det_licencia
and rownum<=1
START WITH prog.id_det_licencia = datos.id_det_licencia
CONNECT BY prior prog.id_det_lic_prorroga=prog.id_det_licencia 
) as motivoLicProg
from (
select 
lic.id_det_licencia,
lic.id_solicitud,
sol.fecha_creacion,
muni.nombre as lugarlic,
muni.nombre||', '||trim(to_char(sol.fecha_creacion,'dd Month','NLS_DATE_LANGUAGE = SPANISH'))||' de ' ||to_char(sol.fecha_creacion,'yyyy') as lugarFecha,
empl.registro_empleado,
empl.nombrecompleto,
empl.orden_cedula,
empl.registro_cedula,
Trim(To_Char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''')) cui,
lic.prorroga,
lic.tipo_licencia,
lic.ayuda_becaria, 
lic.fecha_inicio,
lic.fecha_fin,
(lic.fecha_fin+1-lic.fecha_inicio) - (add_months(lic.fecha_inicio+1,months_between(lic.fecha_fin+1,lic.fecha_inicio))-lic.fecha_inicio)+1 as diasLic,
trunc(months_between(lic.fecha_fin+1,lic.fecha_inicio)) as mesesLic,
mtlic.nombre as motLicencia,
lic.desc_motivo_licencia,
dep.nombre as nomDep,
unie.nombre as nomUni,
part.formato2,
cont.numero_plaza,
pst.id_puesto,
upper(pst.nombre) as nomPuesto,
lic.procede,
lic.fecha_resolucion,
lic.observ_resolucion,
lic.JEFEINMEDIATO
from sis_solicitud sol, 
    sis_det_licencia lic, 
    sis_empleado_vw empl, 
    sis_contrato cont, 
    sis_partida_vw part, 
    sis_puesto pst, 
    sis_dependencia dep, 
    sis_unidad_ejecutora unie,
    sis_municipio muni,
    sis_motivo_det_licencia mtlic
 where sol.id_solicitud=lic.id_solicitud
 and mtlic.id_motivo_det_licencia=lic.id_motivo_det_licencia
 and empl.registro_empleado=sol.registro_empleado
 and cont.id_contrato=sol.id_contrato
 and part.id_partida=cont.id_partida
 and part.id_dependencia=dep.id_dependencia
 and part.id_programa=unie.id_programa
 and part.id_dependencia=unie.id_dependencia
 and part.id_unidad=unie.id_unidad
 and pst.id_puesto=cont.id_puesto
 and unie.id_municipio=muni.id_municipio
 and sol.id_solicitud=$P{pIdSolicitud}
 ) datos]]></queryString>

