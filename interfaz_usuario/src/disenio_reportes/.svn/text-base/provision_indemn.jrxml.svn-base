	<parameter name="pIdRemesa" isForPrompting="true" class="java.math.BigDecimal"/>
	<queryString><![CDATA[select pt.formato2 formato, ujj.nombre unidad_ejecutora, p.nombre nombre, res.valor, round(res.valor*0.1170,2) provision
, (select 'Periodo de '|| to_char(fecha_inicio,'dd/mm/yyyy') || ' al ' || to_char(fecha_fin,'dd/mm/yyyy') from sis_remesa where id_remesa = $P{pIdRemesa}) periodo
from (select n.id_partida, sum(n.nominal) valor
from sis_documento_banco d
inner join sis_pago p on p.ID_DOCUMENTO_BANCO = d.ID_DOCUMENTO_BANCO
inner join sis_nomina n on n.id_nomina = p.id_nomina
inner join partida part on part.id_partida = n.id_partida
inner join sis_unidad_ejecutora uj on uj.id_programa = part.d1||part.d2
                                  and uj.ID_DEPENDENCIA = part.d3d4
                                  and uj.ID_UNIDAD = part.d5||part.d6||part.d7
where
not exists (select 1 from SIS_DOC_PAGO_ANULADO where ID_DOC_PAGO = d.ID_DOCUMENTO_BANCO)
and d.FECHA_ANULACION is null
AND N.ID_TIPO_NOMINA = 1
and uj.nombre not like '%VACAC%' and uj.id_programa = 45 
and d.id_remesa = $P{pIdRemesa}
group by n.id_partida
) res
inner join sis_partida_vw pt on pt.id_partida = res.id_partida
inner join partida p on p.id_partida = res.id_partida
inner join sis_unidad_ejecutora ujj on ujj.id_programa = p.d1||p.d2
                                  and ujj.ID_DEPENDENCIA = p.d3d4
                                  and ujj.ID_UNIDAD = p.d5||p.d6||p.d7                                  
order by p.d3d4, p.d5, p.d6, p.d7, p.d9d10]]></queryString>

