	<parameter name="pRegEmpl" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="pCui" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="pFechaInicial" isForPrompting="true" class="java.lang.String"/>
	<parameter name="pFechaFinal" isForPrompting="true" class="java.lang.String"/>
	<parameter name="pEstado" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="pIdDep" isForPrompting="true" class="java.math.BigDecimal"/>
	<queryString><![CDATA[Select * from (
select id_contrato, part.formato2 as partida, part.d3d4 as id_dependencia, a.numero_plaza, a.registro_empleado, f.nombrecompleto, f.cui,
--c.nombre as nom_puesto,
Case when c.id_puesto in (999990,999994,999999) 
   Then Decode(a.nom_plaza,null,'',InitCap(a.nom_plaza))||' F. C.'
   else c.nombre
   end as nom_puesto,
a.horascontratadas,
nvl(fecha_inicio_doc,fecha_inicio) as fecha_inicio,
nvl (fecha_fin_doc,fecha_fin) as fecha_fin,
a.sueldobase,
a.escalaf,
a.compl_sal,b.nombre as estado,a.fecha_creacion,a.estatus as id_estado, a.tipo_validez
from sis_contrato a,sis_estado_contrato_vw b,sis_puesto c, sis_empleado_vw f,sep_partida_vw part
where a.estatus=b.id_estado
and c.id_puesto=a.id_puesto
and a.registro_empleado=f.registro_empleado
and part.id_partida=a.id_partida
and
exists(
select 1 from usuario_unidad b, partida c
where b.id_partida=c.id_partida
and usuario=$P{pUsuario}
and b.tipo_usuario='TRABAJADOR'
and sysdate between b.vigencia_de and nvl(b.vigencia_a,sysdate)
and part.aniomanual=c.aniomanual
and part.d1=c.d1
and part.d2=c.d2
and part.d3d4=c.d3d4
and 
(
(part.d7=c.d7
and part.d6=c.d6
and part.d5=c.d5
)
or
(
c.d5 is null
and c.d6 is null
and c.d7 is null 
)
)
)
) where
(registro_empleado = $P{pRegEmpl} or $P{pRegEmpl}=0)
and (cui = $P{pCui} or $P{pCui} =0) 
and (id_estado = $P{pEstado} or $P{pEstado} = 0)
and (id_dependencia = $P{pIdDep} or $P{pIdDep} =0)
and (   (trunc (fecha_inicio) <= nvl(to_date($P{pFechaInicial},'yyyy-mm-dd'),sysdate) and tipo_validez ='I' and id_estado not in (15))
     or (trunc (fecha_inicio) between nvl(to_date($P{pFechaInicial},'yyyy-mm-dd'),sysdate) and nvl(to_date($P{pFechaFinal},'yyyy-mm-dd'),sysdate) )
    )]]></queryString>

