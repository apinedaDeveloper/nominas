	<queryString><![CDATA[SELECT * FROM (SELECT Ind.ID_TIPO_PRESTACION, Ind.ANIO, Ind.CORRELATIVO_ANIO, Ind.REGISTRO_EMPLEADO, Ind.ANIOS_TIEMPO_SERVICIO, 
Ind.MESES_TIEMPO_SERVICIO, Ind.DIAS_TIEMPO_SERVICIO, Ind.SUELDO_PROMEDIO, Ind.TOTAL_PRESTACION, 
Ind.CREADO_POR, Ind.FECHA_CREACION, Emp.FECHA_INGRESO as FECHA_INGRESO_USAC, Sol.FECHA_SOLICITUD as FECHA_SOLICITUD_AUTORIZADA, Sol.FECHA_SOLICITUD - 1 as FECHA_ULTIMO_DIA, 
Emp.apellido1 || NVL2 (Emp.apellido2, ' ' || Emp.apellido2, '') as APELLIDOS, NVL2 (Emp.APELLIDOCASADA, Emp.APELLIDOCASADA, '') as APELLIDO_CASADA,
Emp.nombre1 || NVL2 (Emp.nombre2, ' ' || Emp.nombre2, '') || NVL2 (Emp.nombre3, ' ' || Emp.nombre3, '') as NOMBRES, Cont.Id_contrato, 
cont.fecha_inicio, Cont.fecha_fin, Cont.id_puesto, Cont.numero_plaza, Cont.id_partida, (Cont.SUELDOBASE + Cont.COMPL_SAL + Cont.ESCALAF) AS SUELDO_NOMINAL,
Cont.HORASCONTRATADAS as HORAS_CONTRATADAS, Pues.NOMBRE AS NOMBRE_PUESTO, Par2.nombre as nombre_dependencia, Dep.nombre as nombre_unidad, Par_vw.formato2 as partida, 
sysdate FECHA_IMPRESION, Ind.DIFERIDO_CALC, Ind.DIFERIDO_12_CALC, Ind.AGUINALDO_CALC, Ind.BONO14_CALC, Ind.TOTAL_SUELDOS, Ind.FECHA_RETIRO,
SIS_PKG_UTIL.GETNUMERO_TO_LETRAS(NUMERO => Ind.TOTAL_PRESTACION, VOPCION => 1) AS TOTAL_EN_LETRAS
FROM SIS_INDEMNIZACION Ind 
INNER JOIN SIIF.SIS_EMPLEADO Emp ON Ind.REGISTRO_EMPLEADO = Emp.REGISTRO_EMPLEADO
LEFT OUTER JOIN SIIF.SIS_SOLICITUD Sol ON Ind.ID_SOLICITUD = Sol.ID_SOLICITUD  
INNER JOIN SIIF.SIS_CONTRATO Cont ON Ind.REGISTRO_EMPLEADO = Cont.REGISTRO_EMPLEADO and Cont.estatus in (15,43)
INNER JOIN SIIF.SIS_PUESTO Pues on Cont.id_puesto = Pues.id_puesto
INNER JOIN SIIF.PARTIDA Par on Cont.ID_PARTIDA = Par.ID_PARTIDA
INNER JOIN SIIF.PARTIDA Par2 on Par.d1 = Par2.d1 and Par.d2 = Par2.d2 and Par.d3d4 = Par2.d3d4 and Par.d5 = Par2.d5 and Par.d6 = Par2.d6 and Par.d7 = Par2.d7 and Par2.d8 is null and Par2.d9d10 is null
INNER JOIN SIIF.SIS_DEPENDENCIA Dep on Par2.d3d4 = Dep.id_dependencia
INNER JOIN SIIF.SIS_PARTIDA_VW Par_vw on Cont.id_partida = par_vw.id_partida
WHERE Ind.ID_TIPO_PRESTACION = 1 AND Ind.ANIO = $P{pAnio} AND Ind.CORRELATIVO_ANIO = $P{pCorrelativoAnio} ORDER BY Cont.fecha_fin desc, Cont.sueldobase desc) where rownum <=1]]></queryString>

