	<parameter name="varAnexo" isForPrompting="true" class="java.math.BigDecimal"/>
	<queryString><![CDATA[select distinct(movimiento.ID_MOVIMIENTO_PLAZA)
,Case When Extract (year from movimiento.fecha_inicio) < 2017 
  Then To_char(em.registro_empleado) 
  else Case when (select to_number(to_char(movimiento.fecha_inicio,'yyyymm')) from dual) < 201807
       Then Trim(To_Char(cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', '''))
       Else Trim(To_Char(cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', '''))||'<br>'||Decode(em.registro_empleado,null,'','/'||em.registro_empleado)
       End
  End Reg_CUI
, movimiento.OPERACION, movimiento.SUELDO_BASE, movimiento.NUMERO_PLAZA,movimiento.COMPL_SALARIAL, 
movimiento.CANTIDAD_HORAS, movimiento.ESCALAFON, movimiento.TOTAL_SUELDO, movimiento.VACACIONES,
movimiento.DIFERIDO, movimiento.DIFERIDO_50, movimiento.AGUINALDO, puesto.NOMBRE as nompuesto,puesto.ID_PUESTO, 
partidas.ID_RENGLON, tipoMov.NOM_TIPO_MOV_ALTA,depen.NOMBRE,movimiento.FECHA_INICIO, movimiento.FECHA_FIN
, movimiento.FECHA_FIN, unidad.NOMBRE AS unidad,puesto.ESCALAMIN,movimiento.total_escalafon,movimiento.total_comsalarial,
anexo.FECHA_CREACION,movimiento.fac_tiempo as tiempo, anexo.comentario, substr( unidad.ID_PROGRAMA,1,1)||'.'||substr( unidad.ID_PROGRAMA,2,1)||'.'||unidad.ID_DEPENDENCIA||'.'||substr( unidad.ID_unidad,1,1)||'.'||substr( unidad.ID_unidad,2,2) as codigo,
sis_pkg_calculos.GETVALORAJUSTEXVAR(27,0,1,30,30,movimiento.fecha_inicio)as bonoMes,initcap(em.NOMBRECOMPLETO) as nomEmpleado
,em.registro_empleado
,Trim(To_Char(cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''')) cui,
movimiento.PERIODO_PLAZA,nvl(sis_pkg_util.getcadena(anexo.hecho_por,'|',1), (select initcap( nvl(auth.TITULO,' ')||' '||em2.NOMBRECOMPLETO ||' / '||em2.cui) from sis_auth_contrato auth, sis_empleado_vw em2 where em2.REGISTRO_EMPLEADO = auth.REGISTRO_EMPLEADO
and sis_pkg_util.GETIDAUTHCONTRATONOM(partidas.id_partida ,0,3) = auth.ID_AUTH_CONTRATO)) as tesorero, 
nvl(sis_pkg_util.getcadena(anexo.aprobado_por,'|',1),(select initcap(nvl(auth1.TITULO,' ')||' '||em1.NOMBRECOMPLETO ||' / '||em1.REGISTRO_EMPLEADO) from sis_auth_contrato auth1, sis_empleado_vw em1 where em1.REGISTRO_EMPLEADO = auth1.REGISTRO_EMPLEADO
and  sis_pkg_util.GETIDAUTHCONTRATONOM(partidas.id_partida ,0,2) = auth1.ID_AUTH_CONTRATO))as jefe
from sis_anexo anexo, sis_partida partidas, sis_tipo_mov_plaza tipoMov,
sis_dependencia depen, sis_unidad_ejecutora unidad, sis_puesto puesto,sis_movimiento_plaza movimiento
left outer join sis_empleado_vw em on movimiento.REGISTRO_EMPLEADO = em.REGISTRO_EMPLEADO 
where anexo.ID_DEPENDENCIA = depen.ID_DEPENDENCIA and
anexo.ID_TIPO_MOV_PLAZA = tipoMov.ID_TIPO_MOV_ALTA and
anexo.ID_ANEXO = movimiento.ID_ANEXO and
partidas.ID_DEPENDENCIA = unidad.ID_DEPENDENCIA and
partidas.ID_UNIDAD = unidad.ID_UNIDAD and
partidas.ID_UNIDAD = unidad.ID_UNIDAD and
partidas.ID_PROGRAMA = unidad.ID_PROGRAMA and
movimiento.ID_PUESTO = puesto.id_puesto and
movimiento.ID_PARTIDA = partidas.ID_PARTIDA and
movimiento.ID_ANEXO = $P{varAnexo}
order by unidad.NOMBRE, movimiento.OPERACION desc, movimiento.NUMERO_PLAZA]]></queryString>

