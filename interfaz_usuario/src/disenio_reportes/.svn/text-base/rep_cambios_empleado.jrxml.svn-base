	<parameter name="pRegistroEmpleado" isForPrompting="true" class="java.math.BigDecimal"/>
	<queryString><![CDATA[select * from
(select
'NOMBRE' AS TIPOCAMBIO,
emplc.registro_Empleado,
decode(emplc.COD_CAMBIO,1,'FALLECIMIENTO','') AS RAZON,
emplc.nombrecompletoanterior,
emplc.NOMBRECOMPLETOACTUAL,
emplc.fecha_cambio,
usr.usuario,
UPPER(usr.nombres ||' '||apellidos) as usuarioMod,
log 
from sis_empleado_cambios emplc, usuario usr
where registro_empleado=$P{pRegistroEmpleado}
and emplc.modificado_por=usr.usuario
and usr.tipo_usuario='TRABAJADOR'
and 
(log like '%4:%'
or log like '%5:%'
or log like '%6:%'
or log like '%7:%'
or log like '%8:%'
or log like '%9:%'
or nombrecompletoanterior is not null
)
union
select 
'PLAN' AS TIPOCAMBIO,
registro_empleado,
'BAJA PLAN PRESTACIONES',
NULL,
NULL,
empl.FECHA_CREACION,
NVL(empl.CREADO_POR,'system'),
UPPER(usr.nombres ||' '||apellidos) as usuarioMod,
null 
from sis_ajuste_exclud_empleado empl left outer join usuario usr on (usr.usuario=empl.creado_por)
where registro_empleado=$P{pRegistroEmpleado}
--and fecha_baja is null
union
select 
'PLAN_ALTA' AS TIPOCAMBIO,
registro_empleado,
'ALTA PLAN PRESTACIONES',
NULL,
NULL,
empl.FECHA_MODIFICACION,
NVL(empl.mod_por,'system'),
UPPER(usr.nombres ||' '||apellidos) as usuarioMod,
null 
from sis_ajuste_exclud_empleado empl left outer join usuario usr on (usr.usuario=empl.mod_por)
where registro_empleado=$P{pRegistroEmpleado}
and fecha_baja is not null
)
order by fecha_cambio desc]]></queryString>

