	<parameter name="pRegistroEmpleado" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="SUBREPORT_DIR" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["C:\\ireportCompiledFiles\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="pAnio" isForPrompting="true" class="java.math.BigDecimal"/>
	<queryString><![CDATA[select 
empl.registro_empleado,
empl.nombrecompleto,
nvl(empl.tel_casa,empl.celular) as telefono,
(
select initcap(muni.nombre)||', '||direc.direccion  
from sis_direccion direc, sis_municipio muni
where direc_actual='SI'
and muni.id_municipio=direc.id_municipio
and registro_empleado=empl.registro_empleado
and rownum<=1
) as direccion,
to_char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''') as cui,
empl.orden_cedula ||' '|| empl.registro_cedula as cedula,
tipo_declaracion,
anio,
orden,
dj.id_partida,
dep.id_dependencia,
Initcap(dep.nombre) as nombreUnidad,
Initcap(part.nombre) as nombreSubprog,
empresa,
dj.telefono as telefonoEmpresa,
fecha_inicio,
fecha_fin,
sysdate as fecha_declaracion,
dj.direccion as direcciontrab,
nvl(dj.horas_contratadas,0) as horas_contratadas,
sis_util_report_pkg.GET_HORAS_SEMESTRE(dj.fecha_inicio,dj.fecha_fin,nvl(dj.horas_contratadas,0),1,null) as semestre1,
sis_util_report_pkg.GET_HORAS_SEMESTRE(dj.fecha_inicio,dj.fecha_fin,nvl(dj.horas_contratadas,0),2,null) as semestre2
from sis_declaracion_jurada dj left join
partida part on (dj.id_partida=part.id_partida) left join sis_dependencia dep on (dep.id_dependencia=part.d3d4), sis_empleado_vw empl
where dj.registro_empleado=empl.registro_empleado
and dj.registro_empleado=$P{pRegistroEmpleado}
and dj.anio=$P{pAnio}
order by tipo_declaracion desc]]></queryString>

