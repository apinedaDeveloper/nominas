	<queryString><![CDATA[SELECT Ind.ID_INDEMNIZACION, Ind.ID_TIPO_INDEMNIZACION, Ind.ANIO_ACTUAL, Ind.CORRELATIVO_ANIO, Ind.REGISTRO_EMPLEADO,
Ind.TIPO_CARRERA_LABORAL, Ind.ID_SOLICITUD_AUTORIZADO, Ind.ANIOS_TIEMPO_SERVICIO, Ind.MESES_TIEMPO_SERVICIO, Ind.DIAS_TIEMPO_SERVICIO, 
Ind.NOMBRE_SOLICITANTE, Ind.CUI_SOLICITANTE, Ind.DIRECCION_SOLICITANTE, Ind.TELEFONOS_SOLICITA, Ind.CORREO_SOLICITANTE, 
Ind.ID_PARENTESCO_SOL, Ind.OTRO_PARENTESCO_SOL, Ind.DIAS_PRORROGADO, Ind.PROMEDIO_SUELDOS, Ind.GASTO_FUNERARIO, Ind.POST_MORTEM,
Ind.TOTAL_PRESTACION, Ind.USUARIO_CREACION, Ind.FECHA_CREACION, Ind.USUARIO_ULTIMA_MOD, Ind.FECHA_ULTIMA_MOD, Emp.NOMBRECOMPLETO, 
Emp.CUI, EstadoEmpl.NOMBRE as NOMBRE_ESTADO_TRABAJADOR, Est.NOMBRE as NOMBRE_ESTADO_INDEMNIZACION, TipoIndem.NOMBRE as NOMBRE_TIPO_INDEMNIZACION,
Sol.FECHA_SOLICITUD as FECHA_SOLICITUD_AUTORIZADA, Mun.NOMBRE as NOMBRE_MUNICIPIO_SOLICITANTE, Dep.NOMBRE NOMBRE_DEPARTAMENTO_SOLICITA, 
Par.NOMBRE as NOMBRE_PARENTESCO_SOL, sysdate FECHA_IMPRESION 
FROM SIS_INDEMNIZACION Ind 
INNER JOIN SIIF.SIS_EMPLEADO_VW Emp ON Ind.REGISTRO_EMPLEADO = Emp.REGISTRO_EMPLEADO
INNER JOIN SIIF.SIS_ESTADO EstadoEmpl ON Emp.ID_ESTADO = EstadoEmpl.ID_ESTADO 
INNER JOIN SIIF.SIS_ESTADO Est ON Ind.ID_ESTADO = Est.ID_ESTADO
INNER JOIN SIIF.SIS_TIPO_INDEMNIZACION TipoIndem ON Ind.ID_TIPO_INDEMNIZACION = TipoIndem.ID_TIPO_INDEMNIZACION 
INNER JOIN SIIF.SIS_SOLICITUD Sol ON Ind.ID_SOLICITUD_AUTORIZADO = Sol.ID_SOLICITUD 
LEFT OUTER JOIN SIIF.SIS_MUNICIPIO Mun ON Ind.MUNICIPIO_SOLICITANTE = Mun.ID_MUNICIPIO
LEFT OUTER JOIN SIIF.SIS_DEPARTAMENTO Dep ON Mun.ID_DEPARTAMENTO = Dep.ID_DEPARTAMENTO 
LEFT OUTER JOIN SIIF.SIS_PARENTESCO Par ON Ind.ID_PARENTESCO_SOL = Par.ID_PARENTESCO 
WHERE Ind.ID_INDEMNIZACION = $P{pIdIndemnizacion}]]></queryString>

