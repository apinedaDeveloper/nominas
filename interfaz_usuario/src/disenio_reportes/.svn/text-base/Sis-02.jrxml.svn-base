	<field name="FECHA_FIN" class="java.sql.Timestamp"/>
	<field name="HORASCONTRATADAS" class="java.math.BigDecimal"/>
	<field name="FECHAFIRMA" class="java.sql.Timestamp"/>
	<field name="ANIO" class="java.math.BigDecimal"/>
	<field name="ORDEN" class="java.math.BigDecimal"/>
	<field name="TIPO_DECLARACION" class="java.lang.String"/>
	<field name="INSTALACION_POSESION" class="java.lang.String"/>
	<field name="NO_PASAPORTE" class="java.lang.String"/>
	<field name="SIS_PUESTO_NOMBRE" class="java.lang.String"/>
	<field name="TIPO_VALIDEZ" class="java.lang.String"/>
	<field name="FECHAINDEF" class="java.sql.Timestamp"/>
	<field name="NO_EXCEDA_DE" class="java.lang.String"/>
	<field name="NUMERO_PLAZA" class="java.lang.String"/>
	<field name="SEMESTRE1" class="java.math.BigDecimal"/>
	<field name="SEMESTRE2" class="java.math.BigDecimal"/>
	<field name="PERIODO_PLAZA" class="java.lang.String"/>
	<field name="DECLARACION_JUBILADO" class="java.math.BigDecimal"/>

	<variable name="vTotalHorasU" class="java.math.BigDecimal" resetType="Group" resetGroup="CargosU" calculation="Sum">
		<variableExpression><![CDATA[$F{HORASCONTRATADAS}]]></variableExpression>
	</variable>
	<variable name="VNombreCompleto" class="java.lang.String" resetType="Report" calculation="First">
		<variableExpression><![CDATA[$F{NOMBRECOMPLETO}]]></variableExpression>
	</variable>
	<variable name="vDireccion" class="java.lang.String" resetType="Report" calculation="First">
		<variableExpression><![CDATA[$F{DIRECCION}]]></variableExpression>
	</variable>
	<variable name="vTelefono" class="java.lang.String" resetType="Report" calculation="First">
		<variableExpression><![CDATA[$F{TELEFONO}]]></variableExpression>
	</variable>
	<variable name="vCedula" class="java.lang.String" resetType="Report" calculation="First">
		<variableExpression><![CDATA[$F{CEDULA}]]></variableExpression>
	</variable>
