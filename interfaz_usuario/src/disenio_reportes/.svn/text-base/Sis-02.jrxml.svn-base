<jasperReport
		 name="Sis-02"
		 columnCount="1"
		 printOrder="Vertical"
		 orientation="Landscape"
		 pageWidth="792"
		 pageHeight="612"
		 columnWidth="732"
		 columnSpacing="0"
		 leftMargin="30"
		 rightMargin="30"
		 topMargin="20"
		 bottomMargin="20"
		 whenNoDataType="NoPages"
		 isTitleNewPage="false"
		 isSummaryNewPage="false">
	<property name="ireport.scriptlethandling" value="0" />
	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="pIdContrato" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="SUBREPORT_DIR" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["C:\\ireportCompiledFiles\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="pRegistro" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="pFirmaContrato" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select tg.id_contrato,  tg.grupo, tg.nomunidad, tg.nomuni2, tg.id_dependencia, tg.nomdep, tg.ubicacion, tg.teluni, tg.registro_empleado
, tg.nombrecompleto,tg.telefono, tg.direccion, tg.cui, tg.cedula, tg.fecha_inicio, tg.fecha_fin, tg.horascontratadas, 
(case when tipo_validez='I' and (select fecha_creacion from sis_contrato where id_contrato= $P{pIdContrato})<to_date('01/01/'||to_char(sysdate,'yyyy'),'dd/mm/yyyy') 
 then (case when trunc(sysdate) > to_date('30/06/'||to_char(sysdate,'yyyy'),'dd/mm/yyyy') 
       then to_date('01/07/'||to_char(sysdate,'yyyy'),'dd/mm/yyyy')
       else to_date('01/01/'||to_char(sysdate,'yyyy'),'dd/mm/yyyy') 
       end) 
 else nvl(fecha_posesion,sis_pkg_util.GETFECHAFIRMACONTRATO($P{pIdContrato})) end
) as fechafirma, tg.anio, tg.orden, tg.tipo_declaracion, tg.instalacion_posesion, tg.no_pasaporte, tg.sis_puesto_nombre, tg.tipo_validez, tg.fechaindef, tg.no_exceda_de,
tg.numero_plaza, tg.semestre1, tg.semestre2, tg.periodo_plaza,
(case when (select fecha_creacion from sis_contrato where id_contrato=$P{pIdContrato}) >=
(select to_date(valor_cadena,'dd/mm/yyyy') from sis_parametros_aplicacion where nom_parametro='JURAMENTO_JUBILADO')
then 1 else 0 end) declaracion_jubilado
from ( select cont.id_contrato, decode (nvl(cont.contract_vacacion,0), 1, 3, 0, 1) as grupo, uni.nombre as nomUnidad,
initcap(uni.nombre) as nomUni2, uni.id_dependencia,  initcap(dep.nombre) as nomDep, uni.ubicacion, uni.telefono as telUni,
empl.registro_empleado, empl.nombrecompleto, nvl(empl.tel_casa,empl.celular) as telefono,
(
select initcap(dp.nombre)||', '||initcap(muni.nombre)||', '||direc.direccion  
from sis_direccion direc, sis_municipio muni,sis_departamento dp
where direc_actual = 'SI' and muni.id_municipio=direc.id_municipio and dp.id_departamento=muni.id_departamento
and registro_empleado=empl.registro_empleado ) as direccion,
to_char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''') as cui,
empl.orden_cedula ||' '|| empl.registro_cedula as cedula, nvl(cont.fecha_inicio_doc,cont.fecha_inicio) as fecha_inicio,
nvl (cont.fecha_fin_doc,cont.fecha_fin) as fecha_fin,
sis_pkg_calculos.GETPROPORCIONHORASLICENCIA(cont.id_contrato,cont.fecha_inicio )*cont.horascontratadas as horascontratadas,
nvl(fecha_firma,sis_pkg_util.GETFECHAFIRMACONTRATO(  $P{pIdContrato})) as fechaFirma,  cont.fecha_posesion,
0 as anio, 0 as orden, 'U' as tipo_declaracion, cont.instalacion_posesion, empl.no_pasaporte,
case when cont.id_puesto in (999990, 999997, 999994, 999999) 
     then case when cont.nom_plaza is not null then cont.nom_plaza when plz.nomplaza is not null then plz.nomplaza else pst.NOMBRE 
end else pst.NOMBRE end AS SIS_PUESTO_NOMBRE, cont.tipo_validez, nvl(cont.fecha_inicio_doc,
( select decj.fecha_inicio from partida prt2, sis_declaracion_jurada decj, partida prt3
where prt2.id_partida=cont.id_partida and decj.registro_empleado=cont.registro_Empleado
and decj.tipo_declaracion = 'U' 
and decj.anio=(select max(anio) from sis_Declaracion_jurada where registro_Empleado=cont.registro_empleado)
and decj.id_partida=prt3.id_partida and prt2.d1=prt3.d1 and prt2.d2=prt3.d2 and prt2.d3d4=prt3.d3d4
and prt2.d5=prt3.d5 and prt2.d6=prt3.d6 and prt2.d7=prt3.d7 and decj.FECHA_FIN is null
and decj.numero_plaza=cont.numero_plaza and rownum <= 1 )) as fechaIndef,
to_char(cont.no_exceda_de,'dd/mm/yyyy') as no_exceda_de, to_char(cont.numero_plaza) as numero_plaza,
sis_util_report_pkg.GET_HORAS_SEMESTRE2(cont.id_contrato,cont.fecha_inicio,Nvl(cont.No_Exceda_de,cont.fecha_fin),1,cont.opcion_indef) as semestre1,
sis_util_report_pkg.GET_HORAS_SEMESTRE2(cont.id_contrato,cont.fecha_inicio,Nvl(cont.No_Exceda_de,cont.fecha_fin),2,cont.opcion_indef) as semestre2,
plz.periodo_plaza
from sis_contrato cont, sis_empleado_vw empl, sis_partida prt, sis_unidad_ejecutora uni,sis_dependencia dep,sis_puesto pst, sis_plaza plz 
where cont.registro_empleado=empl.registro_empleado and cont.id_contrato = $P{pIdContrato}
and prt.id_partida=cont.id_partida and uni.id_programa=prt.id_programa and uni.id_dependencia = prt.id_dependencia
and uni.id_unidad=prt.id_unidad and dep.id_dependencia=prt.id_dependencia and pst.id_puesto = cont.id_puesto
and plz.id_partida=cont.id_partida and plz.id_puesto=cont.id_puesto and plz.numero_plaza = cont.numero_plaza
and plz.correlativo=cont.correlativo
and (
((select contrat.fecha_inicio from sis_contrato contrat where contrat.ID_CONTRATO =   $P{pIdContrato}) >= cont.FECHA_INICIO and nvl(cont.FECHA_FIN,cont.no_exceda_de) is null)
or ((select contrat.fecha_inicio from sis_contrato contrat where contrat.ID_CONTRATO =   $P{pIdContrato}) between cont.FECHA_INICIO and nvl(cont.FECHA_FIN,cont.no_exceda_de))
or( cont.FECHA_INICIO  between  (select contrat.fecha_inicio from sis_contrato contrat where contrat.ID_CONTRATO =   $P{pIdContrato}) and (select nvl(contrat.fecha_fin,no_exceda_de) from sis_contrato contrat where contrat.ID_CONTRATO =   $P{pIdContrato}))
)
union
select cont.id_contrato, decode (nvl(cont.contract_vacacion,0), 1, 3, 0, 1) as grupo, uni.nombre as nomUnidad,
initcap(uni.nombre) as nomUni2, uni.id_dependencia, initcap(dep.nombre) as nomDep, uni.ubicacion,
uni.telefono as telUni, empl.registro_empleado, empl.nombrecompleto, nvl(empl.tel_casa,empl.celular) as telefono,
( select initcap(dp.nombre)||', '||initcap(muni.nombre)||', '||direc.direccion  
from sis_direccion direc, sis_municipio muni,sis_departamento dp
where direc_actual='SI' and muni.id_municipio=direc.id_municipio and dp.id_departamento=muni.id_departamento
and registro_empleado=empl.registro_empleado ) as direccion,
to_char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''') as cui,
empl.orden_cedula ||' '|| empl.registro_cedula as cedula,
(case when cont.tipo_validez='I' then nvl(cont.fecha_inicio_doc, t.fecha_inicio) else t.fecha_inicio end) fecha_inicio,
(case when cont.tipo_validez='I' then nvl(cont.fecha_fin_doc, t.fecha_fin) else t.fecha_fin end) fecha_fin,
sis_pkg_calculos.GETPROPORCIONHORASLICENCIA(cont.id_contrato,cont.fecha_inicio )*cont.horascontratadas as horascontratadas,
nvl(fecha_firma,sis_pkg_util.GETFECHAFIRMACONTRATO(  $P{pIdContrato})) as fechaFirma, cont.fecha_posesion,
0 as anio, 0 as orden, 'U' as tipo_declaracion, cont.instalacion_posesion, empl.no_pasaporte,
case when cont.id_puesto in (999990,999997,999994,999999) 
     then case when cont.nom_plaza is not null then cont.nom_plaza when plz.nomplaza is not null then plz.nomplaza else pst.NOMBRE 
end else pst.NOMBRE end AS SIS_PUESTO_NOMBRE, cont.tipo_validez, nvl(cont.fecha_inicio_doc,
( select decj.fecha_inicio from partida prt2, sis_declaracion_jurada decj,  partida prt3
where prt2.id_partida=cont.id_partida and decj.registro_empleado=cont.registro_Empleado
and decj.tipo_declaracion='U'
and decj.anio=(select max(anio) from sis_Declaracion_jurada where registro_Empleado=cont.registro_empleado)
and decj.id_partida=prt3.id_partida and prt2.d1=prt3.d1 and prt2.d2=prt3.d2
and prt2.d3d4=prt3.d3d4 and prt2.d5=prt3.d5 and prt2.d6=prt3.d6 and prt2.d7=prt3.d7 and decj.FECHA_FIN is null
and decj.numero_plaza=cont.numero_plaza and rownum<=1 )) as fechaIndef, to_char(cont.no_exceda_de,'dd/mm/yyyy') as no_exceda_de,
to_char(cont.numero_plaza) as numero_plaza,
sis_util_report_pkg.GET_HORAS_SEMESTRE2(cont.id_contrato, cont.fecha_inicio,Nvl(cont.No_Exceda_de, t.fecha_fin),1,cont.opcion_indef) as semestre1,
sis_util_report_pkg.GET_HORAS_SEMESTRE2(cont.id_contrato, cont.fecha_inicio,Nvl(cont.No_Exceda_de, t.fecha_fin),2,cont.opcion_indef) as semestre2,
plz.periodo_plaza
from (--Aqui se agrupan los contratos cuando tienen continuidad y que sean del mismo tipo validez
select max(cont.id_contrato) id_contrato, cont.id_partida, cont.id_puesto, cont.numero_plaza, cont.correlativo,
min(nvl(cont.fecha_inicio_doc, cont.fecha_inicio)) fecha_inicio, max(nvl(cont.fecha_fin_doc,cont.fecha_fin)) fecha_fin
from sis_contrato cont
where cont.registro_empleado=(select xy.registro_empleado from sis_contrato xy where xy.ID_CONTRATO = $P{pIdContrato})
and cont.estatus in (3, 4, 29, 32, 36, 43)--INGRESADO, ACTIVO, REVISANDO, OBJETADO, BAJA LICENCIA, FINALIZADO
and (
((select contrat.fecha_inicio from sis_contrato contrat where contrat.ID_CONTRATO =   $P{pIdContrato}) >= cont.FECHA_INICIO and nvl(cont.FECHA_FIN,cont.no_exceda_de) is null)
or ((select contrat.fecha_inicio from sis_contrato contrat where contrat.ID_CONTRATO =   $P{pIdContrato}) between Nvl(cont.fecha_inicio_doc, cont.FECHA_INICIO) and nvl(cont.FECHA_FIN,cont.no_exceda_de))
or( cont.FECHA_INICIO  between  (select contrat.fecha_inicio from sis_contrato contrat where contrat.ID_CONTRATO = $P{pIdContrato}) and (select nvl(contrat.fecha_fin,no_exceda_de) from sis_contrato contrat where contrat.ID_CONTRATO =   $P{pIdContrato}))
)
group by cont.id_partida, cont.id_puesto, cont.numero_plaza, cont.correlativo, cont.tipo_validez
) t, sis_contrato cont, sis_empleado_vw empl, sis_partida prt, sis_unidad_ejecutora uni,sis_dependencia dep,sis_puesto pst, sis_plaza plz
where cont.id_contrato = t.id_contrato and cont.registro_empleado=empl.registro_empleado
and prt.id_partida=cont.id_partida and uni.id_programa=prt.id_programa and uni.id_dependencia=prt.id_dependencia
and uni.id_unidad=prt.id_unidad and dep.id_dependencia=prt.id_dependencia and pst.id_puesto=cont.id_puesto
and plz.id_partida=cont.id_partida and plz.id_puesto=cont.id_puesto and plz.numero_plaza=cont.numero_plaza
and plz.correlativo=cont.correlativo
union
select cont.id_contrato, decode (nvl(cont.contract_vacacion,0), 1, 3, 0, 1) as grupo, uni.nombre as nomUnidad,
initcap(uni.nombre) as nomUni2, uni.id_dependencia, initcap(dep.nombre) as nomDep, uni.ubicacion,
uni.telefono as telUni, empl.registro_empleado, empl.nombrecompleto, nvl(empl.tel_casa,empl.celular) as telefono,
( select initcap(dp.nombre)||', '||initcap(muni.nombre)||', '||direc.direccion  
from sis_direccion direc, sis_municipio muni,sis_departamento dp
where direc_actual='SI' and muni.id_municipio=direc.id_municipio and dp.id_departamento=muni.id_departamento
and registro_empleado=empl.registro_empleado ) as direccion,
to_char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''') as cui,
empl.orden_cedula ||' '|| empl.registro_cedula as cedula, nvl (cont.fecha_inicio_doc,cont.fecha_inicio ) as fecha_inicio,
nvl (cont.fecha_fin_doc, cont.fecha_fin) as fecha_fin,
sis_pkg_calculos.GETPROPORCIONHORASLICENCIA(cont.id_contrato,cont.fecha_inicio )*cont.horascontratadas as horascontratadas,
nvl(fecha_firma,sis_pkg_util.GETFECHAFIRMACONTRATO(  $P{pIdContrato})) as fechaFirma, cont.fecha_posesion,
0 as anio, 0 as orden, 'U' as tipo_declaracion, cont.instalacion_posesion, empl.no_pasaporte,
case when cont.id_puesto in (999990, 999997, 999994, 999999) 
     then case when cont.nom_plaza is not null then cont.nom_plaza when plz.nomplaza is not null then plz.nomplaza else pst.NOMBRE 
end else pst.NOMBRE end AS SIS_PUESTO_NOMBRE, cont.tipo_validez, nvl(cont.fecha_inicio_doc,
( select decj.fecha_inicio from partida prt2, sis_declaracion_jurada decj, partida prt3
where prt2.id_partida=cont.id_partida and decj.registro_empleado=cont.registro_Empleado
and decj.tipo_declaracion='U' and decj.anio=(select max(anio) from sis_Declaracion_jurada where registro_Empleado=cont.registro_empleado)
and decj.id_partida=prt3.id_partida and prt2.d1=prt3.d1 and prt2.d2=prt3.d2 and prt2.d3d4=prt3.d3d4
and prt2.d5=prt3.d5 and prt2.d6=prt3.d6 and prt2.d7=prt3.d7 and decj.FECHA_FIN is null and decj.numero_plaza=cont.numero_plaza
and rownum<=1 )) as fechaIndef, to_char(cont.no_exceda_de,'dd/mm/yyyy') as no_exceda_de, to_char(cont.numero_plaza) as numero_plaza,
sis_util_report_pkg.GET_HORAS_SEMESTRE2(cont.id_contrato,cont.fecha_inicio,Nvl(cont.No_Exceda_de,cont.fecha_fin),1,cont.opcion_indef) as semestre1,
sis_util_report_pkg.GET_HORAS_SEMESTRE2(cont.id_contrato,cont.fecha_inicio,Nvl(cont.No_Exceda_de,cont.fecha_fin),2,cont.opcion_indef) as semestre2,
plz.periodo_plaza
from sis_contrato cont, sis_empleado_vw empl, sis_partida prt, sis_unidad_ejecutora uni,sis_dependencia dep,sis_puesto pst, sis_plaza plz 
where cont.registro_empleado=empl.registro_empleado
and cont.registro_empleado=(select xy.registro_empleado from sis_contrato xy where xy.ID_CONTRATO = $P{pIdContrato})
and cont.estatus in (4,43) and prt.id_partida=cont.id_partida and uni.id_programa=prt.id_programa
and uni.id_dependencia=prt.id_dependencia and uni.id_unidad=prt.id_unidad and dep.id_dependencia=prt.id_dependencia
and pst.id_puesto=cont.id_puesto and plz.id_partida=cont.id_partida and plz.id_puesto=cont.id_puesto
and plz.numero_plaza=cont.numero_plaza and plz.correlativo=cont.correlativo and cont.TIPO_VALIDEZ='I'
and to_char(nvl(cont.fecha_inicio_doc,cont.fecha_inicio),'yyyy')<=(select to_char(contrat.fecha_inicio,'yyyy') from sis_contrato contrat where contrat.ID_CONTRATO =   $P{pIdContrato})
and not exists ( --Comprueba que no haya tenido una licencia autorizada en el año que se está filtrando.
  select * from sis_solicitud where id_contrato = cont.id_contrato and id_estado = 39
  and anio <= (select to_char(contrat.fecha_inicio,'yyyy') from sis_contrato contrat where contrat.ID_CONTRATO = $P{pIdContrato})
  and procesado = 1 and numero_plaza = cont.numero_plaza
)
union 
select  0, 2, null, initcap(dj.empresa), dep.id_dependencia, null, dj.direccion as direcciontrab,
dj.telefono as telefonoEmpresa, empl.registro_empleado, empl.nombrecompleto, nvl(empl.tel_casa,empl.celular) as telefono,
(
select initcap(muni.nombre)||', '||direc.direccion  
from sis_direccion direc, sis_municipio muni
where direc_actual='SI' and muni.id_municipio=direc.id_municipio
and registro_empleado=empl.registro_empleado ) as direccion,
to_char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''') as cui,
empl.orden_cedula ||' '|| empl.registro_cedula as cedula, dj.fecha_inicio, dj.fecha_fin,
nvl(dj.horas_contratadas,0) as horas_contratadas, sysdate as fecha_declaracion, SysDate fecha_posesion,
dj.anio, dj.orden, dj.tipo_declaracion, null, null, '', 'N', null, null, '',
sis_util_report_pkg.GET_HORAS_SEMESTRE(dj.fecha_inicio,dj.fecha_fin,horas_contratadas ,1) as semestre1,
sis_util_report_pkg.GET_HORAS_SEMESTRE(dj.fecha_inicio,dj.fecha_fin,horas_contratadas ,2) as semestre2,
null
from sis_declaracion_jurada dj left join
partida part on (dj.id_partida=part.id_partida) left join sis_dependencia dep on (dep.id_dependencia=part.d3d4)
, sis_empleado_vw empl, sis_contrato cont
	where dj.registro_empleado=empl.registro_empleado
    and dj.registro_empleado = cont.registro_empleado and dj.anio = cont.correlativo
	and dj.TIPO_DECLARACION='E' and cont.id_contrato=$P{pIdContrato}
	and ( (cont.fecha_inicio >= dj.FECHA_INICIO and dj.FECHA_FIN is null)
	     or (cont.fecha_inicio between dj.FECHA_INICIO and nvl(dj.FECHA_FIN,SysDate))
	     or (dj.FECHA_INICIO  between cont.fecha_inicio and nvl(cont.fecha_fin, cont.no_exceda_de))
	     or dj.fecha_fin is null 
    )
) tg
where semestre1>0 or semestre2>0
order by grupo,fecha_inicio]]></queryString>

	<field name="ID_CONTRATO" class="java.math.BigDecimal"/>
	<field name="GRUPO" class="java.math.BigDecimal"/>
	<field name="NOMUNIDAD" class="java.lang.String"/>
	<field name="NOMUNI2" class="java.lang.String"/>
	<field name="ID_DEPENDENCIA" class="java.math.BigDecimal"/>
	<field name="NOMDEP" class="java.lang.String"/>
	<field name="UBICACION" class="java.lang.String"/>
	<field name="TELUNI" class="java.lang.String"/>
	<field name="REGISTRO_EMPLEADO" class="java.math.BigDecimal"/>
	<field name="NOMBRECOMPLETO" class="java.lang.String"/>
	<field name="TELEFONO" class="java.lang.String"/>
	<field name="DIRECCION" class="java.lang.String"/>
	<field name="CUI" class="java.lang.String"/>
	<field name="CEDULA" class="java.lang.String"/>
