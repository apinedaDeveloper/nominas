	<parameter name="pIdContrato" isForPrompting="true" class="java.math.BigDecimal"/>
	<queryString><![CDATA[select Extract (year from SysDate) AnioActual, Trim(To_Char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''')) cui,
cont.id_contrato,
upper(
case when cont.id_puesto in (999990,999997,999994,999999) 
     then case when cont.nom_plaza is not null then cont.nom_plaza when plz.nomplaza is not null then plz.nomplaza else pst.NOMBRE 
     end else pst.NOMBRE end) AS nombrePuestoMod,
upper(pst.nombre) as nomPuesto,
case when uni.id_dependencia=41 then 'JUNTA ADMINISTRADORA DEL PLAN DE PRESTACIONES'  else upper(uni.nombre) end as nomUnidad,
initcap(uni.nombre) as nomUni2,
uni.id_dependencia,
dep.nombre as nomDep,
empl.registro_empleado,
trim(empl.nombre1) || ' ' || trim(nvl(empl.nombre2,'')) || ' '|| trim(nvl(empl.nombre3,'')) as nombres,
trim(empl.apellido1)||' '|| trim(nvl(empl.apellido2,'')) ||' '|| trim(nvl(empl.apellidocasada,'')) as apellidos,
nvl(empl.tel_casa,empl.celular) as telefono,
(
select initcap(dep.nombre)||', '||initcap(muni.nombre)||', '||direc.direccion  
from sis_direccion direc, sis_municipio muni, sis_departamento dep
where direc_actual='SI'
and muni.id_municipio=direc.id_municipio
and dep.id_departamento=muni.id_departamento
and registro_empleado=empl.registro_empleado
and rownum<=1
) as direccion,
to_char(empl.cui,'9999G99999G9999','NLS_NUMERIC_CHARACTERS= '', ''') as cui,
empl.orden_cedula,
empl.registro_cedula as cedula,
munCedula.nombre ||', '||dept.nombre as extendidaEn,
empl.fecha_nacimiento,
floor(months_between(cont.fecha_inicio,empl.fecha_nacimiento)/12) as edad_empleado,
empl.genero,
empl.estadocivil,
ps.nacionalidad,
naca.descripcion as nivelAcademico,
Upper(empl.titulo) titulo,
empl.colegiado,
cont.primer_ingreso,
empl.igss,
empl.no_pasaporte,
(case when cont.FECHA_CREACION>=
(select to_date(valor_cadena,'dd/mm/yyyy') from sis_parametros_aplicacion where nom_parametro='SIN_CEDULA')
then 1 else 0 end) sin_cedula
from 
    sis_contrato cont, 
    sis_empleado empl left outer join (sis_municipio munCedula 
           inner join sis_departamento dept on dept.id_departamento=munCedula.id_departamento )
       on empl.municipio_cedula=munCedula.id_municipio, 
    sis_partida prt, 
    sis_unidad_ejecutora uni,
    sis_dependencia dep, 
    pais ps, 
    sis_nivel_academico naca,
    sis_puesto pst,
    sis_plaza plz
where cont.registro_empleado=empl.registro_empleado
and id_contrato=$P{pIdContrato}
and plz.id_partida=cont.id_partida
and plz.id_puesto=cont.id_puesto
and plz.numero_plaza=cont.numero_plaza
and plz.correlativo=cont.correlativo
and prt.id_partida=cont.id_partida
and uni.id_programa=prt.id_programa
and uni.id_dependencia=prt.id_dependencia
and uni.id_unidad=prt.id_unidad
and dep.id_dependencia=prt.id_dependencia
and ps.codigo=empl.id_nacionalidad
and naca.id_nivel=empl.id_nivel
and pst.id_puesto=cont.id_puesto]]></queryString>

